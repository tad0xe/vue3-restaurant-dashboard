{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"BankManager\",\n  data() {\n    return {\n      form: {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\"\n        // Replace or set dynamically if needed\n      },\n\n      banks: [],\n      isEditing: false,\n      editId: null\n    };\n  },\n  mounted() {\n    this.fetchBanks();\n  },\n  computed: {\n    ...mapGetters([\"getToken\"])\n  },\n  methods: {\n    async fetchBanks() {\n      try {\n        const res = await axios.get(\"http://localhost:4000/api/bank\", {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        console.log(\"Fetched banks:\", res.data);\n        this.banks = res.data.banks;\n      } catch (err) {\n        console.error(\"Error fetching banks:\", err);\n      }\n    },\n    async createBank() {\n      try {\n        const res = await axios.post(\"http://localhost:4000/api/bank\", this.form, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        console.log(\"Bank created:\", res.data);\n        alert(\"‚úÖ Bank created successfully!\");\n        if (res.data.banks && res.data.banks.length > 0) {\n          this.banks.push(res.data.banks[0]); // ‚úÖ Push only the new bank\n        } else {\n          await this.fetchBanks(); // fallback if no bank was returned\n        }\n\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error creating bank:\", err);\n        alert(\"‚ùå Failed to create.\");\n      }\n    },\n    editBank(bank) {\n      this.isEditing = true;\n      this.editId = bank._id;\n      this.form = {\n        ...bank\n      };\n    },\n    cancelEdit() {\n      this.resetForm();\n    },\n    async updateBank() {\n      try {\n        const res = await axios.put(`http://localhost:4000/api/bank/${this.editId}`, this.form, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        const index = this.banks.findIndex(b => b._id === this.editId);\n        if (res.data.banks && res.data.banks.length > 0) {\n          const updatedBank = res.data.banks[0];\n          if (index !== -1) {\n            this.banks.splice(index, 1, updatedBank); // ‚úÖ Reactive array update\n          }\n        } else {\n          await this.fetchBanks(); // fallback if API returns nothing\n        }\n\n        this.resetForm();\n        //alert(\"‚úÖ Bank updated!\");\n      } catch (err) {\n        console.error(\"Error updating bank:\", err);\n        alert(\"‚ùå Update failed.\");\n      }\n    },\n    async deleteBank(id) {\n      if (!confirm(\"Delete this bank info?\")) return;\n      try {\n        await axios.delete(`http://localhost:4000/api/bank/${id}`, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        this.banks = this.banks.filter(b => b._id !== id);\n      } catch (err) {\n        console.error(\"Error deleting bank:\", err);\n        alert(\"‚ùå Delete failed.\");\n      }\n    },\n    resetForm() {\n      this.form = {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\"\n      };\n      this.isEditing = false;\n      this.editId = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","form","accountName","accountNumber","bankName","banks","isEditing","editId","mounted","fetchBanks","computed","methods","res","get","headers","Authorization","getToken","console","log","err","error","createBank","post","alert","length","push","resetForm","editBank","bank","_id","cancelEdit","updateBank","put","index","findIndex","b","updatedBank","splice","deleteBank","id","confirm","delete","filter"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\app\\calender.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-4\">\r\n    <h2>üè¶ Bank Info Manager</h2>\r\n\r\n    <!-- ‚úÖ Create or Edit Form -->\r\n     <div class=\"row\">\r\n      <div class=\"col-md-8 offset-md-2\">\r\n          <form\r\n      @submit.prevent=\"isEditing ? updateBank() : createBank()\"\r\n       class=\"shadow p-4 w-md-75 mx-auto w-100\"\r\n    >\r\n      <h5>{{ isEditing ? \"‚úèÔ∏è Edit Bank Info\" : \"‚ûï Add Bank Info\" }}</h5>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Account Name</label>\r\n        <input\r\n          v-model=\"form.accountName\"\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Account Number</label>\r\n        <input\r\n          v-model=\"form.accountNumber\"\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Bank Name</label>\r\n        <input\r\n          v-model=\"form.bankName\"\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"btn btn-success\">\r\n        {{ isEditing ? \"Update\" : \"Create\" }}\r\n      </button>\r\n      <button\r\n        v-if=\"isEditing\"\r\n        type=\"button\"\r\n        class=\"btn btn-secondary ms-2\"\r\n        @click=\"cancelEdit\"\r\n      >\r\n        Cancel\r\n      </button>\r\n    </form>\r\n      </div>\r\n\r\n     </div>\r\n  \r\n\r\n    <!-- ‚úÖ Bank Info List -->\r\n    <div class=\"col-md-4\">\r\n      <h5>üìã Saved Bank Info</h5>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          v-for=\"bank in banks\"\r\n          :key=\"bank._id\"\r\n          class=\"list-group-item d-flex justify-content-between align-items-center\"\r\n        >\r\n          <div>\r\n            <strong>{{ bank.accountName }}</strong\r\n            ><br />\r\n            {{ bank.accountNumber }} ‚Äî {{ bank.bankName }}\r\n          </div>\r\n          <div>\r\n            <button class=\"btn btn-sm btn-warning me-2\" @click=\"editBank(bank)\">\r\n              Edit\r\n            </button>\r\n            <button class=\"btn btn-sm btn-danger\" @click=\"deleteBank(bank._id)\">\r\n              Delete\r\n            </button>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"BankManager\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n        // Replace or set dynamically if needed\r\n      },\r\n      banks: [],\r\n      isEditing: false,\r\n      editId: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchBanks();\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n  methods: {\r\n    async fetchBanks() {\r\n      try {\r\n        const res = await axios.get(\"http://localhost:4000/api/bank\", {\r\n          headers: {\r\n            Authorization: `Bearer ${this.getToken}`,\r\n          },\r\n        });\r\n        console.log(\"Fetched banks:\", res.data);\r\n        this.banks = res.data.banks;\r\n      } catch (err) {\r\n        console.error(\"Error fetching banks:\", err);\r\n      }\r\n    },\r\n    async createBank() {\r\n      try {\r\n        const res = await axios.post(\r\n          \"http://localhost:4000/api/bank\",\r\n          this.form,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${this.getToken}`,\r\n            },\r\n          }\r\n        );\r\n\r\n        console.log(\"Bank created:\", res.data);\r\n        alert(\"‚úÖ Bank created successfully!\");\r\n\r\n        if (res.data.banks && res.data.banks.length > 0) {\r\n          this.banks.push(res.data.banks[0]); // ‚úÖ Push only the new bank\r\n        } else {\r\n          await this.fetchBanks(); // fallback if no bank was returned\r\n        }\r\n\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error creating bank:\", err);\r\n        alert(\"‚ùå Failed to create.\");\r\n      }\r\n    },\r\n    editBank(bank) {\r\n      this.isEditing = true;\r\n      this.editId = bank._id;\r\n      this.form = { ...bank };\r\n    },\r\n    cancelEdit() {\r\n      this.resetForm();\r\n    },\r\n    async updateBank() {\r\n      try {\r\n        const res = await axios.put(\r\n          `http://localhost:4000/api/bank/${this.editId}`,\r\n          this.form,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${this.getToken}`,\r\n            },\r\n          }\r\n        );\r\n\r\n        const index = this.banks.findIndex((b) => b._id === this.editId);\r\n\r\n        if (res.data.banks && res.data.banks.length > 0) {\r\n          const updatedBank = res.data.banks[0];\r\n\r\n          if (index !== -1) {\r\n            this.banks.splice(index, 1, updatedBank); // ‚úÖ Reactive array update\r\n          }\r\n        } else {\r\n          await this.fetchBanks(); // fallback if API returns nothing\r\n        }\r\n\r\n        this.resetForm();\r\n        //alert(\"‚úÖ Bank updated!\");\r\n      } catch (err) {\r\n        console.error(\"Error updating bank:\", err);\r\n        alert(\"‚ùå Update failed.\");\r\n      }\r\n    },\r\n    async deleteBank(id) {\r\n      if (!confirm(\"Delete this bank info?\")) return;\r\n      try {\r\n        await axios.delete(`http://localhost:4000/api/bank/${id}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${this.getToken}`,\r\n          },\r\n        });\r\n        this.banks = this.banks.filter((b) => b._id !== id);\r\n      } catch (err) {\r\n        console.error(\"Error deleting bank:\", err);\r\n        alert(\"‚ùå Delete failed.\");\r\n      }\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n      };\r\n      this.isEditing = false;\r\n      this.editId = null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"mappings":";AAyFA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,WAAW,EAAE,EAAE;QACfC,aAAa,EAAE,EAAE;QACjBC,QAAQ,EAAE;QACV;MACF,CAAC;;MACDC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGZ,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EACDa,OAAO,EAAE;IACP,MAAMF,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMG,GAAE,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,gCAAgC,EAAE;UAC5DC,OAAO,EAAE;YACPC,aAAa,EAAG,UAAS,IAAI,CAACC,QAAS;UACzC;QACF,CAAC,CAAC;QACFC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEN,GAAG,CAACZ,IAAI,CAAC;QACvC,IAAI,CAACK,KAAI,GAAIO,GAAG,CAACZ,IAAI,CAACK,KAAK;MAC7B,EAAE,OAAOc,GAAG,EAAE;QACZF,OAAO,CAACG,KAAK,CAAC,uBAAuB,EAAED,GAAG,CAAC;MAC7C;IACF,CAAC;IACD,MAAME,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMT,GAAE,GAAI,MAAMf,KAAK,CAACyB,IAAI,CAC1B,gCAAgC,EAChC,IAAI,CAACrB,IAAI,EACT;UACEa,OAAO,EAAE;YACPC,aAAa,EAAG,UAAS,IAAI,CAACC,QAAS;UACzC;QACF,CACF,CAAC;QAEDC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEN,GAAG,CAACZ,IAAI,CAAC;QACtCuB,KAAK,CAAC,8BAA8B,CAAC;QAErC,IAAIX,GAAG,CAACZ,IAAI,CAACK,KAAI,IAAKO,GAAG,CAACZ,IAAI,CAACK,KAAK,CAACmB,MAAK,GAAI,CAAC,EAAE;UAC/C,IAAI,CAACnB,KAAK,CAACoB,IAAI,CAACb,GAAG,CAACZ,IAAI,CAACK,KAAK,CAAC,CAAC,CAAC,CAAC,EAAE;QACtC,OAAO;UACL,MAAM,IAAI,CAACI,UAAU,CAAC,CAAC,EAAE;QAC3B;;QAEA,IAAI,CAACiB,SAAS,CAAC,CAAC;MAClB,EAAE,OAAOP,GAAG,EAAE;QACZF,OAAO,CAACG,KAAK,CAAC,sBAAsB,EAAED,GAAG,CAAC;QAC1CI,KAAK,CAAC,qBAAqB,CAAC;MAC9B;IACF,CAAC;IACDI,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,CAACtB,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACC,MAAK,GAAIqB,IAAI,CAACC,GAAG;MACtB,IAAI,CAAC5B,IAAG,GAAI;QAAE,GAAG2B;MAAK,CAAC;IACzB,CAAC;IACDE,UAAUA,CAAA,EAAG;MACX,IAAI,CAACJ,SAAS,