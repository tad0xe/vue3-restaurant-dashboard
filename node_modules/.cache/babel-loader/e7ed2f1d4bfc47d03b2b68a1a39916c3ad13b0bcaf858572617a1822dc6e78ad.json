{"ast":null,"code":"export default {\n  data() {\n    return {\n      menuActive: false,\n      scrolled: false,\n      activeSection: \"home\"\n    };\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.handleScroll);\n    this.revealSections();\n  },\n  methods: {\n    toggleMenu() {\n      this.menuActive = !this.menuActive;\n      document.body.classList.toggle(\"no-scroll\", this.menuActive);\n    },\n    closeMenu() {\n      this.menuActive = false;\n      document.body.classList.remove(\"no-scroll\");\n    },\n    handleScroll() {\n      this.scrolled = window.scrollY > 100;\n      const sections = document.querySelectorAll(\"section\");\n      const navbarHeight = document.querySelector(\".navbar\").offsetHeight;\n      const scrollPos = window.scrollY + navbarHeight;\n      sections.forEach(section => {\n        const offsetTop = section.offsetTop;\n        const height = section.offsetHeight;\n        if (scrollPos >= offsetTop && scrollPos < offsetTop + height) {\n          this.activeSection = section.getAttribute(\"id\");\n        }\n        const sectionTop = section.getBoundingClientRect().top;\n        if (sectionTop < window.innerHeight - 150) {\n          section.classList.add(\"active\");\n        }\n      });\n    },\n    revealSections() {\n      setTimeout(() => this.handleScroll(), 50);\n    }\n  }\n};","map":{"version":3,"names":["data","menuActive","scrolled","activeSection","mounted","window","addEventListener","handleScroll","revealSections","methods","toggleMenu","document","body","classList","toggle","closeMenu","remove","scrollY","sections","querySelectorAll","navbarHeight","querySelector","offsetHeight","scrollPos","forEach","section","offsetTop","height","getAttribute","sectionTop","getBoundingClientRect","top","innerHeight","add","setTimeout"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\vue3-restaurant-dashboard\\src\\components\\homepage\\topp.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <header>\r\n      <nav :class=\"['navbar', { scrolled }]\">\r\n        <div class=\"logo\">\r\n          <router-link to=\"/\">NeoVerse</router-link>\r\n        </div>\r\n        <ul :class=\"['nav-menu', { active: menuActive }]\">\r\n          <li class=\"nav-item\">\r\n            <a\r\n              href=\"#home\"\r\n              class=\"nav-link\"\r\n              :class=\"{ active: activeSection === 'home' }\"\r\n              @click=\"closeMenu\"\r\n              >Find Event</a\r\n            >\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <a\r\n              href=\"#about\"\r\n              class=\"nav-link\"\r\n              :class=\"{ active: activeSection === 'about' }\"\r\n              @click=\"closeMenu\"\r\n              >Host Event</a\r\n            >\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <a\r\n              href=\"#services\"\r\n              class=\"nav-link\"\r\n              :class=\"{ active: activeSection === 'services' }\"\r\n              @click=\"closeMenu\"\r\n              >Help</a\r\n            >\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link\r\n              to=\"/Login\"\r\n              class=\"nav-link\"\r\n              :class=\"{ active: activeSection === 'portfolio' }\"\r\n              @click.native=\"closeMenu\"\r\n            >\r\n              Login\r\n            </router-link>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link\r\n              to=\"/Register\"\r\n              class=\"nav-link\"\r\n              :class=\"{ active: activeSection === 'contact' }\"\r\n              @click.native=\"closeMenu\"\r\n            >\r\n              Register\r\n            </router-link>\r\n          </li>\r\n        </ul>\r\n        <div\r\n          class=\"hamburger\"\r\n          :class=\"{ active: menuActive }\"\r\n          @click=\"toggleMenu\"\r\n        >\r\n          <span class=\"bar\"></span>\r\n          <span class=\"bar\"></span>\r\n          <span class=\"bar\"></span>\r\n        </div>\r\n      </nav>\r\n    </header>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      menuActive: false,\r\n      scrolled: false,\r\n      activeSection: \"home\",\r\n    };\r\n  },\r\n  mounted() {\r\n    window.addEventListener(\"scroll\", this.handleScroll);\r\n    this.revealSections();\r\n  },\r\n  methods: {\r\n    toggleMenu() {\r\n      this.menuActive = !this.menuActive;\r\n      document.body.classList.toggle(\"no-scroll\", this.menuActive);\r\n    },\r\n    closeMenu() {\r\n      this.menuActive = false;\r\n      document.body.classList.remove(\"no-scroll\");\r\n    },\r\n    handleScroll() {\r\n      this.scrolled = window.scrollY > 100;\r\n\r\n      const sections = document.querySelectorAll(\"section\");\r\n      const navbarHeight = document.querySelector(\".navbar\").offsetHeight;\r\n      const scrollPos = window.scrollY + navbarHeight;\r\n\r\n      sections.forEach((section) => {\r\n        const offsetTop = section.offsetTop;\r\n        const height = section.offsetHeight;\r\n\r\n        if (scrollPos >= offsetTop && scrollPos < offsetTop + height) {\r\n          this.activeSection = section.getAttribute(\"id\");\r\n        }\r\n\r\n        const sectionTop = section.getBoundingClientRect().top;\r\n        if (sectionTop < window.innerHeight - 150) {\r\n          section.classList.add(\"active\");\r\n        }\r\n      });\r\n    },\r\n    revealSections() {\r\n      setTimeout(() => this.handleScroll(), 50);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Modern Reset */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen,\r\n    Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\r\n  background-color: #f9f9f9;\r\n  color: #333;\r\n  line-height: 1.6;\r\n  overflow-x: hidden;\r\n  scroll-behavior: smooth;\r\n}\r\n\r\n/* New Color Palette \r\n   Primary: #1a2639 (Deep Navy Blue)\r\n   Secondary: #00a8cc (Bright Teal)\r\n   Dark: #0c1e35 (Dark Navy)\r\n   Light: #f5f7fa (Clean Off-white)\r\n   Accent: #fd3a69 (Vibrant Pink)\r\n*/\r\n\r\n/* Navbar Styles */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 0%;\r\n  background: linear-gradient(to right, #0c1e35, #1a2639);\r\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  z-index: 1000;\r\n  transition: all 0.4s ease;\r\n}\r\n\r\n.navbar.scrolled {\r\n  padding: 0.8rem 5%;\r\n  background: rgba(10, 25, 47, 0.95);\r\n  backdrop-filter: blur(10px);\r\n}\r\n\r\n.logo a {\r\n  font-size: 1.8rem;\r\n  font-weight: 700;\r\n  color: #f5f7fa;\r\n  text-decoration: none;\r\n  letter-spacing: 1px;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.logo a:hover {\r\n  color: #00a8cc;\r\n  text-shadow: 0 0 10px rgba(0, 168, 204, 0.3);\r\n}\r\n\r\n.nav-menu {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  list-style: none;\r\n  gap: 2rem;\r\n}\r\n\r\n.nav-link {\r\n  color: #f5f7fa;\r\n  text-decoration: none;\r\n  font-size: 1.1rem;\r\n  font-weight: 500;\r\n  transition: all 0.3s ease;\r\n  padding: 0.5rem 0;\r\n  position: relative;\r\n}\r\n\r\n.nav-link:hover {\r\n  color: #00a8cc;\r\n}\r\n\r\n.nav-link::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 0;\r\n  height: 2px;\r\n  bottom: 0;\r\n  left: 0;\r\n  background-color: #00a8cc;\r\n  transition: width 0.3s ease;\r\n}\r\n\r\n.nav-link:hover::after {\r\n  width: 100%;\r\n}\r\n.nav-link,\r\n.nav-link:visited {\r\n  color: #f5f7fa !important; /* Make text white */\r\n}\r\n\r\n.router-link-exact-active.nav-link,\r\n.router-link-active.nav-link {\r\n  color: #00a8cc !important;\r\n}\r\n\r\n\r\n.nav-link.active {\r\n  color: #00a8cc;\r\n}\r\n\r\n.nav-link.active::after {\r\n  width: 100%;\r\n  background-color: #00a8cc;\r\n}\r\n\r\n.hamburger {\r\n  display: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.bar {\r\n  display: block;\r\n  width: 25px;\r\n  height: 3px;\r\n  margin: 5px auto;\r\n  background-color: #f5f7fa;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\n/* Hero Section */\r\n.hero {\r\n  min-height: 100vh;\r\n  padding-top: 80px; /* Add padding equal to navbar height */\r\n  background: linear-gradient(135deg, #1a2639 0%, #0c1e35 100%);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  text-align: center;\r\n  padding-left: 2rem;\r\n  padding-right: 2rem;\r\n  margin-top: 0;\r\n}\r\n\r\n.hero-content {\r\n  max-width: 800px;\r\n  color: #f5f7fa;\r\n  animation: fadeInUp 1s ease-out;\r\n}\r\n\r\n.hero-content h1 {\r\n  font-size: 4rem;\r\n  margin-bottom: 1.5rem;\r\n  letter-spacing: 1px;\r\n  background: linear-gradient(to right, #f5f7fa, #00a8cc);\r\n  -webkit-background-clip: text;\r\n  background-clip: text;\r\n  color: transparent;\r\n}\r\n\r\n.hero-content p {\r\n  font-size: 1.3rem;\r\n  margin-bottom: 2.5rem;\r\n  line-height: 1.8;\r\n  opacity: 0.9;\r\n}\r\n\r\n.cta-button {\r\n  background-color: #00a8cc;\r\n  color: #f5f7fa;\r\n  border: none;\r\n  padding: 1rem 2.5rem;\r\n  font-size: 1.1rem;\r\n  border-radius: 30px;\r\n  cursor: pointer;\r\n  font-weight: 600;\r\n  letter-spacing: 1px;\r\n  transition: all 0.3s ease;\r\n  box-shadow: 0 4px 15px rgba(0, 168, 204, 0.4);\r\n  position: relative;\r\n  overflow: hidden;\r\n  z-index: 1;\r\n}\r\n\r\n.cta-button:hover {\r\n  background-color: #fd3a69;\r\n  transform: translateY(-3px);\r\n  box-shadow: 0 6px 20px rgba(253, 58, 105, 0.5);\r\n}\r\n\r\n.cta-button::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 0;\r\n  left: -100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(\r\n    to right,\r\n    transparent,\r\n    rgba(255, 255, 255, 0.2),\r\n    transparent\r\n  );\r\n  transition: all 0.5s ease;\r\n  z-index: -1;\r\n}\r\n\r\n.cta-button:hover::before {\r\n  left: 100%;\r\n}\r\n\r\n/* Section Styles */\r\n.section {\r\n  min-height: calc(100vh - 80px); /* Navbar height is approximately 80px */\r\n  padding: 6rem 2rem;\r\n  text-align: center;\r\n  opacity: 0;\r\n  transform: translateY(30px);\r\n  transition: all 1s ease;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.section.active {\r\n  opacity: 1;\r\n  transform: translateY(0);\r\n}\r\n\r\n.section:nth-child(odd) {\r\n  background-color: #f5f7fa;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.section:nth-child(even) {\r\n  background-color: #eef2f7;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.section::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 300px;\r\n  height: 300px;\r\n  border-radius: 50%;\r\n  background: linear-gradient(\r\n    to right,\r\n    rgba(0, 168, 204, 0.1),\r\n    rgba(253, 58, 105, 0.1)\r\n  );\r\n  top: -150px;\r\n  right: -150px;\r\n  z-index: 0;\r\n}\r\n\r\n.section::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 200px;\r\n  height: 200px;\r\n  border-radius: 50%;\r\n  background: linear-gradient(\r\n    to right,\r\n    rgba(0, 168, 204, 0.1),\r\n    rgba(253, 58, 105, 0.1)\r\n  );\r\n  bottom: -100px;\r\n  left: -100px;\r\n  z-index: 0;\r\n}\r\n\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.section-title {\r\n  font-size: 3.5rem;\r\n  margin-bottom: 1rem;\r\n  color: #1a2639;\r\n  letter-spacing: 3px;\r\n  position: relative;\r\n  display: inline-block;\r\n  text-transform: uppercase;\r\n  font-weight: 800;\r\n}\r\n\r\n.section-divider {\r\n  height: 6px;\r\n  width: 120px;\r\n  background: linear-gradient(to right, #00a8cc, #fd3a69);\r\n  margin: 0.8rem auto 4rem;\r\n  border-radius: 3px;\r\n}\r\n\r\n/* Footer Styles */\r\n.footer {\r\n  background-color: #0c1e35;\r\n  color: #f5f7fa;\r\n  padding: 2rem 0;\r\n  text-align: center;\r\n}\r\n\r\n.footer p {\r\n  opacity: 0.8;\r\n}\r\n\r\n/* Animations */\r\n@keyframes fadeInUp {\r\n  from {\r\n    transform: translateY(40px);\r\n    opacity: 0;\r\n  }\r\n  to {\r\n    transform: translateY(0);\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n/* Additional styles for mobile menu */\r\nbody.no-scroll {\r\n  overflow: hidden;\r\n}\r\n\r\n/* Media Queries for Responsive Design */\r\n@media screen and (max-width: 768px) {\r\n  .hamburger {\r\n    display: block;\r\n  }\r\n\r\n  .hamburger.active .bar:nth-child(2) {\r\n    opacity: 0;\r\n  }\r\n\r\n  .hamburger.active .bar:nth-child(1) {\r\n    transform: translateY(8px) rotate(45deg);\r\n  }\r\n\r\n  .hamburger.active .bar:nth-child(3) {\r\n    transform: translateY(-8px) rotate(-45deg);\r\n  }\r\n  .nav-menu {\r\n    position: absolute;\r\n    left: -100%;\r\n    top: 70px;\r\n    flex-direction: column;\r\n    background: rgba(10, 25, 47, 0.98);\r\n    backdrop-filter: blur(10px);\r\n    width: 250px;\r\n    text-align: center;\r\n    transition: 0.3s;\r\n    box-shadow: 0 10px 10px rgba(0, 0, 0, 0.2);\r\n    gap: 0;\r\n    padding: 2rem 0;\r\n    max-height: 350px;\r\n    border-radius: 0 0 10px 0;\r\n    overflow-y: auto;\r\n  }\r\n  .nav-item {\r\n    margin: 0.8rem 0;\r\n  }\r\n\r\n  .nav-menu.active {\r\n    left: 0;\r\n    border-left: 3px solid #00a8cc;\r\n  }\r\n\r\n  .hero-content h1 {\r\n    font-size: 2.5rem;\r\n  }\r\n\r\n  .hero-content p {\r\n    font-size: 1rem;\r\n  }\r\n\r\n  .section-title {\r\n    font-size: 2.2rem;\r\n  }\r\n\r\n  .section {\r\n    padding: 4rem 1.5rem;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 480px) {\r\n  .navbar {\r\n    padding: 1rem 4%;\r\n  }\r\n\r\n  .logo a {\r\n    font-size: 1.5rem;\r\n  }\r\n\r\n  .hero-content h1 {\r\n    font-size: 2rem;\r\n  }\r\n\r\n  .section-title {\r\n    font-size: 1.8rem;\r\n  }\r\n}\r\n\r\n/* Additional padding for sections to account for fixed navbar */\r\n#about,\r\n#services,\r\n#portfolio,\r\n#contact {\r\n  padding-top: 100px; /* Navbar height + some extra space */\r\n}\r\n</style>\r\n"],"mappings":"AAwEA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,QAAQ,EAAE,KAAK;MACfC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;IACpD,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACT,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;MAClCU,QAAQ,CAACC,IAAI,CAACC,SAAS,CAACC,MAAM,CAAC,WAAW,EAAE,IAAI,CAACb,UAAU,CAAC;IAC9D,CAAC;IACDc,SAASA,CAAA,EAAG;MACV,IAAI,CAACd,UAAS,GAAI,KAAK;MACvBU,QAAQ,CAACC,IAAI,CAACC,SAAS,CAACG,MAAM,CAAC,WAAW,CAAC;IAC7C,CAAC;IACDT,YAAYA,CAAA,EAAG;MACb,IAAI,CAACL,QAAO,GAAIG,MAAM,CAACY,OAAM,GAAI,GAAG;MAEpC,MAAMC,QAAO,GAAIP,QAAQ,CAACQ,gBAAgB,CAAC,SAAS,CAAC;MACrD,MAAMC,YAAW,GAAIT,QAAQ,CAACU,aAAa,CAAC,SAAS,CAAC,CAACC,YAAY;MACnE,MAAMC,SAAQ,GAAIlB,MAAM,CAACY,OAAM,GAAIG,YAAY;MAE/CF,QAAQ,CAACM,OAAO,CAAEC,OAAO,IAAK;QAC5B,MAAMC,SAAQ,GAAID,OAAO,CAACC,SAAS;QACnC,MAAMC,MAAK,GAAIF,OAAO,CAACH,YAAY;QAEnC,IAAIC,SAAQ,IAAKG,SAAQ,IAAKH,SAAQ,GAAIG,SAAQ,GAAIC,MAAM,EAAE;UAC5D,IAAI,CAACxB,aAAY,GAAIsB,OAAO,CAACG,YAAY,CAAC,IAAI,CAAC;QACjD;QAEA,MAAMC,UAAS,GAAIJ,OAAO,CAACK,qBAAqB,CAAC,CAAC,CAACC,GAAG;QACtD,IAAIF,UAAS,GAAIxB,MAAM,CAAC2B,WAAU,GAAI,GAAG,EAAE;UACzCP,OAAO,CAACZ,SAAS,CAACoB,GAAG,CAAC,QAAQ,CAAC;QACjC;MACF,CAAC,CAAC;IACJ,CAAC;IACDzB,cAAcA,CAAA,EAAG;MACf0B,UAAU,CAAC,MAAM,IAAI,CAAC3B,YAAY,CAAC,CAAC,EAAE,EAAE,CAAC;IAC3C;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}