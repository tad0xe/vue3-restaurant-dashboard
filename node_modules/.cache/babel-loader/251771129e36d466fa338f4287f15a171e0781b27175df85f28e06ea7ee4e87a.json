{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"BankManager\",\n  data() {\n    return {\n      form: {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\"\n        // Replace or set dynamically if needed\n      },\n\n      banks: [],\n      isEditing: false,\n      editId: null\n    };\n  },\n  mounted() {\n    this.fetchBanks();\n  },\n  computed: {\n    ...mapGetters([\"getToken\"])\n  },\n  methods: {\n    async fetchBanks() {\n      try {\n        const res = await axios.get(\"http://localhost:4000/api/bank\", {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        console.log(\"Fetched banks:\", res.data);\n        this.banks = res.data.banks;\n      } catch (err) {\n        console.error(\"Error fetching banks:\", err);\n      }\n    },\n    async createBank() {\n      try {\n        const res = await axios.post(\"http://localhost:4000/api/bank\", this.form, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        console.log(\"Bank created:\", res.data);\n        alert(\"‚úÖ Bank created successfully!\");\n        this.banks.push(res.data);\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error creating bank:\", err);\n        alert(\"‚ùå Failed to create.\");\n      }\n    },\n    editBank(bank) {\n      this.isEditing = true;\n      this.editId = bank._id;\n      this.form = {\n        ...bank\n      };\n    },\n    cancelEdit() {\n      this.resetForm();\n    },\n    async updateBank() {\n      try {\n        const res = await axios.put(`http://localhost:4000/api/bank/${this.editId}`, this.form, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        const index = this.banks.findIndex(b => b._id === this.editId);\n        if (index !== -1) this.banks[index] = res.data.data;\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error updating bank:\", err);\n        alert(\"‚ùå Update failed.\");\n      }\n    },\n    async deleteBank(id) {\n      if (!confirm(\"Delete this bank info?\")) return;\n      try {\n        await axios.delete(`http://localhost:4000/api/bank/${id}`, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        this.banks = this.banks.filter(b => b._id !== id);\n      } catch (err) {\n        console.error(\"Error deleting bank:\", err);\n        alert(\"‚ùå Delete failed.\");\n      }\n    },\n    resetForm() {\n      this.form = {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\"\n      };\n      this.isEditing = false;\n      this.editId = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","form","accountName","accountNumber","bankName","banks","isEditing","editId","mounted","fetchBanks","computed","methods","res","get","headers","Authorization","getToken","console","log","err","error","createBank","post","alert","push","resetForm","editBank","bank","_id","cancelEdit","updateBank","put","index","findIndex","b","deleteBank","id","confirm","delete","filter"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\app\\calender.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-4\">\r\n    <h2>üè¶ Bank Info Manager</h2>\r\n\r\n    <!-- ‚úÖ Create or Edit Form -->\r\n    <form @submit.prevent=\"isEditing ? updateBank() : createBank()\" class=\"card p-3 mb-4 shadow-sm\">\r\n      <h5>{{ isEditing ? '‚úèÔ∏è Edit Bank Info' : '‚ûï Add Bank Info' }}</h5>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Account Name</label>\r\n        <input v-model=\"form.accountName\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Account Number</label>\r\n        <input v-model=\"form.accountNumber\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Bank Name</label>\r\n        <input v-model=\"form.bankName\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"btn btn-success\">{{ isEditing ? 'Update' : 'Create' }}</button>\r\n      <button v-if=\"isEditing\" type=\"button\" class=\"btn btn-secondary ms-2\" @click=\"cancelEdit\">Cancel</button>\r\n    </form>\r\n\r\n    <!-- ‚úÖ Bank Info List -->\r\n    <div>\r\n      <h5>üìã Saved Bank Info</h5>\r\n      <ul class=\"list-group\">\r\n        <li v-for=\"bank in banks\" :key=\"bank._id\" class=\"list-group-item d-flex justify-content-between align-items-center\">\r\n          <div>\r\n            <strong>{{ bank.accountName }}</strong><br />\r\n            {{ bank.accountNumber }} ‚Äî {{ bank.bankName }}\r\n          </div>\r\n          <div>\r\n            <button class=\"btn btn-sm btn-warning me-2\" @click=\"editBank(bank)\">Edit</button>\r\n            <button class=\"btn btn-sm btn-danger\" @click=\"deleteBank(bank._id)\">Delete</button>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"BankManager\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n         // Replace or set dynamically if needed\r\n      },\r\n      banks: [],\r\n      isEditing: false,\r\n      editId: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchBanks();\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n  methods: {\r\n    async fetchBanks() {\r\n      try {\r\n        const res = await axios.get(\"http://localhost:4000/api/bank\",\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${this.getToken}`,\r\n            },\r\n          }\r\n        );\r\n        console.log(\"Fetched banks:\", res.data);\r\n        this.banks = res.data.banks;\r\n      } catch (err) {\r\n        console.error(\"Error fetching banks:\", err);\r\n      }\r\n    },\r\n    async createBank() {\r\n      try {\r\n        const res = await axios.post(\"http://localhost:4000/api/bank\", this.form,{\r\n          headers: {\r\n            Authorization: `Bearer ${this.getToken}`,\r\n          }\r\n        });\r\n        console.log(\"Bank created:\", res.data);\r\n        alert(\"‚úÖ Bank created successfully!\");\r\n        this.banks.push(res.data);\r\n\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error creating bank:\", err);\r\n        alert(\"‚ùå Failed to create.\");\r\n      }\r\n    },\r\n    editBank(bank) {\r\n      this.isEditing = true;\r\n      this.editId = bank._id;\r\n      this.form = { ...bank };\r\n    },\r\n    cancelEdit() {\r\n      this.resetForm();\r\n    },\r\n    async updateBank() {\r\n      try {\r\n        const res = await axios.put(`http://localhost:4000/api/bank/${this.editId}`, this.form,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${this.getToken}`,\r\n            }\r\n          }\r\n        );\r\n        const index = this.banks.findIndex((b) => b._id === this.editId);\r\n        if (index !== -1) this.banks[index] = res.data.data;\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error updating bank:\", err);\r\n        alert(\"‚ùå Update failed.\");\r\n      }\r\n    },\r\n    async deleteBank(id) {\r\n      if (!confirm(\"Delete this bank info?\")) return;\r\n      try {\r\n        await axios.delete(`http://localhost:4000/api/bank/${id}`,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${this.getToken}`,\r\n            }\r\n          }\r\n        );\r\n        this.banks = this.banks.filter((b) => b._id !== id);\r\n      } catch (err) {\r\n        console.error(\"Error deleting bank:\", err);\r\n        alert(\"‚ùå Delete failed.\");\r\n      }\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n      };\r\n      this.isEditing = false;\r\n      this.editId = null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAgDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,WAAW,EAAE,EAAE;QACfC,aAAa,EAAE,EAAE;QACjBC,QAAQ,EAAE;QACT;MACH,CAAC;;MACDC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGZ,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EACDa,OAAO,EAAE;IACP,MAAMF,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMG,GAAE,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,gCAAgC,EAC1D;UACEC,OAAO,EAAE;YACPC,aAAa,EAAG,UAAS,IAAI,CAACC,QAAS;UACzC;QACF,CACF,CAAC;QACDC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEN,GAAG,CAACZ,IAAI,CAAC;QACvC,IAAI,CAACK,KAAI,GAAIO,GAAG,CAACZ,IAAI,CAACK,KAAK;MAC7B,EAAE,OAAOc,GAAG,EAAE;QACZF,OAAO,CAACG,KAAK,CAAC,uBAAuB,EAAED,GAAG,CAAC;MAC7C;IACF,CAAC;IACD,MAAME,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMT,GAAE,GAAI,MAAMf,KAAK,CAACyB,IAAI,CAAC,gCAAgC,EAAE,IAAI,CAACrB,IAAI,EAAC;UACvEa,OAAO,EAAE;YACPC,aAAa,EAAG,UAAS,IAAI,CAACC,QAAS;UACzC;QACF,CAAC,CAAC;QACFC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEN,GAAG,CAACZ,IAAI,CAAC;QACtCuB,KAAK,CAAC,8BAA8B,CAAC;QACrC,IAAI,CAAClB,KAAK,CAACmB,IAAI,CAACZ,GAAG,CAACZ,IAAI,CAAC;QAEzB,IAAI,CAACyB,SAAS,CAAC,CAAC;MAClB,EAAE,OAAON,GAAG,EAAE;QACZF,OAAO,CAACG,KAAK,CAAC,sBAAsB,EAAED,GAAG,CAAC;QAC1CI,KAAK,CAAC,qBAAqB,CAAC;MAC9B;IACF,CAAC;IACDG,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,CAACrB,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACC,MAAK,GAAIoB,IAAI,CAACC,GAAG;MACtB,IAAI,CAAC3B,IAAG,GAAI;QAA