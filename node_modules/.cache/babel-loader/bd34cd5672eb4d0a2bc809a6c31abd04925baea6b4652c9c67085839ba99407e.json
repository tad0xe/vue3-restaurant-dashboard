{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapActions } from \"vuex\";\nexport default {\n  props: {\n    product: {\n      type: Object,\n      required: true\n    }\n  },\n  data() {\n    return {\n      products: [],\n      product: {\n        photos: [],\n        price: 0 // Default price\n      },\n\n      selectedTab: 0,\n      quantity: 1,\n      // Default quantity,\n      coordinates: null,\n      // Coordinates for Google Maps\n      API_KEY: \"AIzaSyCkbHZXmqyIxjFpN8bMvDeURTmlpEhDHv4\" // ⚠️ Replace with your actual key\n    };\n  },\n\n  async created() {\n    try {\n      // Retrieve the raw product title from Vuex\n      let productTitle = this.$store.getters.getProductUrl;\n      console.log(\"Raw Product Title from Vuex:\", productTitle);\n\n      // Send the raw title to the backend (encode for HTTP request)\n      const response = await axios.get(`https://event-ticket-qa70.onrender.com/api/products/${encodeURIComponent(productTitle)}`);\n      this.product = response.data.product || {};\n      this.coordinates = this.product.event.location.coordinates || null;\n    } catch (error) {\n      console.error(\"Error fetching product details:\", error);\n    }\n    try {\n      const response = await axios.get(`https://event-ticket-qa70.onrender.com/api/products`);\n      console.log(\"API Response:\", response.data);\n      this.products = this.shuffleArray(response.data.products || []).slice(0, 3);\n    } catch (error) {\n      console.error(\"Error fetching products:\", error);\n    }\n  },\n  computed: {\n    mapUrl() {\n      if (!this.coordinates) return \"\";\n      const {\n        lat,\n        lng\n      } = this.coordinates;\n      return `https://www.google.com/maps/embed/v1/view?key=${this.API_KEY}&center=${lat},${lng}&zoom=15`;\n    },\n    totalPrice() {\n      return this.product.price * this.quantity;\n    },\n    productTitle() {\n      return this.$store.state.productTitle;\n    }\n  },\n  methods: {\n    ...mapActions([\"addProductToCart\"]),\n    formatDate(date) {\n      if (!date) return \"No date\";\n      const options = {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return new Date(date).toLocaleDateString(\"en-US\", options);\n    },\n    formatPrice(price) {\n      if (!price || price === 0) return \"Free\";\n      return `$${price}`;\n    },\n    slugify(text) {\n      return text.toLowerCase().trim().replace(/\\s+/g, \"-\") // Replace spaces with hyphens\n      .replace(/[^\\w\\s'-]/g, \"\"); // Remove special characters, keep apostrophes\n    },\n\n    goToProduct(productTitle) {\n      // Normalize the product title for the URL: Replace spaces with hyphens\n      let normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\n      console.log(\"Normalized Product Title for URL:\", normalizedProductTitle);\n\n      // Store the raw title in Vuex (for the backend)\n      this.$store.dispatch(\"setProductUrl\", productTitle);\n\n      // Use the normalized title in the URL (for better SEO)\n      this.$router.push({\n        name: \"ProductDetails\",\n        params: {\n          title: normalizedProductTitle\n        } // For URL with hyphens\n      });\n\n      window.location.reload();\n    },\n    buyNow(product) {\n      this.addProductToCart(product); // Add product to cart\n      this.$router.push(\"/cart\"); // Redirect to checkout page\n    },\n\n    shuffleArray(array) {\n      return array.sort(() => Math.random() - 0.5);\n    },\n    onChangeQuantity(event, product) {\n      let qty = parseInt(event.target.value);\n      console.log(\"Selected Quantity:\", qty);\n      if (!isNaN(qty)) {\n        this.quantity = qty;\n        this.$store.commit(\"changeQty\", {\n          product,\n          qty\n        });\n      } else {\n        console.error(\"Invalid Quantity Selected\");\n      }\n    }\n  },\n  watch: {\n    product: {\n      handler(newProduct) {\n        if (newProduct && newProduct.photos && newProduct.photos.length > 0) {\n          this.selectedPhoto = newProduct.photos[0];\n        }\n      },\n      deep: true,\n      immediate: true\n    }\n  }\n};","map":{"version":3,"names":["axios","mapActions","props","product","type","Object","required","data","products","photos","price","selectedTab","quantity","coordinates","API_KEY","created","productTitle","$store","getters","getProductUrl","console","log","response","get","encodeURIComponent","event","location","error","shuffleArray","slice","computed","mapUrl","lat","lng","totalPrice","state","methods","formatDate","date","options","weekday","year","month","day","Date","toLocaleDateString","formatPrice","slugify","text","toLowerCase","trim","replace","goToProduct","normalizedProductTitle","dispatch","$router","push","name","params","title","window","reload","buyNow","addProductToCart","array","sort","Math","random","onChangeQuantity","qty","parseInt","target","value","isNaN","commit","watch","handler","newProduct","length","selectedPhoto","deep","immediate"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\Product.vue"],"sourcesContent":["<template>\r\n  <div class=\"text-start my-5\">\r\n    <div class=\"background-wrapper container\">\r\n      <img\r\n        class=\"img-fluid\"\r\n        :src=\"\r\n          product.photos[0] ||\r\n          'https://via.placeholder.com/400x300?text=No+Image'\r\n        \"\r\n      />\r\n\r\n      <!-- Main content -->\r\n    </div>\r\n    <div class=\"container\">\r\n      <!-- Blurred background -->\r\n\r\n      <div class=\"row pt-3 text-start\">\r\n        <!-- IMAGE COLUMN -->\r\n        <div class=\"col-md-6\">\r\n          <div class=\"tab-content\"></div>\r\n          <div class=\"\">\r\n            <!-- Event Title -->\r\n            <h2 class=\"fw-bold\">{{ product.title }}</h2>\r\n\r\n            <!-- Date, Time, Location -->\r\n            <div class=\"mt-3\">\r\n              <p class=\"mb-1\">\r\n                <i class=\"bi bi-calendar-event me-2\"></i>\r\n                {{ formatDate(product.event?.date) }}\r\n              </p>\r\n\r\n              <p class=\"mb-2\">\r\n                <i class=\"bi bi-clock me-2\"></i>\r\n                {{ product.event.startTime }} -\r\n                {{ product.event.endTime }}\r\n              </p>\r\n              <p class=\"mb-2\">\r\n                <i class=\"bi bi-geo-alt me-2\"></i>\r\n                {{ product.event.location.name || \"No location\" }}\r\n              </p>\r\n              <div v-if=\"coordinates\" class=\"mt-4\">\r\n                <iframe\r\n                  :src=\"mapUrl\"\r\n                  width=\"100%\"\r\n                  height=\"300\"\r\n                  style=\"border: 0\"\r\n                  allowfullscreen=\"\"\r\n                  loading=\"lazy\"\r\n                  referrerpolicy=\"no-referrer-when-downgrade\"\r\n                ></iframe>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Address Tag -->\r\n\r\n            <div\r\n              class=\"card shadow-sm custom-card-mobile mt-3 d-block d-md-none\"\r\n            >\r\n              <!-- Price -->\r\n              <div\r\n                class=\"d-flex justify-content-between align-items-center mb-3\"\r\n              >\r\n                <span class=\"text-muted fw-medium\">Price</span>\r\n                <span class=\"fw-bold fs-5\"\r\n                  >${{ formatPrice(product.price) }}</span\r\n                >\r\n              </div>\r\n\r\n              <!-- Get Tickets Button -->\r\n              <div class=\"d-grid mb-2\">\r\n                <button\r\n                  @click=\"addProductToCart(product)\"\r\n                  class=\"btn text-decoration-none btn-primary w-100 btn-primary:hover\"\r\n                >\r\n                  <router-link class=\"\" to=\"/checkout\"\r\n                    >Get Tickets\r\n                  </router-link>\r\n                </button>\r\n              </div>\r\n            </div>\r\n\r\n            <hr />\r\n\r\n            <!-- TAGS SECTION -->\r\n            <div class=\"mb-5\">\r\n              <h5 class=\"fw-bold text-dark\">Tags</h5>\r\n              <div class=\"d-flex flex-wrap gap-2 mt-3\">\r\n                <div>\r\n                  <span\r\n                    v-for=\"(tag, index) in product.tag\"\r\n                    :key=\"index\"\r\n                    class=\"tag-badge\"\r\n                  >\r\n                    #{{ tag }}\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Thumbnails -->\r\n          <ul class=\"nav slide-item-list mt-3\" role=\"tablist\">\r\n            <li\r\n              v-for=\"(photo, index) in product.photos\"\r\n              :key=\"index\"\r\n              role=\"presentation\"\r\n            >\r\n              <a href=\"#\" @click.prevent=\"selectedTab = index\">\r\n                <img\r\n                  class=\"img-fluid\"\r\n                  src=\"/c96a641b-4e06-4d15-94e5-4633c6cdfef4_large.webp\"\r\n                />\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <div class=\"card shadow-sm custom-card d-none d-md-block\">\r\n            <!-- Price -->\r\n            <div class=\"d-flex justify-content-between align-items-center mb-3\">\r\n              <span class=\"text-muted fw-medium\">Price</span>\r\n              <span class=\"fw-bold fs-5\">{{ formatPrice(product.price) }}</span>\r\n            </div>\r\n\r\n            <!-- Get Tickets Button -->\r\n            <div class=\"d-grid mb-2\">\r\n              <button\r\n                @click=\"addProductToCart(product)\"\r\n                class=\"btn text-decoration-none btn-primary w-100 btn-primary:hover\"\r\n              >\r\n                <router-link class=\"\" to=\"/checkout\">Get Tickets </router-link>\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- DETAILS COLUMN -->\r\n        <div class=\"pt-2 row\"></div>\r\n        <div class=\"text-start pt-3 ps-0 ms-0 ps-lg-5 ms-lg-5\">\r\n          <b> About this item </b>\r\n          <p class=\"text-muted\">\r\n            {{ product.description }}\r\n          </p>\r\n        </div>\r\n        <h1 class=\"text-start fw-bold\">You may also like</h1>\r\n        <div\r\n          class=\"col-md-4 pt-3 pt-md-0\"\r\n          v-for=\"product in products\"\r\n          :key=\"product._id\"\r\n        >\r\n          <div>\r\n            <img\r\n              :src=\"\r\n                product.photos[0] ||\r\n                'https://via.placeholder.com/400x300?text=No+Image'\r\n              \"\r\n              class=\"img-fluid rounded w-100\"\r\n              style=\"height: 200px; object-fit: cover\"\r\n              alt=\"Event Image\"\r\n            />\r\n            <div class=\"pt-2\">\r\n              <h5>{{ product.title }}</h5>\r\n              <p class=\"mb-1\">\r\n                <i class=\"bi bi-calendar-event me-2\"></i>\r\n                {{ formatDate(product.event?.date) }}\r\n              </p>\r\n              <p class=\"mb-1\">\r\n                <i class=\"bi bi-geo-alt me-2\"></i>\r\n                {{ product.event.location.name || \"No location\" }}\r\n              </p>\r\n              <p>{{ formatPrice(product.price) }}</p>\r\n              <button\r\n                class=\"btn btn-outline-primary\"\r\n                @click=\"goToProduct(product.title)\"\r\n              >\r\n                View event details\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapActions } from \"vuex\";\r\n\r\nexport default {\r\n  props: {\r\n    product: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      product: {\r\n        photos: [],\r\n        price: 0, // Default price\r\n      },\r\n      selectedTab: 0,\r\n      quantity: 1, // Default quantity,\r\n      coordinates: null, // Coordinates for Google Maps\r\n      API_KEY: \"AIzaSyCkbHZXmqyIxjFpN8bMvDeURTmlpEhDHv4\", // ⚠️ Replace with your actual key\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      // Retrieve the raw product title from Vuex\r\n      let productTitle = this.$store.getters.getProductUrl;\r\n      console.log(\"Raw Product Title from Vuex:\", productTitle);\r\n\r\n      // Send the raw title to the backend (encode for HTTP request)\r\n      const response = await axios.get(\r\n        `https://event-ticket-qa70.onrender.com/api/products/${encodeURIComponent(\r\n          productTitle\r\n        )}`\r\n      );\r\n\r\n      this.product = response.data.product || {};\r\n\r\n      this.coordinates = this.product.event.location.coordinates || null;\r\n    } catch (error) {\r\n      console.error(\"Error fetching product details:\", error);\r\n    }\r\n\r\n    try {\r\n      const response = await axios.get(\r\n        `https://event-ticket-qa70.onrender.com/api/products`\r\n      );\r\n      console.log(\"API Response:\", response.data);\r\n\r\n      this.products = this.shuffleArray(response.data.products || []).slice(\r\n        0,\r\n        3\r\n      );\r\n    } catch (error) {\r\n      console.error(\"Error fetching products:\", error);\r\n    }\r\n  },\r\n  computed: {\r\n    mapUrl() {\r\n      if (!this.coordinates) return \"\";\r\n      const { lat, lng } = this.coordinates;\r\n      return `https://www.google.com/maps/embed/v1/view?key=${this.API_KEY}&center=${lat},${lng}&zoom=15`;\r\n    },\r\n    totalPrice() {\r\n      return this.product.price * this.quantity;\r\n    },\r\n    productTitle() {\r\n      return this.$store.state.productTitle;\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions([\"addProductToCart\"]),\r\n    formatDate(date) {\r\n      if (!date) return \"No date\";\r\n      const options = {\r\n        weekday: \"long\",\r\n        year: \"numeric\",\r\n        month: \"long\",\r\n        day: \"numeric\",\r\n      };\r\n      return new Date(date).toLocaleDateString(\"en-US\", options);\r\n    },\r\n    formatPrice(price) {\r\n      if (!price || price === 0) return \"Free\";\r\n      return `$${price}`;\r\n    },\r\n    slugify(text) {\r\n      return text\r\n        .toLowerCase()\r\n        .trim()\r\n        .replace(/\\s+/g, \"-\") // Replace spaces with hyphens\r\n        .replace(/[^\\w\\s'-]/g, \"\"); // Remove special characters, keep apostrophes\r\n    },\r\n    goToProduct(productTitle) {\r\n      // Normalize the product title for the URL: Replace spaces with hyphens\r\n      let normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\r\n\r\n      console.log(\"Normalized Product Title for URL:\", normalizedProductTitle);\r\n\r\n      // Store the raw title in Vuex (for the backend)\r\n      this.$store.dispatch(\"setProductUrl\", productTitle);\r\n\r\n      // Use the normalized title in the URL (for better SEO)\r\n      this.$router.push({\r\n        name: \"ProductDetails\",\r\n        params: { title: normalizedProductTitle }, // For URL with hyphens\r\n      });\r\n      window.location.reload();\r\n    },\r\n\r\n    buyNow(product) {\r\n      this.addProductToCart(product); // Add product to cart\r\n      this.$router.push(\"/cart\"); // Redirect to checkout page\r\n    },\r\n    shuffleArray(array) {\r\n      return array.sort(() => Math.random() - 0.5);\r\n    },\r\n    onChangeQuantity(event, product) {\r\n      let qty = parseInt(event.target.value);\r\n      console.log(\"Selected Quantity:\", qty);\r\n\r\n      if (!isNaN(qty)) {\r\n        this.quantity = qty;\r\n        this.$store.commit(\"changeQty\", { product, qty });\r\n    