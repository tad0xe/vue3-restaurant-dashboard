{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapActions } from \"vuex\";\nexport default {\n  props: {\n    product: {\n      type: Object,\n      required: true\n    }\n  },\n  data() {\n    return {\n      products: [],\n      product: {\n        photos: [],\n        price: 0 // Default price\n      },\n\n      selectedTab: 0,\n      quantity: 1 // Default quantity,\n    };\n  },\n\n  async created() {\n    try {\n      // Retrieve the raw product title from Vuex\n      let productTitle = this.$store.getters.getProductUrl;\n      console.log(\"Raw Product Title from Vuex:\", productTitle);\n\n      // Send the raw title to the backend (encode for HTTP request)\n      const response = await axios.get(`http://localhost:5000/api/products/${encodeURIComponent(productTitle)}`);\n      this.product = response.data.product || {};\n    } catch (error) {\n      console.error(\"Error fetching product details:\", error);\n    }\n    try {\n      const response = await axios.get(`http://localhost:5000/api/products`);\n      console.log(\"API Response:\", response.data);\n      const tags = [\"Sale\", \"Deal\", \"Limited Edition\", \"100% orgasm\", \"As Seen on TV\", \"Best Seller\", \"Top Rated\"];\n      this.products = this.shuffleArray(response.data.products || []).slice(0, 3).map(product => ({\n        ...product,\n        randomTag: Math.random() < 0.5 ? tags[Math.floor(Math.random() * tags.length)] : null\n        // 50% chance of getting a tag\n      }));\n    } catch (error) {\n      console.error(\"Error fetching products:\", error);\n    }\n  },\n  computed: {\n    totalPrice() {\n      return this.product.price * this.quantity;\n    },\n    productTitle() {\n      return this.$store.state.productTitle;\n    }\n  },\n  methods: {\n    async addProductToCart(product) {\n      try {\n        await this.$store.dispatch(\"addProductToCart\", product); // call Vuex action manually\n        this.$toast.open({\n          message: \"✅ Product added to cart!\",\n          type: \"success\",\n          duration: 3000,\n          dismissible: true,\n          position: \"top\"\n        });\n      } catch (error) {\n        this.$toast.open({\n          message: \"✅ Product added to cart!\",\n          type: \"success\",\n          duration: 3000,\n          dismissible: true,\n          position: \"top\"\n        });\n      }\n    },\n    slugify(text) {\n      return text.toLowerCase().trim().replace(/\\s+/g, \"-\") // Replace spaces with hyphens\n      .replace(/[^\\w\\s'-]/g, \"\"); // Remove special characters, keep apostrophes\n    },\n\n    goToProduct(productTitle) {\n      // Normalize the product title for the URL: Replace spaces with hyphens\n      let normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\n      console.log(\"Normalized Product Title for URL:\", normalizedProductTitle);\n\n      // Store the raw title in Vuex (for the backend)\n      this.$store.dispatch(\"setProductUrl\", productTitle);\n\n      // Use the normalized title in the URL (for better SEO)\n      this.$router.push({\n        name: \"ProductDetails\",\n        params: {\n          title: normalizedProductTitle\n        } // For URL with hyphens\n      });\n\n      window.location.reload();\n    },\n    buyNow(product) {\n      this.addProductToCart(product); // Add product to cart\n      this.$router.push(\"/cart\"); // Redirect to checkout page\n    },\n\n    shuffleArray(array) {\n      return array.sort(() => Math.random() - 0.5);\n    },\n    onChangeQuantity(event, product) {\n      let qty = parseInt(event.target.value);\n      console.log(\"Selected Quantity:\", qty);\n      if (!isNaN(qty)) {\n        this.quantity = qty;\n        this.$store.commit(\"changeQty\", {\n          product,\n          qty\n        });\n      } else {\n        console.error(\"Invalid Quantity Selected\");\n      }\n    }\n  },\n  watch: {\n    product: {\n      handler(newProduct) {\n        if (newProduct && newProduct.photos && newProduct.photos.length > 0) {\n          this.selectedPhoto = newProduct.photos[0];\n        }\n      },\n      deep: true,\n      immediate: true\n    }\n  }\n};","map":{"version":3,"names":["axios","mapActions","props","product","type","Object","required","data","products","photos","price","selectedTab","quantity","created","productTitle","$store","getters","getProductUrl","console","log","response","get","encodeURIComponent","error","tags","shuffleArray","slice","map","randomTag","Math","random","floor","length","computed","totalPrice","state","methods","addProductToCart","dispatch","$toast","open","message","duration","dismissible","position","slugify","text","toLowerCase","trim","replace","goToProduct","normalizedProductTitle","$router","push","name","params","title","window","location","reload","buyNow","array","sort","onChangeQuantity","event","qty","parseInt","target","value","isNaN","commit","watch","handler","newProduct","selectedPhoto","deep","immediate"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\vue3-restaurant-dashboard\\src\\components\\Product.vue"],"sourcesContent":["<template>\r\n  <div class=\"container text-start my-5\">\r\n    <img class=\"\" alt=\"Live event\" />\r\n\r\n    <div class=\"container\">\r\n      <!-- IMAGE + DETAILS -->\r\n      <div class=\"row pt-3 text-start\">\r\n        <!-- IMAGE COLUMN -->\r\n        <div class=\"col-sm-6\">\r\n          <div class=\"tab-content\">\r\n            <div\r\n\r\n              :key=\"index\"\r\n              :id=\"'tab' + index\"\r\n              class=\"tab-pane product-img fade\"\r\n              :class=\"{ 'show active': selectedTab === index }\"\r\n            >\r\n              <img\r\n                class=\"border-0  img-fluid\"\r\n                src=\"https://images.unsplash.com/photo-1521737604893-d14cc237f11d\"\r\n                alt=\"Product Image\"\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Thumbnails -->\r\n          <ul class=\"nav slide-item-list mt-3\" role=\"tablist\">\r\n            <li\r\n              v-for=\"(photo, index) in product.photos\"\r\n              :key=\"index\"\r\n              role=\"presentation\"\r\n            >\r\n              <a href=\"#\" @click.prevent=\"selectedTab = index\">\r\n                <img class=\"img-fluid\"  src=\"/c96a641b-4e06-4d15-94e5-4633c6cdfef4_large.webp\"/>\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n\r\n        <!-- DETAILS COLUMN -->\r\n        <div class=\"col-sm-6\">\r\n          <div class=\"mt-3 mt-sm-0\">\r\n            <h4 class=\"w-75\">{{ product.title }}</h4>\r\n\r\n            <!-- Rating -->\r\n            <div class=\"d-flex rating justify-content-start\">\r\n              <span class=\"fas fa-star\"></span>\r\n              <span class=\"fas fa-star\"></span>\r\n              <span class=\"fas fa-star\"></span>\r\n              <span class=\"fas fa-star\"></span>\r\n              <span class=\"fas fa-star\"></span>\r\n            </div>\r\n\r\n            <!-- Price -->\r\n            <div class=\"row pt-2\">\r\n              <div class=\"text-start\">\r\n                $\r\n                <span class=\"price col-6 col-md-12\"\r\n                  >{{ product.price }}.99</span\r\n                >\r\n              </div>\r\n              <p class=\"text-muted col-md-12\">Delivery Thu Apr 1</p>\r\n            </div>\r\n\r\n            <!-- Quantity -->\r\n            <select\r\n              class=\"form-control w-25 text-center bg-white mr-2\"\r\n              @change=\"onChangeQuantity($event, product)\"\r\n            >\r\n              <option v-for=\"i in 10\" :key=\"i\" :value=\"i\">\r\n                Qty: &nbsp;{{ i }}\r\n              </option>\r\n            </select>\r\n\r\n            <p class=\"text-muted pt-2\">Shipping calculated at checkout</p>\r\n\r\n            <!-- Buttons -->\r\n            <div class=\"row\">\r\n              <div class=\"col-12 col-md-3\">\r\n                <div class=\"mb-2\">\r\n                  <button\r\n                    @click=\"addProductToCart(product)\"\r\n                    class=\"btn btn-outline-dark w-100 btn-custom mt-lg-2 mt-0\"\r\n                  >\r\n                    Add to Cart\r\n                  </button>\r\n                </div>\r\n                <div class=\"pt-2\">\r\n                  <button\r\n                    @click=\"buyNow(product)\"\r\n                    class=\"btn btn-dark w-100 btn-custom mt-lg-2 mt-0\"\r\n                  >\r\n                    Buy it Now\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Product Details -->\r\n            <div style=\"line-height: 1.2\">\r\n              <div class=\"mt-3\">\r\n                <p class=\"fw-bold mb-0\">\r\n                  BRAND: <span class=\"fw-normal\">{{ product.brand }}</span>\r\n                </p>\r\n                <p class=\"fw-bold mb-0\">\r\n                  LENGTH:\r\n                  <span class=\"fw-normal\">{{ product.size }} inches</span>\r\n                </p>\r\n              </div>\r\n              <div>\r\n                <p class=\"fw-bold mb-0\">\r\n                  MATERIAL:\r\n                  <span class=\"fw-normal\">{{ product.material }}</span>\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"text-start pt-3 ps-0 ms-0 ps-lg-5 ms-lg-5\">\r\n        <b> About this item </b>\r\n        <p class=\"text-muted\">\r\n          {{ product.description }}\r\n        </p>\r\n      </div>\r\n      <h1 class=\"text-start fw-bold\">You may also like</h1>\r\n      <div v-if=\"products.length\" class=\"pt-3\">\r\n        <div class=\"row\">\r\n          <div\r\n            class=\"col-lg-4 col-sm-6 d-flex flex-column align-items-center justify-content-center product-item\"\r\n            v-for=\"product in products\"\r\n            :key=\"product._id\"\r\n          >\r\n            <div class=\"product\">\r\n              <img :src=\"product.photos[0]\" class=\"img-fluid\" alt=\"\" />\r\n              <ul\r\n                class=\"d-flex align-items-center justify-content-center list-unstyled icons\"\r\n              >\r\n                <li\r\n                  class=\"icon\"\r\n                  data-bs-toggle=\"tooltip\"\r\n                  title=\"View Description\"\r\n                  @click=\"goToProduct(product.title)\"\r\n                >\r\n                  <span class=\"fas fa-expand-arrows-alt\"></span>\r\n                </li>\r\n                <li\r\n                  class=\"icon mx-3\"\r\n                  data-bs-toggle=\"tooltip\"\r\n                  title=\"Add to Wishlist\"\r\n                >\r\n                  <span class=\"far fa-heart\"></span>\r\n                </li>\r\n                <li\r\n                  class=\"icon\"\r\n                  @click=\"addProductToCart(product)\"\r\n                  data-bs-toggle=\"tooltip\"\r\n                  title=\"Add to Cart\"\r\n                >\r\n                  <span class=\"fas fa-shopping-bag\"></span>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"tag bg-red\">{{ product.randomTag }}</div>\r\n            <div class=\"flex-column text-start w-100 justify-content-start\">\r\n              <div class=\"title pt-4 pb-1 text-start\">{{ product.title }}</div>\r\n              <div class=\"d-flex align-content-start justify-content-start\">\r\n                <span class=\"fas fa-star\"></span>\r\n                <span class=\"fas fa-star\"></span>\r\n                <span class=\"fas fa-star\"></span>\r\n                <span class=\"fas fa-star\"></span>\r\n                <span class=\"fas fa-star\"></span>\r\n              </div>\r\n              \r\n              <div class=\"text-start\">\r\n                $ <span class=\"price\">{{ product.price }}.999</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapActions } from \"vuex\";\r\n\r\nexport default {\r\n  props: {\r\n    product: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      product: {\r\n        photos: [],\r\n        price: 0, // Default price\r\n      },\r\n      selectedTab: 0,\r\n      quantity: 1, // Default quantity,\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      // Retrieve the raw product title from Vuex\r\n      let productTitle = this.$store.getters.getProductUrl;\r\n      console.log(\"Raw Product Title from Vuex:\", productTitle);\r\n\r\n      // Send the raw title to the backend (encode for HTTP request)\r\n      const response = await axios.get(\r\n        `http://localhost:5000/api/products/${encodeURIComponent(productTitle)}`\r\n      );\r\n\r\n      this.product = response.data.product || {};\r\n    } catch (error) {\r\n      console.error(\"Error fetching product details:\", error);\r\n    }\r\n\r\n    try {\r\n      const response = await axios.get(`http://localhost:5000/api/products`);\r\n      console.log(\"API Response:\", response.data);\r\n      const tags = [\r\n        \"Sale\",\r\n        \"Deal\",\r\n        \"Limited Edition\",\r\n        \"100% orgasm\",\r\n        \"As Seen on TV\",\r\n        \"Best Seller\",\r\n        \"Top Rated\",\r\n      ];\r\n      this.products = this.shuffleArray(response.data.products || [])\r\n        .slice(0, 3)\r\n        .map((product) => ({\r\n          ...product,\r\n          randomTag:\r\n            Math.random() < 0.5\r\n              ? tags[Math.floor(Math.random() * tags.length)]\r\n              : null,\r\n          // 50% chance of getting a tag\r\n        }));\r\n    } catch (error) {\r\n      console.error(\"Error fetching products:\", error);\r\n    }\r\n  },\r\n  computed: {\r\n    totalPrice() {\r\n      return this.product.price * this.quantity;\r\n    },\r\n    productTitle() {\r\n      return this.$store.state.productTitle;\r\n    },\r\n  },\r\n  methods: {\r\n    async addProductToCart(product) {\r\n      try {\r\n        await this.$store.dispatch(\"addProductToCart\", product); // call Vuex action manually\r\n        this.$toast.open({\r\n          message: \"✅ Product added to cart!\",\r\n          type: \"success\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n          position: \"top\",\r\n        });\r\n      } catch (error) {\r\n        this.$toast.open({\r\n          message: \"✅ Product added to cart!\",\r\n          type: \"success\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n          position: \"top\",\r\n        });\r\n      }\r\n    },\r\n    slugify(text) {\r\n      return text\r\n        .toLowerCase()\r\n        .trim()\r\n        .replace(/\\s+/g, \"-\") // Replace spaces with hyphens\r\n        .replace(/[^\\w\\s'-]/g, \"\"); // Remove special characters, keep apostrophes\r\n    },\r\n    goToProduct(productTitle) {\r\n      // Normalize the product title for the URL: Replace spaces with hyphens\r\n      let normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\r\n\r\n      console.log(\"Normalized Product Title for URL:\", normalizedProductTitle);\r\n\r\n      // Store the raw title in Vuex (for the backend)\r\n      this.$store.dispatch(\"setProductUrl\", productTitle);\r\n\r\n      // Use the normalized title in the URL (for better SEO)\r\n      this.$router.push({\r\n        name: \"ProductDetails\",\r\n        params: { title: normalizedProductTitle }, // For URL with hyphens\r\n      });\r\n      window.location.reload();\r\n    },\r\n\r\n    buyNow(product) {\r\n      this.addProductToCart(product); // Add product to cart\r\n      this.$router.push(\"/cart\"); // Redirect to checkout page\r\n    },\r\n    shuffleArray(array) {\r\n      return array.sort(() => Math.random() - 0.5);\r\n    },\r\n    onChangeQuantity(event, product) {\r\n      let qty = parseInt(event.target.value);\r\n      console.log(\"Selected Quantity:\", qty);\r\n\r\n      if (!isNaN(qty)) {\r\n        this.quantity = qty;\r\n        this.$store.commit(\"changeQty\", { product, qty });\r\n      } else {\r\n        console.error(\"Invalid Quantity Selected\");\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    product: {\r\n      handler(newProduct) {\r\n        if (newProduct && newProduct.photos && newProduct.photos.length > 0) {\r\n          this.selectedPhoto = newProduct.photos[0];\r\n        }\r\n      },\r\n      deep: true,\r\n      immediate: true,\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAyLA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZL,OAAO,EAAE;QACPM,MAAM,EAAE,EAAE;QACVC,KAAK,EAAE,CAAC,CAAE;MACZ,CAAC;;MACDC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,CAAC,CAAE;IACf,CAAC;EACH,CAAC;;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF;MACA,IAAIC,YAAW,GAAI,IAAI,CAACC,MAAM,CAACC,OAAO,CAACC,aAAa;MACpDC,OAAO,CAACC,GAAG,CAAC,8BAA8B,EAAEL,YAAY,CAAC;;MAEzD;MACA,MAAMM,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAC7B,sCAAqCC,kBAAkB,CAACR,YAAY,CAAE,EACzE,CAAC;MAED,IAAI,CAACX,OAAM,GAAIiB,QAAQ,CAACb,IAAI,CAACJ,OAAM,IAAK,CAAC,CAAC;IAC5C,EAAE,OAAOoB,KAAK,EAAE;MACdL,OAAO,CAACK,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;IACzD;IAEA,IAAI;MACF,MAAMH,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAAE,oCAAmC,CAAC;MACtEH,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEC,QAAQ,CAACb,IAAI,CAAC;MAC3C,MAAMiB,IAAG,GAAI,CACX,MAAM,EACN,MAAM,EACN,iBAAiB,EACjB,aAAa,EACb,eAAe,EACf,aAAa,EACb,WAAW,CACZ;MACD,IAAI,CAAChB,QAAO,GAAI,IAAI,CAACiB,YAAY,CAACL,QAAQ,CAACb,IAAI,CAACC,QAAO,IAAK,EAAE,EAC3DkB,KAAK,CAAC,CAAC,EAAE,CAAC,EACVC,GAAG,CAAExB,OAAO,KAAM;QACjB,GAAGA,OAAO;QACVyB,S