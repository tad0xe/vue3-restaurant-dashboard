{"ast":null,"code":"import axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nimport QrcodeVue from \"qrcode.vue\";\nexport default {\n  name: \"PaymentSuccess\",\n  components: {\n    QrcodeVue\n  },\n  data() {\n    return {\n      reference: \"\",\n      verified: false,\n      loading: true\n    };\n  },\n  computed: {\n    ...mapGetters([\"getContactInfo\", \"getCart\", \"getSelectedTickets\", \"getCartTotal\"])\n  },\n  methods: {\n    async sendOrderInfo() {\n      try {\n        const reference = localStorage.getItem(\"paystack_reference\");\n        const payload = {\n          reference,\n          // Paystack reference\n          title: this.getCart[0]?.title,\n          // Get name from first cart item\n          contact: {\n            email: this.getContactInfo.email,\n            phone: this.getContactInfo.phone\n          },\n          userId: this.getCart[0]?.user,\n          // Get user from first cart item\n          tickets: this.getSelectedTickets,\n          // Only selected ones\n          price: this.getCartTotal // Total price from cart\n        };\n\n        console.log(\"Sending order info:\", payload);\n        const res = await axios.post(\"http://localhost:4000/api/order\", payload);\n        console.log(\"Order info sent:\", res.data);\n      } catch (err) {\n        console.error(\"Failed to send order info:\", err);\n      }\n    }\n  },\n  async mounted() {\n    this.reference = localStorage.getItem(\"paystack_reference\");\n    if (!this.reference) {\n      this.loading = false;\n      return;\n    }\n    try {\n      const res = await axios.get(`http://localhost:4000/api/verify/${this.reference}`);\n      if (res.data.data.status === \"success\") {\n        this.verified = true;\n        await this.sendOrderInfo(); // Send order info after successful verification\n      }\n    } catch (err) {\n      console.error(\"Verification error:\", err);\n    } finally {\n      this.loading = false;\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","QrcodeVue","name","components","data","reference","verified","loading","computed","methods","sendOrderInfo","localStorage","getItem","payload","title","getCart","contact","email","getContactInfo","phone","userId","user","tickets","getSelectedTickets","price","getCartTotal","console","log","res","post","err","error","mounted","get","status"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\PaymentSuccess.vue"],"sourcesContent":["<template>\r\n  <div class=\"container text-center mt-5\">\r\n    <h2>üéâ Payment Status</h2>\r\n\r\n    <div v-if=\"loading\">Verifying payment...</div>\r\n    <div v-else-if=\"verified\">\r\n      <p class=\"text-success\">‚úÖ Payment successful!</p>\r\n      <p><strong>Reference:</strong> {{ reference }}</p>\r\n      <qrcode-vue :value=\"reference\" :size=\"200\" />\r\n    </div>\r\n    <div v-else>\r\n      <p class=\"text-danger\">‚ùå Payment verification failed.</p>\r\n    </div>\r\n\r\n    <router-link to=\"/\" class=\"btn btn-primary mt-3\">Go Home</router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\nimport QrcodeVue from \"qrcode.vue\";\r\n\r\nexport default {\r\n  name: \"PaymentSuccess\",\r\n  components: { QrcodeVue },\r\n  data() {\r\n    return {\r\n      reference: \"\",\r\n      verified: false,\r\n      loading: true,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getContactInfo\", \"getCart\",\"getSelectedTickets\",\"getCartTotal\"]),\r\n  },\r\n  methods: {\r\n    async sendOrderInfo() {\r\n      try {\r\n        const reference = localStorage.getItem(\"paystack_reference\");\r\n\r\n        const payload = {\r\n          reference, // Paystack reference\r\n          title:this.getCart[0]?.title, // Get name from first cart item\r\n          contact: {\r\n            email: this.getContactInfo.email,\r\n            phone: this.getContactInfo.phone,\r\n          },\r\n          userId: this.getCart[0]?.user, // Get user from first cart item\r\n          tickets: this.getSelectedTickets, // Only selected ones\r\n          price: this.getCartTotal, // Total price from cart\r\n        };\r\n        console.log(\"Sending order info:\", payload);\r\n        const res = await axios.post(\r\n          \"http://localhost:4000/api/order\",\r\n          payload\r\n        );\r\n        console.log(\"Order info sent:\", res.data);\r\n      } catch (err) {\r\n        console.error(\"Failed to send order info:\", err);\r\n      }\r\n    },\r\n  },\r\n  async mounted() {\r\n    this.reference = localStorage.getItem(\"paystack_reference\");\r\n\r\n    if (!this.reference) {\r\n      this.loading = false;\r\n      return;\r\n    }\r\n\r\n    try {\r\n      const res = await axios.get(\r\n        `http://localhost:4000/api/verify/${this.reference}`\r\n      );\r\n      if (res.data.data.status === \"success\") {\r\n        this.verified = true;\r\n        await this.sendOrderInfo(); // Send order info after successful verification\r\n      }\r\n    } catch (err) {\r\n      console.error(\"Verification error:\", err);\r\n    } finally {\r\n      this.loading = false;\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 500px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"mappings":"AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AACjC,OAAOC,SAAQ,MAAO,YAAY;AAElC,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IAAEF;EAAU,CAAC;EACzBG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,KAAK;MACfC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGR,UAAU,CAAC,CAAC,gBAAgB,EAAE,SAAS,EAAC,oBAAoB,EAAC,cAAc,CAAC;EACjF,CAAC;EACDS,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAML,SAAQ,GAAIM,YAAY,CAACC,OAAO,CAAC,oBAAoB,CAAC;QAE5D,MAAMC,OAAM,GAAI;UACdR,SAAS;UAAE;UACXS,KAAK,EAAC,IAAI,CAACC,OAAO,CAAC,CAAC,CAAC,EAAED,KAAK;UAAE;UAC9BE,OAAO,EAAE;YACPC,KAAK,EAAE,IAAI,CAACC,cAAc,CAACD,KAA