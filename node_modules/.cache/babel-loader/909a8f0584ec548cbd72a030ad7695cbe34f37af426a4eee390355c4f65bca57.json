\n    <!-- Submit -->\r\n    <button type=\"submit\">Submit Product</button>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ProductForm\",\r\n\r\n  data() {\r\n    return {\r\n      product: {\r\n        title: \"\",\r\n        description: \"\",\r\n        tag: \"\",\r\n        price: {\r\n          normal: 0,\r\n          vip: 0,\r\n          vvip: 0,\r\n        },\r\n        event: {\r\n          date: \"\",\r\n          startTime: \"\",\r\n          endTime: \"\",\r\n          timezone: \"UTC\",\r\n          location: \"\",\r\n        },\r\n      },\r\n      photos: [],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n\r\n  methods: {\r\n    handleFiles(event) {\r\n      this.photos = Array.from(event.target.files);\r\n    },\r\n\r\n    async submitProduct() {\r\n      const formData = new FormData();\r\n\r\n      formData.append(\"title\", this.product.title);\r\n      formData.append(\"description\", this.product.description);\r\n      formData.append(\"tag\", this.product.tag);\r\n      formData.append(\"price\", JSON.stringify(this.product.price));\r\n\r\n      formData.append(\"eventDate\", this.product.event.date);\r\n      formData.append(\"startTime\", this.product.event.startTime);\r\n      formData.append(\"endTime\", this.product.event.endTime);\r\n      formData.append(\"timezone\", this.product.event.timezone);\r\n      formData.append(\"location\", this.product.event.location);\r\n\r\n      // Photos\r\n      this.photos.forEach((photo) => {\r\n        formData.append(\"photos\", photo);\r\n      });\r\n\r\n      try {\r\n        const response = await axios.post(\"http://localhost:5000/api/products\", formData, {\r\n          headers: {\r\n            Authorization: `Bearer ${this.getToken}`,\r\n            \"Content-Type\": \"multipart/form-data\",\r\n          },\r\n        });\r\n\r\n        console.log(\"Product created:\", response.data);\r\n        alert(\"Product created successfully!\");\r\n      } catch (error) {\r\n        console.error(\"Error creating product:\", error);\r\n        alert(\"Failed to create product\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAkCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EAEnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,GAAG,EAAE,EAAE;QACPC,KAAK,EAAE;UACLC,MAAM,EAAE,CAAC;UACTC,GAAG,EAAE,CAAC;UACNC,IAAI,EAAE;QACR,CAAC;QACDC,KAAK,EAAE;UACLC,IAAI,EAAE,EAAE;UACRC,SAAS,EAAE,EAAE;UACbC,OAAO,EAAE,EAAE;UACXC,QAAQ,EAAE,KAAK;UACfC,QAAQ,EAAE;QACZ;MACF,CAAC;MACDC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACR,GAAGlB,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EAEDmB,OAAO,EAAE;IACPC,WAAWA,CAACT,KAAK,EAAE;MACjB,IAAI,CAACM,MAAK,GAAII,KAAK,CAACC,IAAI,CAACX,KAAK,CAACY,MAAM,CAACC,KAAK,CAAC;IAC9C,CAAC;IAED,MAAMC,aAAaA,CAAA,EAAG;MACpB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAE/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACzB,OAAO,CAACC,KAAK,CAAC;MAC5CsB,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAACzB,OAAO,CAACE,WAAW,CAAC;MACxDqB,QAAQ,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAACzB,OAAO,CAACG,GAAG,CAAC;MACxCoB,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAAC3B,OAAO,CAACI,KAAK,CAAC,CAAC;MAE5DmB,QAAQ,CAACE,MAAM,CAAC,WAAW,EAAE,IAAI,CAACzB,OAAO,CAACQ,KAAK,CAACC,IAAI,CAAC;MACrDc,QAAQ,CAACE,MAAM,CAAC,WAAW,EAAE,IAAI,CAACzB,OAAO,CAACQ,KAAK,CAACE,SAAS,CAAC;MAC1Da,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACzB,OAAO,CAACQ,KAAK,CAACG,OAAO,CAAC;MACtDY,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACzB,OAAO,CAACQ,KAAK,CAACI,QAAQ,CAAC;MACxDW,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACzB,OAAO,CAACQ,KAAK,CAACK,QAAQ,CAAC;;MAExD;MACA,IAAI,CAACC,MAAM,CAACc,OAAO,CAAEC,KAAK,IAAK;QAC7BN,QAAQ,CAACE,MAAM,CAAC,QAAQ,EAAEI,KAAK,CAAC;MAClC,CAAC,CAAC;MAEF,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMlC,KAAK,CAACmC,IAAI,CAAC,oCAAoC,EAAER,QAAQ,EAAE;UAChFS,OAAO,EAAE;YACPC,aAAa,EAAG,UAAS,IAAI,CAACC,QAAS,E