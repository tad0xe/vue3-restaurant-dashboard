{"ast":null,"code":"export default {\n  data() {\n    return {\n      step: 1,\n      timer: \"10:00\",\n      countdown: null,\n      tickets: [{\n        id: 1,\n        name: \"EDE-LMD25 Regular\",\n        price: 3000,\n        quantity: 0\n      }, {\n        id: 2,\n        name: \"EDE-LMD25 VIP\",\n        price: 10000,\n        quantity: 1\n      }, {\n        id: 3,\n        name: \"EDE-LMD25 SOAT\",\n        price: 25000,\n        quantity: 0\n      }],\n      contact: {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        confirmEmail: \"\",\n        phone: \"\"\n      },\n      paymentMethod: \"\",\n      termsAccepted: false,\n      subscribe: false\n    };\n  },\n  computed: {\n    selectedTicket() {\n      return this.tickets.find(ticket => ticket.quantity > 0);\n    },\n    canCheckout() {\n      return this.paymentMethod && this.termsAccepted;\n    }\n  },\n  methods: {\n    nextStep() {\n      if (this.step === 2 && this.contact.email !== this.contact.confirmEmail) {\n        alert(\"Email addresses do not match.\");\n        return;\n      }\n      this.step++;\n    },\n    prevStep() {\n      this.step--;\n    },\n    checkout() {\n      alert(\"‚úÖ Checkout successful!\");\n    },\n    startTimer() {\n      let totalSeconds = 600;\n      this.countdown = setInterval(() => {\n        totalSeconds--;\n        const minutes = String(Math.floor(totalSeconds / 60)).padStart(2, \"0\");\n        const seconds = String(totalSeconds % 60).padStart(2, \"0\");\n        this.timer = `${minutes}:${seconds}`;\n        if (totalSeconds <= 0) {\n          clearInterval(this.countdown);\n          alert(\"‚è∞ Time expired! Please restart.\");\n          location.reload();\n        }\n      }, 1000);\n    }\n  },\n  mounted() {\n    this.startTimer();\n  }\n};","map":{"version":3,"names":["data","step","timer","countdown","tickets","id","name","price","quantity","contact","firstName","lastName","email","confirmEmail","phone","paymentM