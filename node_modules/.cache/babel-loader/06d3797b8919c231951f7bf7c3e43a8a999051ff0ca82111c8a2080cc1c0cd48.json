{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentStep: 0,\n      timer: \"10:00\",\n      countdown: null,\n      steps: [\"Tickets\", \"Contact\", \"Payment\"],\n      tickets: [{\n        id: 1,\n        name: \"EDE-LMD25 Regular\",\n        price: 3000,\n        quantity: 0\n      }, {\n        id: 2,\n        name: \"EDE-LMD25 VIP\",\n        price: 10000,\n        quantity: 1\n      }, {\n        id: 3,\n        name: \"EDE-LMD25 SOAT\",\n        price: 25000,\n        quantity: 0\n      }],\n      contact: {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        confirmEmail: \"\",\n        phone: \"\"\n      },\n      paymentMethod: \"\",\n      termsAccepted: false,\n      subscribe: false\n    };\n  },\n  computed: {\n    selectedTicket() {\n      return this.tickets.find(ticket => ticket.quantity > 0);\n    },\n    canCheckout() {\n      return this.paymentMethod && this.termsAccepted;\n    }\n  },\n  watch: {\n    currentStep() {\n      this.updateProgressLine();\n    }\n  },\n  methods: {\n    updateProgressLine() {\n      const wrapper = this.$el.querySelector(\".stepper-wrapper\");\n      if (wrapper) {\n        const value = this.currentStep / (this.steps.length - 1);\n        wrapper.style.setProperty(\"--progress\", value);\n      }\n    },\n    nextStep() {\n      if (this.currentStep < this.steps.length - 1) this.currentStep++;\n    },\n    prevStep() {\n      if (this.currentStep > 0) this.currentStep--;\n    },\n    checkout() {\n      alert(\"‚úÖ Checkout successful!\");\n    },\n    startTimer() {\n      let totalSeconds = 600;\n      this.countdown = setInterval(() => {\n        totalSeconds--;\n        const minutes = String(Math.floor(totalSeconds / 60)).padStart(2, \"0\");\n        const seconds = String(totalSeconds % 60).padStart(2, \"0\");\n        this.timer = `${minutes}:${seconds}`;\n        if (totalSeconds <= 0) {\n          clearInterval(this.countdown);\n          alert(\"‚è∞ Time expired! Please restart.\");\n          location.reload();\n        }\n      }, 1000);\n    }\n  },\n  mounted() {\n    this.startTimer();\n    this.updateProgressLine();\n  }\n};","map":{"version":3,"names":["data","currentStep","timer","countdown","steps","tickets","id","name","price","quantity","contact","firstName","lastName","email","confirmEmail","phone","paymentMethod","termsAccepted","subscribe","computed","selectedTicket","find","ticket","canCheckout","watch","updateProgressLine","methods","wrapper","$el","querySelector","value","length","style","setProperty","nextStep","prevStep","checkout","alert","startTimer","totalSeconds","setInterval","minutes","String","Math","floor","padStart","seconds","clearInterval","location","reload","mounted"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\vue3-restaurant-dashboard\\src\\components\\Payment.vue"],"sourcesContent":["<template>\r\n  <div class=\"container my-5\">\r\n    <div class=\"row\">\r\n      <!-- Step Progress Bar -->\r\n\r\n      <!-- Left column: steps -->\r\n\r\n      <div class=\"col-md-8\">\r\n        <!-- Progress Bar -->\r\n        <div class=\"stepper mb-5 position-relative\">\r\n          <!-- Progress Line -->\r\n          <div class=\"progress-line\">\r\n            <div\r\n              class=\"progress-fill\"\r\n              :style=\"{ width: (currentStep / (steps.length - 1)) * 100 + '%' }\"\r\n            ></div>\r\n          </div>\r\n\r\n          <!-- Steps -->\r\n          <div\r\n            v-for=\"(stepLabel, index) in steps\"\r\n            :key=\"index\"\r\n            class=\"currentStep text-center\"\r\n            :class=\"{\r\n              active: currentStep === index,\r\n              completed: index < currentStep,\r\n            }\"\r\n          >\r\n            <div class=\"circle\">\r\n              <span v-if=\"index < currentStep\">‚úî</span>\r\n              <span v-else>{{ index + 1 }}</span>\r\n            </div>\r\n            <div class=\"label\">{{ stepLabel }}</div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Step 1: Ticket Selection -->\r\n        <div v-if=\"currentStep === 0\">\r\n          <h3>üéüÔ∏è Choose Tickets</h3>\r\n          <div class=\"card mb-3\" v-for=\"ticket in tickets\" :key=\"ticket.id\">\r\n            <div\r\n              class=\"card-body d-flex justify-content-between align-items-center\"\r\n            >\r\n              <div>\r\n                <h5>{{ ticket.name }}</h5>\r\n                <p class=\"mb-1 text-danger fw-bold\">\r\n                  ‚Ç¶{{ ticket.price.toLocaleString() }}\r\n                </p>\r\n                <p class=\"text-muted small\">Full Access To Event</p>\r\n              </div>\r\n              <select\r\n                v-model.number=\"ticket.quantity\"\r\n                class=\"form-control w-auto\"\r\n              >\r\n                <option v-for=\"n in 5\" :key=\"n\" :value=\"n - 1\">\r\n                  {{ n - 1 }}\r\n                </option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Step 2: Contact Info -->\r\n        <div v-else-if=\"currentStep === 1\" class=\"text-start\">\r\n          <h3>üìá Contact Information</h3>\r\n          <p class=\"alert alert-warning\">\r\n            We've reserved your ticket. Please complete checkout within\r\n            {{ timer }} to secure your tickets.\r\n          </p>\r\n          <form @submit.prevent=\"nextStep\">\r\n            <div class=\"row mb-3\">\r\n              <div class=\"col\">\r\n                <label class=\"form-label\">First Name *</label>\r\n                <input\r\n                  v-model=\"contact.firstName\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <label class=\"form-label\">Last Name *</label>\r\n                <input\r\n                  v-model=\"contact.lastName\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">Email Address *</label>\r\n              <input\r\n                v-model=\"contact.email\"\r\n                type=\"email\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">Confirm Email Address *</label>\r\n              <input\r\n                v-model=\"contact.confirmEmail\"\r\n                type=\"email\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">Phone Number *</label>\r\n              <div class=\"input-group\">\r\n                <span class=\"input-group-text\">+234</span>\r\n                <input\r\n                  v-model=\"contact.phone\"\r\n                  type=\"tel\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n          </form>\r\n        </div>\r\n\r\n        <!-- Step 3: Payment -->\r\n        <div v-else-if=\"currentStep === 2\" class=\"text-start\">\r\n          <h3>üí≥ Payment Options</h3>\r\n          <p class=\"alert alert-warning\">\r\n            We've reserved your ticket. Please complete checkout within\r\n            {{ timer }} to secure your tickets.\r\n          </p>\r\n\r\n          <div class=\"form-check mb-2\">\r\n            <input\r\n              class=\"form-check-input\"\r\n              type=\"radio\"\r\n              v-model=\"paymentMethod\"\r\n              value=\"card\"\r\n              id=\"payCard\"\r\n            />\r\n            <label class=\"form-check-label\" for=\"payCard\"\r\n              >Pay with Card or Bank</label\r\n            >\r\n          </div>\r\n          <div class=\"form-check mb-4\">\r\n            <input\r\n              class=\"form-check-input\"\r\n              type=\"radio\"\r\n              v-model=\"paymentMethod\"\r\n              value=\"kuda\"\r\n              id=\"payKuda\"\r\n            />\r\n            <label class=\"form-check-label\" for=\"payKuda\">Pay with Kuda</label>\r\n          </div>\r\n\r\n          <div class=\"form-check mb-2\">\r\n            <input\r\n              class=\"form-check-input\"\r\n              type=\"checkbox\"\r\n              v-model=\"termsAccepted\"\r\n              id=\"terms\"\r\n            />\r\n            <label class=\"form-check-label\" for=\"terms\">\r\n              I accept the <a href=\"#\">Tix Terms and Conditions</a>,\r\n              <a href=\"#\">Refund Policy</a> and <a href=\"#\">Privacy Policy</a>.\r\n            </label>\r\n          </div>\r\n          <div class=\"form-check mb-4\">\r\n            <input\r\n              class=\"form-check-input\"\r\n              type=\"checkbox\"\r\n              v-model=\"subscribe\"\r\n              id=\"subscribe\"\r\n            />\r\n            <label class=\"form-check-label\" for=\"subscribe\">\r\n              Send me emails about the best events happening nearby or online.\r\n            </label>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center mt-4 d-flex justify-content-between\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn btn-secondary\"\r\n            @click=\"prevStep\"\r\n            :disabled=\"currentStep === 0\"\r\n          >\r\n            Back\r\n          </button>\r\n          <button\r\n            v-if=\"currentStep < steps.length - 1\"\r\n            type=\"button\"\r\n            class=\"btn btn-primary btn-primary:hover\"\r\n            @click=\"nextStep\"\r\n          >\r\n            Next\r\n          </button>\r\n          <button\r\n            v-else\r\n            type=\"submit\"\r\n            class=\"btn btn-primary btn-primary:hover\"\r\n          >\r\n            Create Event\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Right column: Summary -->\r\n      <div class=\"col-md-4 text-start\">\r\n        <div class=\"p-4 bg-light border-start\">\r\n          <h5>Summary</h5>\r\n          <p class=\"fw-bold\">\r\n            Extraordinary Dance Experience: Last Man Dancing 2025\r\n          </p>\r\n          <div v-if=\"selectedTicket\">\r\n            <p>\r\n              {{ selectedTicket.quantity }} √ó {{ selectedTicket.name }}<br />‚Ç¶{{\r\n                selectedTicket.price * selectedTicket.quantity\r\n              }}\r\n            </p>\r\n            <p class=\"text-muted small\">Fees: ‚Ç¶0</p>\r\n            <p class=\"fw-bold\">\r\n              Total: ‚Ç¶{{ selectedTicket.price * selectedTicket.quantity }}\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentStep: 0,\r\n      timer: \"10:00\",\r\n      countdown: null,\r\n\r\n      steps: [\"Tickets\", \"Contact\", \"Payment\"],\r\n      tickets: [\r\n        { id: 1, name: \"EDE-LMD25 Regular\", price: 3000, quantity: 0 },\r\n        { id: 2, name: \"EDE-LMD25 VIP\", price: 10000, quantity: 1 },\r\n        { id: 3, name: \"EDE-LMD25 SOAT\", price: 25000, quantity: 0 },\r\n      ],\r\n      contact: {\r\n        firstName: \"\",\r\n        lastName: \"\",\r\n        email: \"\",\r\n        confirmEmail: \"\",\r\n        phone: \"\",\r\n      },\r\n      paymentMethod: \"\",\r\n      termsAccepted: false,\r\n      subscribe: false,\r\n    };\r\n  },\r\n  computed: {\r\n    selectedTicket() {\r\n      return this.tickets.find((ticket) => ticket.quantity > 0);\r\n    },\r\n    canCheckout() {\r\n      return this.paymentMethod && this.termsAccepted;\r\n    },\r\n  },\r\n  watch: {\r\n    currentStep() {\r\n      this.updateProgressLine();\r\n    },\r\n  },\r\n  methods: {\r\n    updateProgressLine() {\r\n      const wrapper = this.$el.querySelector(\".stepper-wrapper\");\r\n      if (wrapper) {\r\n        const value = this.currentStep / (this.steps.length - 1);\r\n        wrapper.style.setProperty(\"--progress\", value);\r\n      }\r\n    },\r\n    nextStep() {\r\n      if (this.currentStep < this.steps.length - 1) this.currentStep++;\r\n    },\r\n    prevStep() {\r\n      if (this.currentStep > 0) this.currentStep--;\r\n    },\r\n\r\n    checkout() {\r\n      alert(\"‚úÖ Checkout successful!\");\r\n    },\r\n    startTimer() {\r\n      let totalSeconds = 600;\r\n      this.countdown = setInterval(() => {\r\n        totalSeconds--;\r\n        const minutes = String(Math.floor(totalSeconds / 60)).padStart(2, \"0\");\r\n        const seconds = String(totalSeconds % 60).padStart(2, \"0\");\r\n        this.timer = `${minutes}:${seconds}`;\r\n        if (totalSeconds <= 0) {\r\n          clearInterval(this.countdown);\r\n          alert(\"‚è∞ Time expired! Please restart.\");\r\n          location.reload();\r\n        }\r\n      }, 1000);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.startTimer();\r\n    this.updateProgressLine();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.stepper-wrapper {\r\n  position: relative;\r\n  width: 100%;\r\n  padding: 0 15px;\r\n}\r\n\r\n.stepper {\r\n  position: relative;\r\n  align-items: center;\r\n  display: flex;\r\n}\r\n\r\n.currentStep {\r\n  position: relative;\r\n  z-index: 2;\r\n  flex: 1;\r\n}\r\n\r\n.circle {\r\n  width: 30px;\r\n  height: 30px;\r\n  border-radius: 50%;\r\n  border: 2px solid #adb5bd;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin: 0 auto 8px;\r\n  background-color: white;\r\n  color: #adb5bd;\r\n  font-weight: bold;\r\n}\r\n\r\n.currentStep.active .circle {\r\n  border-color: #429371;\r\n  background-color: #429371;\r\n  color: white;\r\n  transition: background-color 0.3s, border-color 0.3s;\r\n}\r\n\r\n.currentStep.completed .circle {\r\n  background-color: #429371;\r\n  color: white;\r\n  border-color: #429371;\r\n}\r\n\r\n.label {\r\n  font-size: 14px;\r\n  color: #6c757d;\r\n}\r\n\r\n.currentStep.active .label,\r\n.currentStep.completed .label {\r\n  color: #429371;\r\n}\r\n\r\n/* Progress line behind steps */\r\n.progress-line {\r\n  position: absolute;\r\n  top: 15px;\r\n  left: 0;\r\n  right: 0;\r\n  height: 2px;\r\n  background-color: #dee2e6;\r\n  z-index: 1;\r\n}\r\n\r\n.progress-fill {\r\n  height: 100%;\r\n  background-color: #429371;\r\n  width: 0%;\r\n  transition: width 0.4s ease-in-out;\r\n}\r\n</style>\r\n"],"mappings":"AAsOA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,KAAK,EAAE,OAAO;MACdC,SAAS,EAAE,IAAI;MAEfC,KAAK,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;MACxCC,OAAO,EAAE,CACP;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,mBAAmB;QAAEC,KAAK,EAAE,IAAI;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAC9D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,eAAe;QAAEC,KAAK,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAC3D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,KAAK,EAAE,KAAK;QAAEC,QAAQ,EAAE;MAAE,CAAC,CAC7D;MACDC,OAAO,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE;MACT,CAAC;MACDC,aAAa,EAAE,EAAE;MACjBC,aAAa,EAAE,KAAK;MACpBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACf,OAAO,CAACgB,IAAI,CAAEC,MAAM,IAAKA,MAAM,CAACb,QAAO,GAAI,CAAC,CAAC;IAC3D,CAAC;IACDc,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACP,aAAY,IAAK,IAAI,CAACC,aAAa;IACjD;EACF,CAAC;EACDO,KAAK,EAAE;IACLvB,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACwB,kBAAkB,CAAC,CAAC;IAC3B;EACF,CAAC;EACDC,OAAO,EAAE;IACPD,kBAAkBA,CAAA,EAAG;MACnB,MAAME,OAAM,GAAI,IAAI,CAACC,GAAG,CAACC,aAAa,CAAC,kBAAkB,CAAC;MAC1D,IAAIF,OAAO,EAAE;QACX,MAAMG,KAAI,GAAI,IAAI,CAAC7B,WAAU,IAAK,IAAI,CAACG,KAAK,CAAC2B,MAAK,GAAI