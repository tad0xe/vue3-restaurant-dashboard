{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"Orders\",\n  data() {\n    return {\n      orders: [],\n      selectedStatus: \"\",\n      searchQuery: \"\",\n      loading: true,\n      error: null\n    };\n  },\n  mounted() {\n    this.fetchOrders();\n  },\n  computed: {\n    filteredOrders() {\n      return this.orders.filter(order => {\n        // Filter by status if selected\n        const statusMatch = !this.selectedStatus || order.status === this.selectedStatus;\n\n        // Filter by search query in reference or email (case insensitive)\n        const query = this.searchQuery.trim().toLowerCase();\n        const referenceMatch = order.reference?.toLowerCase().includes(query);\n        const emailMatch = order.contact?.email?.toLowerCase().includes(query);\n        const searchMatch = !query || referenceMatch || emailMatch;\n        return statusMatch && searchMatch;\n      });\n    }\n  },\n  methods: {\n    async fetchOrders() {\n      try {\n        const response = await axios.get(\"http://localhost:4000/api/all-orders\");\n        this.orders = response.data.orders;\n      } catch (err) {\n        this.error = \"Failed to fetch orders\";\n        console.error(err);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async deleteOrder(orderId) {\n      if (!confirm(\"Are you sure you want to delete this order?\")) return;\n      try {\n        await axios.delete(`http://localhost:4000/api/order/${orderId}`);\n        alert(\"Order deleted successfully\");\n        this.fetchOrders(); // Refresh the list\n      } catch (error) {\n        console.error(\"Delete failed:\", error);\n        alert(\"Failed to delete order\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","orders","selectedStatus","searchQuery","loading","error","mounted","fetchOrders","computed","filteredOrders","filter","order","statusMatch","status","query","trim","toLowerCase","referenceMatch","reference","includes","emailMatch","contact","email","searchMatch","methods","response","get","err","console","deleteOrder","orderId","confirm","delete","alert"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\rex.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"filter row align-items-center\">\r\n      <div class=\"col-auto\">\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          v-model=\"searchQuery\"\r\n          placeholder=\"Search by Email or Reference\"\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"table-container\">\r\n      <table class=\"custom-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Order ID</th>\r\n            <th>Reference</th>\r\n            <th>Date</th>\r\n            <th>Name of event</th>\r\n            <th>Customer Email</th>\r\n            <th>Phone number</th>\r\n            <th>Amount</th>\r\n            <th>Ticket type</th>\r\n            <th>Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"order in filteredOrders\" :key=\"order._id\">\r\n            <td>{{ order._id }}</td>\r\n            <td>{{ order.reference }}</td>\r\n            <td>{{ new Date(order.createdAt).toLocaleString() }}</td>\r\n            <td>{{ order.title }}</td>\r\n            <td>{{ order.contact?.email || \"N/A\" }}</td>\r\n            <td>{{ order.contact?.phone || \"N/A\" }}</td>\r\n            <td>${{ order.price }}</td>\r\n            <td>\r\n              <div v-for=\"ticket in order.tickets\" :key=\"ticket._id\">\r\n                {{ ticket.name }} x {{ ticket.quantity }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <button @click=\"deleteOrder(order._id)\">Delete</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Orders\",\r\n  data() {\r\n    return {\r\n      orders: [],\r\n      selectedStatus: \"\",\r\n      searchQuery: \"\",\r\n      loading: true,\r\n      error: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchOrders();\r\n  },\r\n  computed: {\r\n    filteredOrders() {\r\n      return this.orders.filter((order) => {\r\n        // Filter by status if selected\r\n        const statusMatch =\r\n          !this.selectedStatus || order.status === this.selectedStatus;\r\n\r\n        // Filter by search query in reference or email (case insensitive)\r\n        const query = this.searchQuery.trim().toLowerCase();\r\n        const referenceMatch = order.reference?.toLowerCase().includes(query);\r\n        const emailMatch = order.contact?.email?.toLowerCase().includes(query);\r\n\r\n        const searchMatch = !query || referenceMatch || emailMatch;\r\n\r\n        return statusMatch && searchMatch;\r\n      });\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchOrders() {\r\n      try {\r\n        const response = await axios.get(\r\n          \"http://localhost:4000/api/all-orders\"\r\n        );\r\n        this.orders = response.data.orders;\r\n      } catch (err) {\r\n        this.error = \"Failed to fetch orders\";\r\n        console.error(err);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    async deleteOrder(orderId) {\r\n      if (!confirm(\"Are you sure you want to delete this order?\")) return;\r\n\r\n      try {\r\n        await axios.delete(`http://localhost:4000/api/order/${orderId}`);\r\n        alert(\"Order deleted successfully\");\r\n        this.fetchOrders(); // Refresh the list\r\n      } catch (error) {\r\n        console.error(\"Delete failed:\", error);\r\n        alert(\"Failed to delete order\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.custom-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.custom-table th,\r\n.custom-table td {\r\n  padding: 10px;\r\n  text-align: left;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.custom-table th {\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n.custom-table td button {\r\n  padding: 5px 10px;\r\n  font-size: 14px;\r\n  border-radius: 4px;\r\n}\r\n\r\n.custom-table td .dropdown-menu {\r\n  margin-top: 5px;\r\n}\r\n\r\n.custom-table td .dropdown-menu button {\r\n  width: 100%;\r\n  text-align: left;\r\n}\r\n.pagination {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 10px;\r\n}\r\n\r\n.pagination button {\r\n  border: 1px solid #ccc;\r\n  background-color: #fff;\r\n  color: #000;\r\n  padding: 5px 10px;\r\n  margin: 0 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.pagination button.active {\r\n  background-color: #f4a213;\r\n  color: #fff;\r\n}\r\n\r\n.filter {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.filter label {\r\n  margin-right: 5px;\r\n}\r\n.table-container {\r\n  overflow-x: auto;\r\n}\r\n\r\n.scrollable-tbody {\r\n  display: block;\r\n  white-space: nowrap;\r\n}\r\n</style>\r\n"],"mappings":"AAqDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACR,MAAM,CAACS,MAAM,CAAEC,KAAK,IAAK;QACnC;QACA,MAAMC,WAAU,GACd,CAAC,IAAI,CAACV,cAAa,IAAKS,KAAK,CAACE,MAAK,KAAM,IAAI,CAACX,cAAc;;QAE9D;QACA,MAAMY,KAAI,GAAI,IAAI,CAACX,WAAW,CAACY,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;QACnD,MAAMC,cAAa,GAAIN,KAAK,CAACO,SAAS,EAAEF,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACL,KAAK,CAAC;QACrE,MAAMM,UAAS,GAAIT,KAAK,CAACU,OAAO,EAAEC,KAAK,EAAEN,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACL,KAAK,CAAC;QAEtE,MAAMS,WAAU,GAAI,CAACT,KAAI,IAAKG,cAAa,IAAKG,UAAU;QAE1D,OAAOR,WAAU,IAAKW,WAAW;MACnC,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMjB,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAMkB,QAAO,GAAI,MAAM3B,KAAK,C