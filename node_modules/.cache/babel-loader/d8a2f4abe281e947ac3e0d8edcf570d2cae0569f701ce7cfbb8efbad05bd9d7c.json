ull,\r\n      orderData: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async onDecode(ref) {\r\n      this.reference = ref;\r\n      try {\r\n        const res = await axios.get(`http://localhost:4000/api/order/${ref}`);\r\n        this.orderData = res.data.order;\r\n      } catch (err) {\r\n        alert(\"Order not found.\");\r\n        console.error(err);\r\n      }\r\n    },\r\n    onInit(promise) {\r\n      promise\r\n        .then(() => console.log(\"Camera initialized\"))\r\n        .catch((e) => console.error(\"Camera error:\", e));\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAqBA,SAASA,YAAW,QAAS,mBAAmB;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IAAEF;EAAa,CAAC;EAC5BG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAACC,GAAG,EAAE;MAClB,IAAI,CAACJ,SAAQ,GAAII,GAAG;MACpB,IAAI;QACF,MAAMC,GAAE,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAE,mCAAkCF,GAAI,EAAC,CAAC;QACrE,IAAI,CAACH,SAAQ,GAAII,GAAG,CAACN,IAAI,CAACQ,KAAK;MACjC,EAAE,OAAOC,GAAG,EAAE;QACZC,KAAK,CAAC,kBAAkB,CAAC;QACzBC,OAAO,CAACC,KAAK,CAACH,GAAG,CAAC;MACpB;IACF,CAAC;IACDI,MAAMA,CAACC,OAAO,EAAE;MACdA,OAAM,CACHC,IAAI,CAAC,MAAMJ,OAAO,CAACK,GAAG,CAAC,oBAAoB,CAAC,EAC5CC,KAAK,CAAEC,CAAC,IAAKP,OAAO,CAACC,KAAK,CAAC,eAAe,EAAEM,CAAC,CAAC,CAAC;IACpD;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}