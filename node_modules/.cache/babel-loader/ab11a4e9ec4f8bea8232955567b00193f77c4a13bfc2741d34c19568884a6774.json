) {\n      if (this.currentStep > 0) this.currentStep--;\n    },\n    mounted() {\n      try {\n        const fileInput = this.$refs.fileInput;\n        if (fileInput?.files?.length) {\n          for (let i = 0; i < fileInput.files.length; i++) {\n            formData.append(\"photos\", fileInput.files[i]);\n          }\n        }\n      } catch (err) {\n        console.warn(\"File input error:\", err);\n      }\n    },\n    async handleSubmit() {\n      const fileInput = this.$refs.fileInput;\n      const formData = new FormData();\n      formData.append(\"title\", this.title);\n      formData.append(\"description\", this.description);\n      formData.append(\"price\", this.price);\n      formData.append(\"location\", this.location);\n      formData.append(\"isVirtual\", this.isVirtual);\n      if (fileInput && fileInput.files.length > 0) {\n        for (let i = 0; i < fileInput.files.length; i++) {\n          formData.append(\"photos\", fileInput.files[i]);\n        }\n      }\n      try {\n        await this.createProduct(formData);\n        Swal.fire(\"Success\", \"Product created!\", \"success\");\n      } catch (error) {\n        console.error(\"Product creation failed:\", error);\n        Swal.fire(\"Error\", \"Failed to create product\", \"error\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["QuillEditor","mapActions","Swal","components","data","steps","currentStep","title","previewUrl","price","description","location","isVirtual","eventType","timezone","startDate","endDate","startTime","endTime","startAmPm","endAmPm","eventFrequency","tickets","name","quantity","methods","addTicket","type","defaultPrice","push","removeTicket","index","splice","duplicateTicket","ticket","handleImageUpload","event","file","target","files","reader","FileReader","onload","e","result","readAsDataURL","nextStep","length","prevStep","mounted","fileInput","$refs","i","formData","append","err","console","warn","handleSubmit","FormData","createProduct","fire","error"],"sou