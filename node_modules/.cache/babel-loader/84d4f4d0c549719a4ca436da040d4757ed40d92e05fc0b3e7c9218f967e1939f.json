Instance\r\n        .delete(`/addresses/${id}`, {\r\n\r\n        })\r\n        .then(response => {\r\n        //  console.log(response);\r\n          this.message = response.message;\r\n          this.addresses.splice(index, 1);\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    onSetDefault(id) {\r\n\r\n\r\n      axiosInstance\r\n        .put(\r\n          `/addresses/set/default`,\r\n          { id: id },\r\n          //token:token,\r\n          {\r\n\r\n          }\r\n        )\r\n        .then(res => {\r\n          this.message = res.data.message;\r\n\r\n         // console.log(id);\r\n         // console.log(res);\r\n        })\r\n        .catch(error => {\r\n          this.message = error.message;\r\n          console.log(error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";AAqCA,SAASA,UAAS,QAAS,MAAM;AACjC,OAAOC,aAAY,MAAO,6BAA6B;AAIvD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG,CAEV,CAAC;EACDC,OAAOA,CAAA,EAAG;IAERR,aAAY,CACTS,GAAG,CAAC,YAAY,EAAE,CAEnB,CAAC,EACAC,IAAI,CAACC,GAAE,IAAK;MACb;MACE,IAAI,CAACV,IAAG,GAAIU,GAAG,CAACT,IAAI,CAACU,IAAI,CAACX,IAAI;MAC9B,IAAI,CAACY,KAAI,GAAIF,GAAG,CAACT,IAAI,CAACU,IAAI,CAACC,KAAK;IAClC,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;MACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;IACpB,CAAC,CAAC;IACJf,aAAY,CACTS,GAAG,CAAC,YAAY,EAAE,CAEnB,CAAC,EACAC,IAAI,CAACC,GAAE,IAAK;MACZ;;MAEC,IAAI,CAACR,SAAQ,GAAIQ,GAAG,CAACT,IAAI,CAACC,SAAS;IACrC,CAAC,CAAC;EACN,CAAC;EAEDe,OAAO,EAAE;IACP,GAAGnB,UAAU,CAAC,CAAC,kBAAkB,CAAC,CAAC;IACnCoB,MAAMA,CAAA,EAAG;MACPC,YAAY,CAACC,KAAK,CAAC,CAAC;MACpB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IAEDC,eAAeA,CAACC,EAAE,EAAEC,KAAK,EAAE;MAEzB1B,aAAY,CACT2B,MAAM,CAAE,cAAaF,EAAG,EAAC,EAAE,CAE5B,CAAC,EACAf,IAAI,CAACkB,QAAO,IAAK;QAClB;QACE,IAAI,CAACxB,OAAM,GAAIwB,QAAQ,CAACxB,OAAO;QAC/B,IAAI,CAACD,SAAS,CAAC0B,M