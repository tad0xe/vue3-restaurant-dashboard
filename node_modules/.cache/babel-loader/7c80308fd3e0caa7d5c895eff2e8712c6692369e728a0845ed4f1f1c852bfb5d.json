ch(\"fetchOrders\");\n    this.$store.dispatch(\"fetchProductsByUser\");\n    this.$store.dispatch(\"fetchOrdersByUser\");\n    this.$store.dispatch(\"fetchProducts\");\n    this.$store.dispatch(\"fetchUsers\");\n    this.$store.dispatch(\"fetchRevenues\").then(response => {\n      this.revenues = response.data.revenues;\n      const options = {\n        series: [{\n          name: \"Income\",\n          data: []\n        }, {\n          name: \"Expenses\",\n          data: []\n        }],\n        chart: {\n          type: \"area\"\n        },\n        xaxis: {\n          categories: []\n        }\n      };\n      this.computedRevenues.forEach(revenue => {\n        options.series[0].data.push(revenue.income);\n        options.series[1].data.push(revenue.expenses);\n        options.xaxis.categories.push(new Date(revenue.time));\n      });\n      var chart = new ApexCharts(document.querySelector(\"#chart\"), options);\n      chart.render();\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"names":["barchart","defineComponent","Vue3autocounter","piechart","linechart","mapGetters","axios","components","data","revenues","computed","orders","getOrders","ordersByUser","getOrdersByUser","products","getProducts","productsByUser","getProductsByUser","users","getUsers","computedRevenues","getRevenues","methods","fetchOrders","response","get","headers","Authorization","getToken","success","console","log","error","message","mounted","$store","dispatch","then","options","series","name","chart","type","xaxis","categories","forEach","revenue","push","income","expenses","Date","time","ApexCharts","document","querySelector","render","catch"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\dashboard\\dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"container py-4\">\r\n    <h2 class=\"mb-4 text-center\">Dashboard Overview</h2>\r\n\r\n    <!-- Metrics Row -->\r\n    <div class=\"row g-3\">\r\n      <ul>\r\n        <li v-for=\"product in getProductsByUser\" :key=\"product._id\">\r\n          <h4>{{ product.title }}</h4>\r\n          <p>Price: â‚¦{{ product.price }}</p>\r\n          <p>Category: {{ product.category }}</p>\r\n        </li>\r\n      </ul>\r\n\r\n      <div class=\"col-md-3\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          {{ getProductsByUser.length }}\r\n\r\n          <p class=\"mb-1 fw-bold\">Total Events</p>\r\n          <small>{{ totalPercentage }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-md-3\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          <vue3-autocounter\r\n            :startAmount=\"0\"\r\n            :endAmount=\"totalOrdersPrice\"\r\n            :duration=\"3\"\r\n            separator=\",\"\r\n            decimalSeparator=\".\"\r\n            :autoinit=\"true\"\r\n          />\r\n          <p class=\"mb-1 fw-bold\">Total Revenue</p>\r\n          <small>{{ totalOrdersPercentage }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-md-3\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          <vue3-autocounter\r\n            :startAmount=\"0\"\r\n            :endAmount=\"orders.length\"\r\n            :duration=\"3\"\r\n            separator=\",\"\r\n            decimalSeparator=\".\"\r\n            :autoinit=\"true\"\r\n          />\r\n          <p class=\"mb-1 fw-bold\">Total Orders</p>\r\n          <small>{{ totalPercent }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-md-3\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          <vue3-autocounter\r\n            :startAmount=\"0\"\r\n            :endAmount=\"users.length\"\r\n            :duration=\"3\"\r\n            separator=\",\"\r\n            decimalSeparator=\".\"\r\n            :autoinit=\"true\"\r\n          />\r\n          <p class=\"mb-1 fw-bold\">Total Clients</p>\r\n          <small>{{ totalUserPercent }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Charts Row -->\r\n    <div class=\"row g-3 mt-4\">\r\n      <div class=\"col-md-6\">\r\n        <div class=\"border rounded p-3\">\r\n          <piechart />\r\n        </div>\r\n      </div>\r\n      <div class=\"col-md-6\">\r\n        <div class=\"border rounded p-3\">\r\n          <linechart />\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 mt-3\">\r\n        <div class=\"border rounded p-3\">\r\n          <barchart />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport barchart from \"../charts/barChart.vue\";\r\nimport { defineComponent } from \"vue\";\r\nimport Vue3autocounter from \"vue3-autocounter\";\r\n\r\nimport piechart from \"../charts/pieChart.vue\";\r\nimport linechart from \"../charts/lineChart.vue\";\r\n\r\nimport { mapGetters } from \"vuex\";\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: {\r\n    barchart,\r\n    piechart,\r\n    linechart,\r\n    \"vue3-autocounter\": Vue3autocounter,\r\n  },\r\n  data() {\r\n    return {\r\n      revenues: \"\",\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\r\n      \"getToken\",\r\n      \"getProductsByUser\",\r\n      \"getOrdersByUser\",\r\n      \"getOrders\",\r\n      \"getProducts\",\r\n      \"deliveredCount\",\r\n      \"cancelledCount\",\r\n      \"onDeliveryCount\",\r\n      \"allProducts\",\r\n      \"allOrders\",\r\n      \"getUsers\",\r\n      \"allOrdersLength\",\r\n      \"totalUsers\",\r\n      \"totalUserPercent\",\r\n      \"totalPercentage\",\r\n      \"totalPercent\",\r\n      \"totalOrdersPercentage\",\r\n      \"totalOrdersPrice\",\r\n    ]),\r\n    orders() {\r\n      return this.getOrders;\r\n    },\r\n    ordersByUser() {\r\n      return this.getOrdersByUser;\r\n    },\r\n    products() {\r\n      return this.getProducts;\r\n    },\r\n    productsByUser() {\r\n      return this.getProductsByUser;\r\n    },\r\n    users() {\r\n      return this.getUsers;\r\n    },\r\n    computedRevenues() {\r\n      return this.getRevenues;\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchOrders() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:4000/api/orders\", {\r\n          headers: {\r\n            Authorization: `Bearer ${this.getToken}`,\r\n          },\r\n        });\r\nif (response.data.success) {\r\n          this.orders = response.data.products;\r\n          console.log(\"Orders fetched successfully:\", this.orders);\r\n        } else {\r\n          console.error(\"Failed to fetch orders:\", response.data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Axios error fetching orders:\", error);\r\n      }\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.fetchOrders();\r\n    this.$store.dispatch(\"fetchOrders\");\r\n    this.$store.dispatch(\"fetchProductsByUser\");\r\n    this.$store.dispatch(\"fetchOrdersByUser\");\r\n    this.$store.dispatch(\"fetchProducts\");\r\n    this.$store.dispatch(\"fetchUsers\");\r\n    this.$store\r\n      .dispatch(\"fetchRevenues\")\r\n      .then((response) => {\r\n        this.revenues = response.data.revenues;\r\n\r\n        const options = {\r\n          series: [\r\n            {\r\n              name: \"Income\",\r\n              data: [],\r\n            },\r\n            {\r\n              name: \"Expenses\",\r\n              data: [],\r\n            },\r\n          ],\r\n          chart: {\r\n            type: \"area\",\r\n          },\r\n          xaxis: {\r\n            categories: [],\r\n          },\r\n        };\r\n\r\n        this.computedRevenues.forEach((revenue) => {\r\n          options.series[0].data.push(revenue.income);\r\n          options.series[1].data.push(revenue.expenses);\r\n          options.xaxis.categories.push(new Date(revenue.time));\r\n        });\r\n\r\n        var chart = new ApexCharts(document.querySelector(\"#chart\"), options);\r\n        chart.render();\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.typewriter {\r\n  display: inline-block;\r\n  overflow: hidden;\r\n  animation: typing 2s steps(40, end), blink-caret 0.75s step-end infinite;\r\n}\r\n\r\n@keyframes typing {\r\n  from {\r\n    width: 0;\r\n  }\r\n  to {\r\n    width: 100%;\r\n  }\r\n}\r\n\r\n@keyframes blink-caret {\r\n  from,\r\n  to {\r\n    border-color: transparent;\r\n  }\r\n  50% {\r\n    border-color: #000;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA2FA,OAAOA,QAAO,MAAO,wBAAwB;AAC7C,SAASC,eAAc,QAAS,KAAK;AACrC,OAAOC,eAAc,MAAO,kBAAkB;AAE9C,OAAOC,QAAO,MAAO,wBAAwB;AAC7C,OAAOC,SAAQ,MAAO,yBAAyB;AAE/C,SAASC,UAAS,QAAS,MAAM;AAEjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVP,QAAQ;IACRG,QAAQ;IACRC,SAAS;IACT,kBAAkB,EAAEF;EACtB,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACR,GAAGL,UAAU,CAAC,CACZ,UAAU,EACV,mBAAmB,EACnB,iBAAiB,EACjB,WAAW,EACX,aAAa,EACb,gBAAgB,EAChB,gBAAgB,EAChB,iBAAiB,EACjB,aAAa,EACb,WAAW,EACX,UAAU,EACV,iBAAiB,EACjB,YAAY,EACZ,kBAAkB,EAClB,iBAAiB,EACjB,cAAc,EACd,uBAAuB,EACvB,kBAAkB,CACnB,CAAC;IACFM,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACC,SAAS;IACvB,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACC,eAAe;IAC7B,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,WAAW;IACzB,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACC,iBAAiB;IAC/B,CAAC;IACDC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACC,QAAQ;IACtB,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACC,WAAW;IACzB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAC,kCAAkC,EAAE;UACnEC,OAAO,EAAE;YACPC,aAAa,EAAG,UAAS,IAAI,CAACC,QAAS;UACzC;QACF,CAAC,CAAC;QACV,IAAIJ,QAAQ,CAACjB,IAAI,CAACsB,OAAO,EAAE;UACjB,IAAI,CAACnB,MAAK,GAAIc,QAAQ,CAACjB,IAAI,CAACO,QAAQ;UACpCgB,OAAO,CAACC,GAAG,CAAC,8BAA8B,EAAE,IAAI,CAACrB,MAAM,CAAC;QA