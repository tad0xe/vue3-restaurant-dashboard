{"ast":null,"code":"import { QrcodeStream } from \"vue-qrcode-reader\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    QrcodeStream\n  },\n  data() {\n    return {\n      reference: null,\n      orderData: null\n    };\n  },\n  methods: {\n    async onDecode(ref) {\n      console.log(\"🔍 Scanned reference:\", ref);\n      this.reference = ref;\n      try {\n        const res = await axios.get(`http://localhost:4000/api/order/${ref}`);\n        if (res.data && res.data.success) {\n          this.orderData = res.data.order;\n          console.log(\"✅ Order loaded:\", this.orderData);\n        } else {\n          throw new Error(res.data.error || \"Order not found\");\n        }\n      } catch (err) {\n        alert(\"❌ Order not found or server error.\");\n        console.error(\"Fetch error:\", err);\n        this.orderData = null;\n      }\n    },\n    onInit(promise) {\n      promise.then(() => console.log(\"📷 Camera initialized\")).catch(e => console.error(\"🚫 Camera error:\", e));\n    }\n  }\n};","map":{"version":3,"names":["QrcodeStream","axios","components","data","reference","orderData","methods","onDecode","ref","console","log","res","get","success","order","Error","error","err","alert","onInit","promise","then","catch","e"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\rex.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- QR Scanner -->\r\n    <qrcode-stream @decode=\"onDecode\" @init=\"onInit\" />\r\n\r\n    <!-- Scanned Reference -->\r\n    <p v-if=\"reference\">Scanned Reference: <strong>{{ reference }}</strong></p>\r\n\r\n    <!-- Order Info -->\r\n    <div v-if=\"orderData\">\r\n      <h3>🎟 Order Details</h3>\r\n      <p><strong>Title:</strong> {{ orderData.title }}</p>\r\n      <p><strong>Email:</strong> {{ orderData.contact.email }}</p>\r\n      <p><strong>Phone:</strong> {{ orderData.contact.phone }}</p>\r\n      <p><strong>Price:</strong> ₦{{ orderData.price }}</p>\r\n      <ul>\r\n        <li v-for=\"ticket in orderData.tickets\" :key=\"ticket.name\">\r\n          {{ ticket.name }} × {{ ticket.quantity }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { QrcodeStream } from \"vue-qrcode-reader\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: { QrcodeStream },\r\n  data() {\r\n    return {\r\n      reference: null,\r\n      orderData: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async onDecode(ref) {\r\n      console.log(\"🔍 Scanned reference:\", ref);\r\n      this.reference = ref;\r\n\r\n      try {\r\n        const res = await axios.get(`http://localhost:4000/api/order/${ref}`);\r\n        if (res.data && res.data.success) {\r\n          this.orderData = res.data.order;\r\n          console.log(\"✅ Order loaded:\", this.orderData);\r\n        } else {\r\n          throw new Error(res.data.error || \"Order not found\");\r\n        }\r\n      } catch (err) {\r\n        alert(\"❌ Order not found or server error.\");\r\n        console.error(\"Fetch error:\", err);\r\n        this.orderData = null;\r\n      }\r\n    },\r\n\r\n    onInit(promise) {\r\n      promise\r\n        .then(() => console.log(\"📷 Camera initialized\"))\r\n        .catch((e) => console.error(\"🚫 Camera error:\", e));\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAyBA,SAASA,YAAW,QAAS,mBAAmB;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IAAEF;EAAa,CAAC;EAC5BG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAACC,GAAG,EAAE;MAClBC,OAAO,CAACC,GAAG,CAAC,uBAAuB,EAAEF,GAAG,CAAC;MACzC,IAAI,CAACJ,SAAQ,GAAII,GAAG;MAEpB,IAAI;QACF,MAAMG,GAAE,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAE,mCAAkCJ,GAAI,EAAC,CAAC;QACrE,IAAIG,GAAG,CAACR,IAAG,IAAKQ,GAAG,CAACR,IAAI,CAACU,OAAO,EAAE;UAChC,IAAI,CAACR,SAAQ,GAAIM,GAAG,CAACR,IAAI,CAACW,KAAK;UAC/BL,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACL,SAAS,CAAC;QAChD,OAAO;UACL,MAAM,IAAIU,K