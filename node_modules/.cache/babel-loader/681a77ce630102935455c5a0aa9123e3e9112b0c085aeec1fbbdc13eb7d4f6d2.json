\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentStep: 0,\r\n      timer: \"10:00\",\r\n      countdown: null,\r\n\r\n      steps: [\"Tickets\", \"Contact\", \"Payment\"],\r\n      tickets: [\r\n        { id: 1, name: \"EDE-LMD25 Regular\", price: 3000, quantity: 0 },\r\n        { id: 2, name: \"EDE-LMD25 VIP\", price: 10000, quantity: 1 },\r\n        { id: 3, name: \"EDE-LMD25 SOAT\", price: 25000, quantity: 0 },\r\n      ],\r\n      contact: {\r\n        firstName: \"\",\r\n        lastName: \"\",\r\n        email: \"\",\r\n        confirmEmail: \"\",\r\n        phone: \"\",\r\n      },\r\n      paymentMethod: \"\",\r\n      termsAccepted: false,\r\n      subscribe: false,\r\n    };\r\n  },\r\n  computed: {\r\n    selectedTicket() {\r\n      return this.tickets.find((ticket) => ticket.quantity > 0);\r\n    },\r\n    canCheckout() {\r\n      return this.paymentMethod && this.termsAccepted;\r\n    },\r\n  },\r\n  watch: {\r\n    currentStep() {\r\n      this.updateProgressLine();\r\n    },\r\n  },\r\n  methods: {\r\n    updateProgressLine() {\r\n      const wrapper = this.$el.querySelector(\".stepper-wrapper\");\r\n      if (wrapper) {\r\n        const value = this.currentStep / (this.steps.length - 1);\r\n        wrapper.style.setProperty(\"--progress\", value);\r\n      }\r\n    },\r\n    nextStep() {\r\n      if (this.currentStep < this.steps.length - 1) this.currentStep++;\r\n    },\r\n    prevStep() {\r\n      if (this.currentStep > 0) this.currentStep--;\r\n    },\r\n\r\n    checkout() {\r\n      alert(\"✅ Checkout successful!\");\r\n    },\r\n    startTimer() {\r\n      let totalSeconds = 600;\r\n      this.countdown = setInterval(() => {\r\n        totalSeconds--;\r\n        const minutes = String(Math.floor(totalSeconds / 60)).padStart(2, \"0\");\r\n        const seconds = String(totalSeconds % 60).padStart(2, \"0\");\r\n        this.timer = `${minutes}:${seconds}`;\r\n        if (totalSeconds <= 0) {\r\n          clearInterval(this.countdown);\r\n          alert(\"⏰ Time expired! Please restart.\");\r\n          location.reload();\r\n        }\r\n      }, 1000);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.startTimer();\r\n    this.updateProgressLine();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 1200px;\r\n}\r\n.stepper-wrapper {\r\n  position: relative;\r\n  width: 100%;\r\n  padding: 0 15px;\r\n}\r\n\r\n.stepper {\r\n  position: relative;\r\n  align-items: center;\r\n  display: flex;\r\n}\r\n\r\n.currentStep {\r\n  position: relative;\r\n  z-index: 2;\r\n  flex: 1;\r\n}\r\n\r\n.circle {\r\n  width: 30px;\r\n  height: 30px;\r\n  border-radius: 50%;\r\n  border: 2px solid #adb5bd;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin: 0 auto 8px;\r\n  background-color: white;\r\n  color: #adb5bd;\r\n  font-weight: bold;\r\n}\r\n\r\n.currentStep.active .circle {\r\n  border-color: #429371;\r\n  background-color: #429371;\r\n  color: white;\r\n  transition: background-color 0.3s, border-color 0.3s;\r\n}\r\n\r\n.currentStep.completed .circle {\r\n  background-color: #429371;\r\n  color: white;\r\n  border-color: #429371;\r\n}\r\n\r\n.label {\r\n  font-size: 14px;\r\n  color: #6c757d;\r\n}\r\n\r\n.currentStep.active .label,\r\n.currentStep.completed .label {\r\n  color: #429371;\r\n}\r\n\r\n/* Progress line behind steps */\r\n.progress-line {\r\n  position: absolute;\r\n  top: 15px;\r\n  left: 0;\r\n  right: 0;\r\n  height: 2px;\r\n  background-color: #dee2e6;\r\n  z-index: 1;\r\n}\r\n\r\n.progress-fill {\r\n  height: 100%;\r\n  background-color: #429371;\r\n  width: 0%;\r\n  transition: width 0.4s ease-in-out;\r\n}\r\n</style>\r\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAgB;;oBAC7BC,UA4BU,C;;uBA7BRC,mBAAA,CA+BM,OA/BNC,UA+BM,EAAAC,UAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}