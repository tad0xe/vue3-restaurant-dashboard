{"ast":null,"code":"import { QrcodeStream } from \"vue-qrcode-reader\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    QrcodeStream\n  },\n  data() {\n    return {\n      reference: null,\n      orderData: null,\n      loading: false,\n      error: null\n    };\n  },\n  methods: {\n    async onDecode(ref) {\n      const cleanRef = ref.trim(); // ‚úÖ Important: remove hidden chars\n      this.reference = cleanRef;\n      this.loading = true;\n      this.error = null;\n      this.orderData = null;\n      try {\n        const res = await axios.get(`http://localhost:4000/api/order/${cleanRef}`);\n        this.orderData = res.data.order;\n      } catch (err) {\n        this.error = \"Order not found or server error.\";\n        console.error(\"Error fetching order:\", err);\n      } finally {\n        this.loading = false;\n      }\n    },\n    onInit(promise) {\n      promise.then(() => console.log(\"üì∑ Camera initialized\")).catch(e => console.error(\"Camera error:\", e));\n    }\n  }\n};","map":{"version":3,"names":["QrcodeStream","axios","components","data","reference","orderData","loading","error","methods","onDecode","ref","cleanRef","trim","res","get","order","err","console","onInit","promise","then","log","catch","e"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\rex.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- QR Scanner -->\r\n    <qrcode-stream @decode=\"onDecode\" @init=\"onInit\" />\r\n\r\n    <!-- Feedback -->\r\n    <p v-if=\"loading\">üîÑ Loading order...</p>\r\n    <p v-if=\"error\" class=\"text-danger\">‚ùå {{ error }}</p>\r\n    <p v-if=\"reference && !loading && !error\">\r\n      ‚úÖ Scanned Reference: <strong>{{ reference }}</strong>\r\n    </p>\r\n\r\n    <!-- Order Info -->\r\n    <div v-if=\"orderData\">\r\n      <h3>üéü Order Details</h3>\r\n      <p><strong>Title:</strong> {{ orderData.title }}</p>\r\n      <p><strong>Email:</strong> {{ orderData.contact.email }}</p>\r\n      <p><strong>Phone:</strong> {{ orderData.contact.phone }}</p>\r\n      <p><strong>Price:</strong> ‚Ç¶{{ orderData.price }}</p>\r\n\r\n      <ul>\r\n        <li v-for=\"ticket in orderData.tickets\" :key=\"ticket.name\">\r\n          {{ ticket.name }} √ó {{ ticket.quantity }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { QrcodeStream } from \"vue-qrcode-reader\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: { QrcodeStream },\r\n  data() {\r\n    return {\r\n      reference: null,\r\n      orderData: null,\r\n      loading: false,\r\n      error: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async onDecode(ref) {\r\n      const cleanRef = ref.trim(); // ‚úÖ Important: remove hidden chars\r\n      this.reference = cleanRef;\r\n      this.loading = true;\r\n      this.error = null;\r\n      this.orderData = null;\r\n\r\n      try {\r\n        const res = await axios.get(`http://localhost:4000/api/order/${cleanRef}`);\r\n        this.orderData = res.data.order;\r\n      } catch (err) {\r\n        this.error = \"Order not found or server error.\";\r\n        console.error(\"Error fetching order:\", err);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    onInit(promise) {\r\n      promise\r\n        .then(() => console.log(\"üì∑ Camera initialized\"))\r\n        .catch((e) => console.error(\"Camera error:\", e));\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.text-danger {\r\n  color: red;\r\n}\r\n</style>\r\n"],"mappings":"AA8BA,SAASA,YAAW,QAAS,mBAAmB;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IAAEF;EAAa,CAAC;EAC5BG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAACC,GAAG,EAAE;MAClB,MAAMC,QAAO,GAAID,GAAG,CAACE,IAAI,CAAC,CAAC,EAAE;MAC7B,IAAI,CAACR,SAAQ,GAAIO,QAAQ;MACzB,IAAI,CAACL,OAAM,GAAI,IAAI;