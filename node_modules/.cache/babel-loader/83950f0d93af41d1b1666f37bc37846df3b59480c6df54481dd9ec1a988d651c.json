{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"ProductForm\",\n  data() {\n    return {\n      currentStep: 0,\n      totalSteps: 5,\n      categories: [],\n      categoryID: \"\",\n      product: {\n        title: \"\",\n        description: \"\",\n        tag: \"\",\n        price: {\n          normal: 0,\n          vip: 0,\n          vvip: 0\n        },\n        event: {\n          date: \"\",\n          startTime: \"\",\n          endTime: \"\",\n          timezone: \"UTC\",\n          location: \"\"\n        }\n      },\n      photos: [],\n      tickets: [{\n        name: \"\",\n        price: 0,\n        quantity: 0\n      }]\n    };\n  },\n  computed: {\n    ...mapGetters([\"getToken\"])\n  },\n  async created() {\n    try {\n      const res = await axios.get(\"http://localhost:4000/api/categories\");\n      this.categories = res.data.categories || res.data;\n    } catch (error) {\n      console.error(\"Failed to fetch categories:\", error);\n    }\n  },\n  methods: {\n    nextStep() {\n      if (this.currentStep < this.totalSteps - 1) {\n        this.currentStep++;\n      }\n    },\n    prevStep() {\n      if (this.currentStep > 0) {\n        this.currentStep--;\n      }\n    },\n    handleFiles(event) {\n      this.photos = Array.from(event.target.files);\n    },\n    addTicket() {\n      this.tickets.push({\n        name: \"\",\n        price: 0,\n        quantity: 0\n      });\n    },\n    removeTicket(index) {\n      this.tickets.splice(index, 1);\n    },\n    async submitProduct() {\n      const formData = new FormData();\n      formData.append(\"categoryID\", this.categoryID);\n      formData.append(\"title\", this.product.title);\n      formData.append(\"description\", this.product.description);\n      formData.append(\"tag\", this.product.tag);\n      formData.append(\"price\", JSON.stringify(this.product.price));\n      formData.append(\"eventDate\", this.product.event.date);\n      formData.append(\"startTime\", this.product.event.startTime);\n      formData.append(\"endTime\", this.product.event.endTime);\n      formData.append(\"timezone\", this.product.event.timezone);\n      formData.append(\"location\", this.product.event.location);\n      this.tickets.forEach(ticket => {\n        formData.append(\"ticketName\", ticket.name);\n        formData.append(\"ticketPrice\", ticket.price);\n        formData.append(\"ticketQuantity\", ticket.quantity);\n      });\n      this.photos.forEach(photo => {\n        formData.append(\"photos\", photo);\n      });\n      try {\n        const res = await axios.post(\"http://localhost:4000/api/products\", formData, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`,\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        console.log(\"Product created:\", res.data);\n        alert(\"Product created successfully!\");\n      } catch (err) {\n        console.error(\"Failed to create product:\", err);\n        alert(\"Failed to create product\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","currentStep","totalSteps","categories","categoryID","product","title","description","tag","price","normal","vip","vvip","event","date","startTime","endTime","timezone","location","photos","tickets","quantity","computed","created","res","get","error","console","methods","nextStep","prevStep","handleFiles","Array","from","target","files","addTicket","push","removeTicket","index","splice","submitProduct","formData","FormData","append","JSON","stringify","forEach","ticket","photo","post","headers","Authorization","getToken","log","alert","err"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\rex.vue"],"sourcesContent":["<template>\r\n  <form @submit.prevent=\"submitProduct\" enctype=\"multipart/form-data\">\r\n    <h2>Create Product</h2>\r\n\r\n    <!-- Step 1: Basic Info -->\r\n    <div v-if=\"currentStep === 0\">\r\n      <div class=\"mb-3\">\r\n        <label class=\"form-label\">Event Category</label>\r\n        <select class=\"form-select\" v-model=\"categoryID\" required>\r\n          <option value=\"\" disabled>Select a category</option>\r\n          <option v-for=\"category in categories\" :key=\"category._id\" :value=\"category._id\">\r\n            {{ category.type }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n\r\n      <input v-model=\"product.title\" type=\"text\" placeholder=\"Title\" required />\r\n      <textarea v-model=\"product.description\" placeholder=\"Description\" required></textarea>\r\n      <input v-model=\"product.tag\" type=\"text\" placeholder=\"Tags (comma-separated)\" />\r\n    </div>\r\n\r\n    <!-- Step 2: Pricing -->\r\n    <div v-if=\"currentStep === 1\">\r\n      <input v-model.number=\"product.price.normal\" type=\"number\" placeholder=\"Normal Price\" required />\r\n      <input v-model.number=\"product.price.vip\" type=\"number\" placeholder=\"VIP Price\" required />\r\n      <input v-model.number=\"product.price.vvip\" type=\"number\" placeholder=\"VVIP Price\" required />\r\n    </div>\r\n\r\n    <!-- Step 3: Event Details -->\r\n    <div v-if=\"currentStep === 2\">\r\n      <input v-model=\"product.event.date\" type=\"date\" required />\r\n      <input v-model=\"product.event.startTime\" type=\"time\" required />\r\n      <input v-model=\"product.event.endTime\" type=\"time\" required />\r\n      <input v-model=\"product.event.timezone\" type=\"text\" placeholder=\"Timezone (e.g. UTC)\" required />\r\n      <input v-model=\"product.event.location\" type=\"text\" placeholder=\"Location\" required />\r\n    </div>\r\n\r\n    <!-- Step 4: Tickets -->\r\n    <div v-if=\"currentStep === 3\">\r\n      <div v-for=\"(ticket, index) in tickets\" :key=\"index\" class=\"ticket-group\">\r\n        <input v-model=\"ticket.name\" type=\"text\" placeholder=\"Ticket Name\" required />\r\n        <input v-model.number=\"ticket.price\" type=\"number\" placeholder=\"Ticket Price\" required />\r\n        <input v-model.number=\"ticket.quantity\" type=\"number\" placeholder=\"Ticket Quantity\" required />\r\n        <button type=\"button\" @click=\"removeTicket(index)\">Remove</button>\r\n      </div>\r\n      <button type=\"button\" @click=\"addTicket\">Add Ticket</button>\r\n    </div>\r\n\r\n    <!-- Step 5: Photos -->\r\n    <div v-if=\"currentStep === 4\">\r\n      <input type=\"file\" multiple @change=\"handleFiles\" accept=\"image/*\" />\r\n      <p v-if=\"photos.length\">Selected: {{ photos.length }} image(s)</p>\r\n    </div>\r\n\r\n    <!-- Navigation Buttons -->\r\n    <div class=\"mt-4 d-flex justify-content-between\">\r\n      <button type=\"button\" @click=\"prevStep\" :disabled=\"currentStep === 0\">Back</button>\r\n      <button\r\n        v-if=\"currentStep < totalSteps - 1\"\r\n        type=\"button\"\r\n        @click=\"nextStep\"\r\n      >\r\n        Next\r\n      </button>\r\n      <button v-else type=\"submit\">Submit Product</button>\r\n    </div>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ProductForm\",\r\n  data() {\r\n    return {\r\n      currentStep: 0,\r\n      totalSteps: 5,\r\n      categories: [],\r\n      categoryID: \"\",\r\n      product: {\r\n        title: \"\",\r\n        description: \"\",\r\n        tag: \"\",\r\n        price: {\r\n          normal: 0,\r\n          vip: 0,\r\n          vvip: 0,\r\n        },\r\n        event: {\r\n          date: \"\",\r\n          startTime: \"\",\r\n          endTime: \"\",\r\n          timezone: \"UTC\",\r\n          location: \"\",\r\n        },\r\n      },\r\n      photos: [],\r\n      tickets: [{ name: \"\", price: 0, quantity: 0 }],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const res = await axios.get(\"http://localhost:4000/api/categories\");\r\n      this.categories = res.data.categories || res.data;\r\n    } catch (error) {\r\n      console.error(\"Failed to fetch categories:\", error);\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    nextStep() {\r\n      if (this.currentStep < this.totalSteps - 1) {\r\n        this.currentStep++;\r\n      }\r\n    },\r\n    prevStep() {\r\n      if (this.currentStep > 0) {\r\n        this.currentStep--;\r\n      }\r\n    },\r\n    handleFiles(event) {\r\n      this.photos = Array.from(event.target.files);\r\n    },\r\n    addTicket() {\r\n      this.tickets.push({ name: \"\", price: 0, quantity: 0 });\r\n    },\r\n    removeTicket(index) {\r\n      this.tickets.splice(index, 1);\r\n    },\r\n    async submitProduct() {\r\n      const formData = new FormData();\r\n\r\n      formData.append(\"categoryID\", this.categoryID);\r\n      formData.append(\"title\", this.product.title);\r\n      formData.append(\"description\", this.product.description);\r\n      formData.append(\"tag\", this.product.tag);\r\n      formData.append(\"price\", JSON.stringify(this.product.price));\r\n      formData.append(\"eventDate\", this.product.event.date);\r\n      formData.append(\"startTime\", this.product.event.startTime);\r\n      formData.append(\"endTime\", this.product.event.endTime);\r\n      formData.append(\"timezone\", this.product.event.timezone);\r\n      formData.append(\"location\", this.product.event.location);\r\n\r\n      this.tickets.forEach((ticket) => {\r\n        formData.append(\"ticketName\", ticket.name);\r\n        formData.append(\"ticketPrice\", ticket.price);\r\n        formData.append(\"ticketQuantity\", ticket.quantity);\r\n      });\r\n\r\n      this.photos.forEach((photo) => {\r\n        formData.append(\"photos\", photo);\r\n      });\r\n\r\n      try {\r\n        const res = await axios.post(\"http://localhost:4000/api/products\", formData, {\r\n          headers: {\r\n            Authorization: `Bearer ${this.getToken}`,\r\n            \"Content-Type\": \"multipart/form-data\",\r\n          },\r\n        });\r\n        console.log(\"Product created:\", res.data);\r\n        alert(\"Product created successfully!\");\r\n      } catch (err) {\r\n        console.error(\"Failed to create product:\", err);\r\n        alert(\"Failed to create product\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAsEA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,UAAU,EAAE,CAAC;MACbC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,GAAG,EAAE,EAAE;QACPC,KAAK,EAAE;UACLC,MAAM,EAAE,CAAC;UACTC,GAAG,EAAE,CAAC;UACNC,IAAI,EAAE;QACR,CAAC;QACDC,KAAK,EAAE;UACLC,IAAI,EAAE,EAAE;UACRC,SAAS,EAAE,EAAE;UACbC,OAAO,EAAE,EAAE;UACXC,QAAQ,EAAE,KAAK;UACfC,QAAQ,EAAE;QACZ;MACF,CAAC;MACDC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,CAAC;QAAErB,IAAI,EAAE,EAAE;QAAEU,KAAK,EAAE,CAAC;QAAEY,QAAQ,EAAE;MAAE,CAAC;IAC/C,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACR,GAAGxB,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EAED,MAAMyB,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAM3B,KAAK,CAAC4B,GAAG,CAAC,sCAAsC,CAAC;MACnE,IAAI,CAACtB,UAAS,GAAIqB,GAAG,CAACxB,IAAI,CAACG,UAAS,IAAKqB,GAAG,CAACxB,IAAI;IACnD,EAAE,OAAO0B,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;IACrD;EACF,CAAC;EAEDE,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAAC5B,WAAU,GAAI,IAAI,CAACC,UAAS,GAAI,CAAC,EAAE;QAC1C,IAAI,CAACD,WAAW,EAAE;MACpB;IACF,CAAC;IACD6B,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAAC7B,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF,CAAC;IACD8B,WAAWA,CAAClB,KAAK,EAAE;MACjB,IAAI,CAACM,MAAK,GAAIa,KAAK,CAACC,IAAI,CAACpB,KAAK,CAACqB,MAAM,CAACC,KAAK,CAAC;IAC9C,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAAChB,OAAO,CAACiB,IAAI,CAAC;QAAEtC,IAAI,EAAE,EAAE;QAAEU,KAAK,EAAE,CAAC;QAAEY,QAAQ,EAAE;MAAE,CAAC,CAAC;IACxD,CAAC;IACDiB,YAAYA,CAACC,KAAK,EAAE