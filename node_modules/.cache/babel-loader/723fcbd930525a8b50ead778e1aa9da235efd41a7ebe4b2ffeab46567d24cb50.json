[];\n      const tags = [\"Sale\", \"Deal\", \"Limited Edition\", \"100% orgasm\", \"As Seen on TV\", \"Best Seller\", \"Top Rated\"];\n      this.products = this.shuffleArray(productsResponse.data.products || []).map(product => ({\n        ...product,\n        randomTag: Math.random() < 0.5 ? tags[Math.floor(Math.random() * tags.length)] : null\n      }));\n\n      // Set ranges based on products\n      if (this.products.length) {\n        const prices = this.products.map(product => product.price);\n        this.minPrice = Math.min(...prices);\n        this.maxPrice = Math.max(...prices);\n        this.selectedMinPrice = this.minPrice;\n        this.selectedMaxPrice = this.maxPrice;\n        const sizes = this.products.map(product => product.size);\n        this.minSize = Math.min(...sizes);\n        this.maxSize = Math.max(...sizes);\n        this.selectedSize = this.maxSize;\n      }\n    } catch (err) {\n      console.error(err.response?.data?.message || \"An error occurred\");\n    }\n  },\n  methods: {\n    openModal() {\n      this.isModalOpen = true;\n      document.body.style.overflow = \"hidden\"; // Disable body scrolling when modal is open\n    },\n\n    // Close the modal\n    closeModal() {\n      this.isModalOpen = false;\n      document.body.style.overflow = \"\"; // Re-enable body scrolling\n    },\n\n    shuffleArray(array) {\n      return array.sort(() => Math.random() - 0.5);\n    },\n    ...mapActions([\"addProductToCart\"]),\n    async addProductToCart(product) {\n      try {\n        await this.$store.dispatch(\"addProductToCart\", product); // call Vuex action manually\n        this.$toast.open({\n          message: \"✅ Product added to cart!\",\n          type: \"success\",\n          duration: 3000,\n          dismissible: true,\n          position: \"top\"\n        });\n      } catch (error) {\n        this.$toast.open({\n          message: \"✅ Product added to cart!\",\n          type: \"success\",\n          duration: 3000,\n          dismissible: true,\n          position: \"top\"\n        });\n      }\n    },\n    goToProduct(productTitle) {\n      // Normalize the product title for the URL: Replace spaces with hyphens\n      let normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\n      console.log(\"Normalized Product Title for URL:\", normalizedProductTitle);\n\n      // Store the raw title in Vuex (for the backend)\n      this.$store.dispatch(\"setProductUrl\", productTitle);\n\n      // Use the normalized title in the URL (for better SEO)\n      this.$router.push({\n        name: \"ProductDetails\",\n        params: {\n          title: normalizedProductTitle\n        } // For URL with hyphens\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","mapActions","name","data","isModalOpen","categories","products","searchQuery","selectedBrands","selectedMaterials","selectedSize","selectedMinPrice","selectedMaxPrice","minSize","maxSize","minPrice","maxPrice","computed","uniqueBrands","brandSet","Set","map","product","brand","trim","toLowerCase","Array","from","uniqueMaterials","materialSet","material","filteredProducts","filter","matchesSearch","title","includes","matchesBrand","length","matchesMaterial","matchesSize","size","matchesPrice","price","created","categoriesResponse","productsResponse","Promise","all","get","tags","shuffleArray","randomTag","Math","random","floor","prices","min","max","sizes","err","console","error","response","message","methods","openModal","document","body","style","overflow","closeModal","array","sort","addProductToCart","$store","dispatch","$toast","open","type","duration","dismissible","position","goToProduct","productTitle","normalizedProductTitle","replace","log","$router","push","params"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\vue3-restaurant-dashboard\\src\\components\\discover.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"container-fluid bg-light py-3 border-top\">\r\n      <div\r\n        class=