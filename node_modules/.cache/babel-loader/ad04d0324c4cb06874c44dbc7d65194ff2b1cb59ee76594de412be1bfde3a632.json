v class='demo-app-sidebar-section'>\r\n        <label>\r\n          <input\r\n            type='checkbox'\r\n            v-model='calendarOptions.weekends'\r\n          />\r\n          toggle weekends\r\n        </label>\r\n      </div>\r\n      <div class='demo-app-sidebar-section'>\r\n        <h2>All Events ({{ currentEvents.length }})</h2>\r\n        <ul>\r\n          <li v-for='event in currentEvents' :key='event.id'>\r\n            <b>{{ event.start }}</b>\r\n            <i>{{ event.title }}</i>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <div class='demo-app-main'>\r\n      <FullCalendar\r\n        class='demo-app-calendar'\r\n        :options='calendarOptions'\r\n      >\r\n        <template #eventContent='arg'>\r\n          <b>{{ arg.timeText }}</b>\r\n          <i>{{ arg.event.title }}</i>\r\n        </template>\r\n      </FullCalendar>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent } from 'vue'\r\nimport FullCalendar from '@fullcalendar/vue3'\r\nimport dayGridPlugin from '@fullcalendar/daygrid'\r\nimport timeGridPlugin from '@fullcalendar/timegrid'\r\nimport interactionPlugin from '@fullcalendar/interaction'\r\nimport axios from 'axios'\r\n\r\nfunction createEventId() {\r\n  // Generate a unique ID for the event\r\n  return String(Date.now());\r\n}\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    FullCalendar,\r\n  },\r\n  data() {\r\n    return {\r\n      calendarOptions: {\r\n        plugins: [\r\n          dayGridPlugin,\r\n          timeGridPlugin,\r\n          interactionPlugin // needed for dateClick\r\n        ],\r\n        headerToolbar: {\r\n          left: 'prev,next today',\r\n          center: 'title',\r\n          right: 'dayGridMonth,timeGridWeek,timeGridDay'\r\n        },\r\n        initialView: 'dayGridMonth',\r\n        editable: true,\r\n        selectable: true,\r\n        selectMirror: true,\r\n        dayMaxEvents: true,\r\n        weekends: true,\r\n        select: this.handleDateSelect.bind(this), // Bind handleDateSelect method here\r\n        eventClick: this.handleEventClick.bind(this),\r\n        eventsSet: this.handleEvents.bind(this),\r\n        events: [] // Initial events array\r\n      },\r\n      currentEvents: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchEvents();\r\n  },\r\n  methods: {\r\n    fetchEvents() {\r\n      axios.get('https://restaurant-n54j.onrender.com/api/events')\r\n        .then(response => {\r\n          this.currentEvents = response.data;\r\n          this.calendarOptions.events = response.data; // Update the events in the calendar\r\n        })\r\n        .catch(error => {\r\n          console.error('Failed to fetch events:', error);\r\n        });\r\n    },\r\n    handleDateSelect(selectInfo) {\r\n      let title = prompt('Please enter a new title for your event');\r\n\r\n      if (title) {\r\n        let event = {\r\n          id: createEventId(),\r\n          title,\r\n          start: selectInfo.startStr,\r\n          end: selectInfo.endStr,\r\n          allDay: selectInfo.allDay\r\n        };\r\n\r\n        axios.post('https://restaurant-n54j.onrender.com/api/events', event)\r\n          .then(() => {\r\n            this.currentEvents.push(event);\r\n            this.calendarOptions.events.push(event); // Add the new event to the calendar\r\n          })\r\n          .catch(error => {\r\n            console.error('Failed to create event:', error);\r\n          });\r\n      }\r\n    },\r\n    handleEventClick(clickInfo) {\r\n      if (confirm(`Are you sure you want to delete the event '${clickInfo.event.title}'`)) {\r\n        let eventId = clickInfo.event.id;\r\n\r\n        axios.delete(`https://restaurant-n54j.onrender.com/api/events/${eventId}`)\r\n          .then(() => {\r\n            this.currentEvents = this.currentEvents.filter(event => event.id !== eventId);\r\n            this.calendarOptions.events = this.calendarOptions.events.filter(event => event.id !== eventId); // Remove the event from the calendar\r\n          })\r\n          .catch(error => {\r\n            console.error('Failed to delete event:', error);\r\n          });\r\n      }\r\n    },\r\n    handleEvents(events) {\r\n      this.currentEvents = events;\r\n    },\r\n  }\r\n});\r\n</script>\r\n\r\n<style lang='css'>\r\n/* CSS styles here */\r\n</style>\r\n"],"mappings":";;EACQA,KAAK,EAAC;AAAa;;EAElBA,KAAK,EAAC;AAAgB;gCACzBC,mBAAA,CAOM;EAPDD,KAAK,EAAC;AAA0B,I,aACnCC,mBAAA,CAAqB,YAAjB,cAAY,G,aAChBA,mBAAA,CAIK,a,aAHHA,mBAAA,CAAoE,YAAhE,6DAA2D,G,aAC/DA,mBAAA,CAAsC,YAAlC,+BAA6B,G,aACjCA,mBAAA,CAAoC,YAAhC,6BAA2B,E;;EAG9BD,KAAK,EAAC;AAA0B;;EAShCA,KAAK,EAAC;AAA0B;;EAUlCA,KAAK,EAAC;AAAe;;;uBA9B3BE,mBAAA,CAyCK,OAzCLC,UAyCK,GAxCJC,mBAAA,SAAY,EACZH,mBAAA,CA2BM,OA3BNI,UA2BM,GA1BJC,UAOM,EACNL,mBAAA,CAQM,OARNM,UAQM,GAPJN,mBAAA,CAMQ,gB,gBALNA,mBAAA,CAGE;IAFAO,IAAI,EAAC,UAAU;+DACNC,IAAA,CAAAC,eAAe,CAACC,QAAQ,GAAAC,MAAA;qDAAxBH,IAAA,CAAAC,eAAe,CAACC,QAAQ,E,oBACjC,mBAEJ,E,KAEFV,mBAAA,CAQM,OARNY,UAQM,GAPJZ,mBAAA,CAAgD,YAA5C,cAAY,GAAAa,gBAAA,CAAGL,IAAA,CAAAM,aAAa,CAACC,MAAM,IAAG,GAAC,iBAC3Cf,mBAAA,CAKK,c,kBAJHC,mBAAA,CAGKe,SAAA,QAAAC,WAAA,CAHeT,IAAA,CAAAM,aAAa,EAAtBI,KAAK;yBAAhBjB,mBAAA,CAGK;MAH+BkB,GAAG,EAAED,KAAK,CAACE;QAC7CpB,mBAAA,CAAwB,WAAAa,gBAAA,CAAlBK,KAAK,CAACG,KAAK,kBACjBrB,mBAAA,CAAwB,WAAAa,gBAAA,CAAlBK,KAAK,CAACI,KAAK,iB;wCAKzBtB,mBAAA,CAUM,OAVNuB,UAUM,GATJC,YAAA,CAQeC,uBAAA;IAPb1B,KAAK,EAAC,mBAAmB;IACxB2B,OAAO,EAAElB,IAAA,CAAAC;;IAECkB,YAAY,EAAAC,QAAA,CAAEC,GAAG,KAC1B7B,mBAAA,CAAyB,WAAAa,gBAAA,CAAnBgB,GAAG,CAACC,QAAQ,kBAClB9B,mBAAA,CAA4B,WAAAa,gBAAA,CAAtBgB,GAAG,CAACX,KAAK,CAACI,KAAK,iB"},"metadata":{},"sourceType":"module","externalDependencies":[]}