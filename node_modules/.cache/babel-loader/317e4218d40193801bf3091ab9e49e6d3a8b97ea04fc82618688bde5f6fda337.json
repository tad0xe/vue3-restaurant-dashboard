{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      // Price range data\n      minPrice: 0,\n      maxPrice: 1000,\n      selectedMinPrice: 0,\n      selectedMaxPrice: 1000,\n      isModalOpen: false,\n      products: [],\n      searchQuery: \"\",\n      selectedBrands: [],\n      // Selected brands filter\n      selectedMaterials: [],\n      // Selected materials filter\n      selectedSize: 50,\n      // Default max size for slider\n      minSize: 0,\n      // Minimum size\n      maxSize: 100 // Maximum size\n    };\n  },\n\n  computed: {\n    // Get unique brands (case-insensitive & remove extra spaces)\n\n    // Get min & max size from products\n    productSizes() {\n      const sizes = this.products.map(product => product.size);\n      return {\n        min: Math.min(...sizes),\n        max: Math.max(...sizes)\n      };\n    },\n    // Filtered Products\n    filteredProducts() {\n      return this.products.filter(product => {\n        const matchesSearch = product.title.toLowerCase().includes(this.searchQuery.toLowerCase());\n        const matchesBrand = this.selectedBrands.length === 0 || this.selectedBrands.includes(product.brand.toLowerCase().trim());\n        const matchesMaterial = this.selectedMaterials.length === 0 || this.selectedMaterials.includes(product.material.toLowerCase().trim());\n        const matchesSize = product.size <= this.selectedSize;\n        const matchesPrice = product.price >= this.selectedMinPrice && product.price <= this.selectedMaxPrice;\n        return matchesSearch && matchesBrand && matchesMaterial && matchesSize && matchesPrice;\n      });\n    }\n  },\n  async created() {\n    const tags = [\"Sale\", \"Deal\", \"Limited Edition\", \"100% orgasm\", \"As Seen on TV\", \"Best Seller\", \"Top Rated\"];\n    const slug = this.$route.params.categoryType;\n\n    // Convert slug to readable category name\