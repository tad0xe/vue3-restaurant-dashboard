osition: \"top\"\n        });\n      } catch (error) {\n        this.$toast.open({\n          message: \"✅ Product added to cart!\",\n          type: \"success\",\n          duration: 3000,\n          dismissible: true,\n          position: \"top\"\n        });\n      }\n    },\n    shuffleArray(array) {\n      return array.map(item => ({\n        item,\n        sort: Math.random()\n      })).sort((a, b) => a.sort - b.sort).map(({\n        item\n      }) => item);\n    },\n    goToProduct(productTitle) {\n      // Normalize the product title for the URL: Replace spaces with hyphens\n      let normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\n      console.log(\"Normalized Product Title for URL:\", normalizedProductTitle);\n\n      // Store the raw title in Vuex (for the backend)\n      this.$store.dispatch(\"setProductUrl\", productTitle);\n\n      // Use the normalized title in the URL (for better SEO)\n      this.$router.push({\n        name: \"ProductDetails\",\n        params: {\n          title: normalizedProductTitle\n        } // For URL with hyphens\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapActions","axios","data","minPrice","maxPrice","selectedMinPrice","selectedMaxPrice","isModalOpen","products","searchQuery","selectedBrands","selectedMaterials","selectedSize","minSize","maxSize","computed","productSizes","sizes","map","product","size","min","Math","max","filteredProducts","filter","matchesSearch","title","toLowerCase","includes","matchesBrand","length","brand","trim","matchesMaterial","material","matchesSize","matchesPrice","price","created","tags","slug","$route","params","categoryType","replace","finalCategory","l","toUpperCase","response","get","encodeURIComponent","console","log","shuffledProducts","shuffleArray","randomTag","random","floor","categories","prices","methods","openModal","document","body","style","overflow","closeModal","addProductToCart","$store","dispatch","$toast","open","message","type","duration","dis