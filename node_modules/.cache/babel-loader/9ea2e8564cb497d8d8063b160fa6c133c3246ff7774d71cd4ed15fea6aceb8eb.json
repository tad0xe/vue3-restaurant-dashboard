{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"BankManager\",\n  data() {\n    return {\n      form: {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\",\n        userId: \"example-user-id\" // Replace or set dynamically if needed\n      },\n\n      banks: [],\n      isEditing: false,\n      editId: null\n    };\n  },\n  mounted() {\n    this.fetchBanks();\n  },\n  methods: {\n    async fetchBanks() {\n      try {\n        const res = await axios.get(\"http://localhost:4000/api/bank\");\n        this.banks = res.data.data;\n      } catch (err) {\n        console.error(\"Error fetching banks:\", err);\n      }\n    },\n    async createBank() {\n      try {\n        const res = await axios.post(\"http://localhost:4000/api/bank\", this.form);\n        console.log(\"Bank created:\", res.data);\n        alert(\"‚úÖ Bank created successfully!\");\n        this.banks.push(res.data.data);\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error creating bank:\", err);\n        alert(\"‚ùå Failed to create.\");\n      }\n    },\n    editBank(bank) {\n      this.isEditing = true;\n      this.editId = bank._id;\n      this.form = {\n        ...bank\n      };\n    },\n    cancelEdit() {\n      this.resetForm();\n    },\n    async updateBank() {\n      try {\n        const res = await axios.put(`http://localhost:4000/api/bank/${this.editId}`, this.form);\n        const index = this.banks.findIndex(b => b._id === this.editId);\n        if (index !== -1) this.banks[index] = res.data.data;\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error updating bank:\", err);\n        alert(\"‚ùå Update failed.\");\n      }\n    },\n    async deleteBank(id) {\n      if (!confirm(\"Delete this bank info?\")) return;\n      try {\n        await axios.delete(`http://localhost:4000/api/bank/${id}`);\n        this.banks = this.banks.filter(b => b._id !== id);\n      } catch (err) {\n        console.error(\"Error deleting bank:\", err);\n        alert(\"‚ùå Delete failed.\");\n      }\n    },\n    resetForm() {\n      this.form = {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\",\n        userId: \"example-user-id\"\n      };\n      this.isEditing = false;\n      this.editId = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","accountName","accountNumber","bankName","userId","banks","isEditing","editId","mounted","fetchBanks","methods","res","get","err","console","error","createBank","post","log","alert","push","resetForm","editBank","bank","_id","cancelEdit","updateBank","put","index","findIndex","b","deleteBank","id","confirm","delete","filter"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\app\\calender.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-4\">\r\n    <h2>üè¶ Bank Info Manager</h2>\r\n\r\n    <!-- ‚úÖ Create or Edit Form -->\r\n    <form @submit.prevent=\"isEditing ? updateBank() : createBank()\" class=\"card p-3 mb-4 shadow-sm\">\r\n      <h5>{{ isEditing ? '‚úèÔ∏è Edit Bank Info' : '‚ûï Add Bank Info' }}</h5>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Account Name</label>\r\n        <input v-model=\"form.accountName\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Account Number</label>\r\n        <input v-model=\"form.accountNumber\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <label>Bank Name</label>\r\n        <input v-model=\"form.bankName\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"btn btn-success\">{{ isEditing ? 'Update' : 'Create' }}</button>\r\n      <button v-if=\"isEditing\" type=\"button\" class=\"btn btn-secondary ms-2\" @click=\"cancelEdit\">Cancel</button>\r\n    </form>\r\n\r\n    <!-- ‚úÖ Bank Info List -->\r\n    <div v-if=\"banks\">\r\n      <h5>üìã Saved Bank Info</h5>\r\n      <ul class=\"list-group\">\r\n        <li v-for=\"bank in banks\" :key=\"bank._id\" class=\"list-group-item d-flex justify-content-between align-items-center\">\r\n          <div>\r\n            <strong>{{ bank.accountName }}</strong><br />\r\n            {{ bank.accountNumber }} ‚Äî {{ bank.bankName }}\r\n          </div>\r\n          <div>\r\n            <button class=\"btn btn-sm btn-warning me-2\" @click=\"editBank(bank)\">Edit</button>\r\n            <button class=\"btn btn-sm btn-danger\" @click=\"deleteBank(bank._id)\">Delete</button>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div v-else>\r\n      <p>No bank records yet.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"BankManager\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n        userId: \"example-user-id\", // Replace or set dynamically if needed\r\n      },\r\n      banks: [],\r\n      isEditing: false,\r\n      editId: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchBanks();\r\n  },\r\n  methods: {\r\n    async fetchBanks() {\r\n      try {\r\n        const res = await axios.get(\"http://localhost:4000/api/bank\");\r\n        this.banks = res.data.data;\r\n      } catch (err) {\r\n        console.error(\"Error fetching banks:\", err);\r\n      }\r\n    },\r\n    async createBank() {\r\n      try {\r\n        const res = await axios.post(\"http://localhost:4000/api/bank\", this.form);\r\n        console.log(\"Bank created:\", res.data);\r\n        alert(\"‚úÖ Bank created successfully!\");\r\n        this.banks.push(res.data.data);\r\n\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error creating bank:\", err);\r\n        alert(\"‚ùå Failed to create.\");\r\n      }\r\n    },\r\n    editBank(bank) {\r\n      this.isEditing = true;\r\n      this.editId = bank._id;\r\n      this.form = { ...bank };\r\n    },\r\n    cancelEdit() {\r\n      this.resetForm();\r\n    },\r\n    async updateBank() {\r\n      try {\r\n        const res = await axios.put(`http://localhost:4000/api/bank/${this.editId}`, this.form);\r\n        const index = this.banks.findIndex((b) => b._id === this.editId);\r\n        if (index !== -1) this.banks[index] = res.data.data;\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error updating bank:\", err);\r\n        alert(\"‚ùå Update failed.\");\r\n      }\r\n    },\r\n    async deleteBank(id) {\r\n      if (!confirm(\"Delete this bank info?\")) return;\r\n      try {\r\n        await axios.delete(`http://localhost:4000/api/bank/${id}`);\r\n        this.banks = this.banks.filter((b) => b._id !== id);\r\n      } catch (err) {\r\n        console.error(\"Error deleting bank:\", err);\r\n        alert(\"‚ùå Delete failed.\");\r\n      }\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n        userId: \"example-user-id\",\r\n      };\r\n      this.isEditing = false;\r\n      this.editId = null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 600px;\r\n}\r\n</style>\r\n"],"mappings":";AAkDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,WAAW,EAAE,EAAE;QACfC,aAAa,EAAE,EAAE;QACjBC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,iBAAiB,CAAE;MAC7B,CAAC;;MACDC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,GAAE,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,gCAAgC,CAAC;QAC7D,IAAI,CAACP,KAAI,GAAIM,GAAG,CA