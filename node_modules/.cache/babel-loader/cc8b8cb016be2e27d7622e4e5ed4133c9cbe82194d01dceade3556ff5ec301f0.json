{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nimport { QuillEditor } from \"@vueup/vue-quill\";\nimport \"@vueup/vue-quill/dist/vue-quill.snow.css\";\nexport default {\n  components: {\n    QuillEditor\n  },\n  data() {\n    return {\n      currentStep: 0,\n      steps: [\"Basic Info\", \"Event Details\", \"Media Upload\", \"Tickets\"],\n      categories: [],\n      categoryID: \"\",\n      product: {\n        title: \"\",\n        description: \"\",\n        tag: \"\",\n        price: \"\",\n        event: {\n          date: \"\",\n          startTime: \"\",\n          endTime: \"\",\n          timezone: \"UTC\",\n          location: \"\"\n        }\n      },\n      address: \"\",\n      coordinates: null,\n      API_KEY: \"AIzaSyCkbHZXmqyIxjFpN8bMvDeURTmlpEhDHv4\",\n      // ⚠️ Replace with your actual key\n      startDate: \"\",\n      endDate: \"\",\n      startAmPm: \"AM\",\n      endAmPm: \"PM\",\n      eventType: \"single\",\n      // or \"recurring\"\n      eventFrequency: \"\",\n      photos: [],\n      previewUrl: null,\n      tickets: [{\n        name: \"\",\n        price: null,\n        quantity: null\n      }]\n    };\n  },\n  computed: {\n    ...mapGetters([\"getToken\"]),\n    mapUrl() {\n      if (!this.coordinates) return \"\";\n      const {\n        lat,\n        lng\n      } = this.coordinates;\n      return `https://www.google.com/maps/embed/v1/view?key=${this.API_KEY}&center=${lat},${lng}&zoom=15`;\n    }\n  },\n  async created() {\n    try {\n      const res = await axios.get(\"https://event-ticket-qa70.onrender.com/api/categories\");\n      this.categories = res.data.categories || res.data;\n      console.log(\"Categories fetched:\", this.categories);\n    } catch (error) {\n      console.error(\"Failed to fetch categories:\", error);\n    }\n  },\n  methods: {\n    isValidTitle() {\n      return this.product.title.trim().split(/\\s+/).length >= 2;\n    },\n    async handleGeocode() {\n      try {\n        const response = await axios.get(\"https://maps.googleapis.com/maps/api/geocode/json\", {\n          params: {\n            address: this.address,\n            key: this.API_KEY\n          }\n        });\n        const result = response.data.results[0];\n        if (result) {\n          const {\n            lat,\n            lng\n          } = result.geometry.location;\n          this.coordinates = {\n            lat,\n            lng\n          };\n        } else {\n          alert(\"❌ No results found for this address.\");\n          this.coordinates = null;\n        }\n      } catch (error) {\n        console.error(\"Geocoding error:\", error);\n        alert(\"❌ Failed to fetch location.\");\n      }\n    },\n    nextStep() {\n      if (this.currentStep < this.steps.length - 1) {\n        this.currentStep++;\n      }\n    },\n    prevStep() {\n      if (this.currentStep > 0) {\n        this.currentStep--;\n      }\n    },\n    handleFiles(event) {\n      const files = Array.from(event.target.files);\n      this.photos = files;\n      if (files.length > 0) {\n        const reader = new FileReader();\n        reader.onload = e => {\n          this.previewUrl = e.target.result;\n        };\n        reader.readAsDataURL(files[0]);\n      }\n    },\n    addTicket(type) {\n      const defaultPrice = type === \"Paid\" ? \"Cost\" : type === \"Donation\" ? \"Any\" : \"Free\";\n      this.tickets.push({\n        name: \"\",\n        quantity: null,\n        price: null\n      });\n    },\n    removeTicket(index) {\n      this.tickets.splice(index, 1);\n    },\n    duplicateTicket(index) {\n      const ticket = this.tickets[index];\n      this.tickets.splice(index + 1, 0, {\n        ...ticket\n      });\n    },\n    async submitProduct() {\n      const formData = new FormData();\n      formData.append(\"categoryID\", this.categoryID);\n      formData.append(\"title\", this.product.title);\n      formData.append(\"description\", this.product.description);\n      formData.append(\"tag\", this.product.tag);\n      formData.append(\"price\", this.product.price);\n      formData.append(\"eventDate\", this.product.event.startDate);\n      formData.append(\"endDate\", this.product.event.endDate);\n      console.log(this.startDate, this.endDate);\n      formData.append(\"startTime\", this.product.event.startTime + \" \" + this.startAmPm);\n      formData.append(\"endTime\", this.product.event.endTime + \" \" + this.endAmPm);\n      formData.append(\"timezone\", this.product.event.timezone);\n      formData.append(\"locationName\", this.address); // Send address as name\n      formData.append(\"lat\", this.coordinates.lat);\n      formData.append(\"lng\", this.coordinates.lng);\n      if (this.eventType === \"recurring\") {\n        formData.append(\"eventFrequency\", this.eventFrequency);\n      }\n      formData.append(\"tickets\", JSON.stringify(this.tickets));\n      console.log(this.tickets);\n      this.photos.forEach(photo => {\n        formData.append(\"photos\", photo);\n      });\n      console.log(formData);\n      try {\n        const response = await axios.post(\"https://event-ticket-qa70.onrender.com/api/products\", formData, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`,\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        console.log(\"Product created:\", response.data);\n        alert(\"Event created successfully!\");\n      } catch (error) {\n        console.error(\"Error creating event:\", error);\n        alert(\"Failed to create event\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","QuillEditor","components","data","currentStep","steps","categories","categoryID","product","title","description","tag","price","event","date","startTime","endTime","timezone","location","address","coordinates","API_KEY","startDate","endDate","startAmPm","endAmPm","eventType","eventFrequency","photos","previewUrl","tickets","name","quantity","computed","mapUrl","lat","lng","created","res","get","console","log","error","methods","isValidTitle","trim","split","length","handleGeocode","response","params","key","result","results","geometry","alert","nextStep","prevStep","handleFiles","files","Array","from","target","reader","FileReader","onload","e","readAsDataURL","addTicket","type","defaultPrice","push","removeTicket","index","splice","duplicateTicket","ticket","submitProduct","formData","FormData","append","JSON","stringify","forEach","photo","post","headers","Authorization","getToken"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\app\\addProduct.vue"],"sourcesContent":["<template>\r\n  <div class=\" \">\r\n    <!-- Stepper -->\r\n    <div class=\"stepper pt-2 mx-auto\">\r\n      <div\r\n        v-for=\"(step, index) in steps\"\r\n        :key=\"index\"\r\n        class=\"step\"\r\n        :class=\"{\r\n          active: currentStep === index,\r\n          completed: index < currentStep,\r\n        }\"\r\n      >\r\n        <div class=\"circle\">\r\n          <span v-if=\"index < currentStep\">✔</span>\r\n          <span v-else>{{ index + 1 }}</span>\r\n        </div>\r\n        <div class=\"label\">{{ step }}</div>\r\n      </div>\r\n      <div class=\"progress-line\">\r\n        <div\r\n          class=\"progress-fill\"\r\n          :style=\"{ width: (currentStep / (steps.length - 1)) * 100 + '%' }\"\r\n        ></div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Form -->\r\n    <div class=\"row justify-content-center\">\r\n      <div class=\"col-12 col-md-9 col-lg-6\">\r\n        <form\r\n          @submit.prevent=\"submitProduct\"\r\n          enctype=\"multipart/form-data\"\r\n          class=\"shadow p-4 w-md-75 mx-auto w-100\"\r\n        >\r\n          <!-- STEP 1: Basic Info -->\r\n          <div class=\"\" v-if=\"currentStep === 0\">\r\n            <div class=\"row\">\r\n              <div class=\"mb-3 col-md-7\">\r\n                <label for=\"title\" class=\"form-label\"\r\n                  >What is your event name:</label\r\n                >\r\n                <input\r\n                  v-model=\"product.title\"\r\n                  placeholder=\"Tech Summit, Afro summit\"\r\n                  type=\"text\"\r\n                  id=\"title\"\r\n                  :class=\"{ 'is-invalid': product.title && !isValidTitle() }\"\r\n                  class=\"form-control\"\r\n                />\r\n                <div v-if=\"product.title && !isValidTitle()\" class=\"invalid-feedback\">\r\n  Title must be at least two words.\r\n</div>\r\n              </div>\r\n              <div class=\"mb-3 col-md-5\">\r\n                <div>\r\n                  <div>\r\n                    <label for=\"title\" class=\"form-label\">Tag:</label>\r\n                    <input\r\n                      v-model=\"product.tag\"\r\n                      type=\"text\"\r\n                      placeholder=\"Tags (comma-separated)\"\r\n                      id=\"title\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row mb-3\">\r\n             \r\n\r\n              <div class=\"col-md-7\">\r\n                <label class=\"form-label\">Event Category</label>\r\n                <div>\r\n                  <select class=\"form-select\" v-model=\"categoryID\">\r\n                    <option value=\"\" disabled>Select a category</option>\r\n                    <option\r\n                      v-for=\"category in categories\"\r\n                      :key=\"category._id\"\r\n                      :value=\"category._id\"\r\n                    >\r\n                      {{ category.type }}\r\n                    </option>\r\n                    <!-- Add more as needed -->\r\n                  </select>\r\n                </div>\r\n              </div>\r\n               <div class=\"col-md-5\">\r\n                <label for=\"Price\" class=\"form-label mb-3\">Price:</label>\r\n                <input\r\n                  v-model=\"product.price\"\r\n                  type=\"text\"\r\n                  placeholder=\"price\"\r\n                  id=\"price\"\r\n                  class=\"form-control\"\r\n                />\r\n                <small>Leave blank for a free event</small>\r\n              </div>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">What's your event about:</label>\r\n              <QuillEditor\r\n                v-model=\"product.description\"\r\n                theme=\"snow\"\r\n                style=\"height: 300px\"\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          <!-- STEP 2: Description -->\r\n          <div v-if=\"currentStep === 1\">\r\n            <div class=\"mt-4 text-start\">\r\n              <h4><strong>When is your event?</strong></h4>\r\n              <p class=\"text-muted\">Select all the dates of your event</p>\r\n\r\n         \r\n\r\n              <!-- Time Zone -->\r\n              <div class=\"row mb-3\">\r\n                <div class=\"col-md-6\">\r\n                  <form\r\n                    @submit.prevent=\"handleGeocode\"\r\n                    class=\"card p-4 shadow-sm mb-4\"\r\n                  >\r\n                    <div class=\"mb-3\">\r\n                      <label for=\"address\" class=\"form-label\"\r\n                        >Enter Address:</label\r\n                      >\r\n                      <input\r\n                        v-model=\"address\"\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"address\"\r\n                        placeholder=\"e.g. 1600 Amphitheatre Parkway, CA\"\r\n                        required\r\n                      />\r\n                    </div>\r\n\r\n                    <button type=\"submit\" class=\"btn btn-primary\">\r\n                      Get Coordinates\r\n                    </button>\r\n                    <div v-if=\"coordinates\" class=\"alert alert-success\">\r\n                      <strong>Latitude:</strong> {{ coordinates.lat }} <br />\r\n                      <strong>Longitude:</strong> {{ coordinates.lng }}\r\n                    </div>\r\n\r\n                    <div v-if=\"coordinates\" class=\"mt-4\">\r\n                      <iframe\r\n                        :src=\"mapUrl\"\r\n                        width=\"100%\"\r\n                        height=\"300\"\r\n                        style=\"border: 0\"\r\n                        allowfullscreen=\"\"\r\n                        loading=\"lazy\"\r\n                        referrerpolicy=\"no-referrer-when-downgrade\"\r\n                      ></iframe>\r\n                    </div>\r\n                  </form>\r\n                </div>\r\n\r\n                <div class=\"col-md-6\">\r\n                  <label class=\"form-label\">* Select time zone</label>\r\n                  <select class=\"form-select\" v-model=\"product.event.timezone\">\r\n                    <option value=\"\">Select Time Zone</option>\r\n                    <option value=\"UTC+01:00\">\r\n                      (UTC+01:00) West Central Africa\r\n                    </option>\r\n                    <option value=\"UTC+00:00\">(UTC+00:00) GMT</option>\r\n                    <option value=\"UTC-05:00\">\r\n                      (UTC-05:00) Eastern Time (US & Canada)\r\n                    </option>\r\n                    <!-- Add more as needed -->\r\n                  </select>\r\n                </div>\r\n              </div>\r\n                   <!-- Toggle Buttons -->\r\n              <div class=\"btn-group mb-3\" role=\"group\">\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn\"\r\n                  :class=\"\r\n                    eventType === 'single'\r\n                      ? 'btn-primary active'\r\n                      : 'btn-outline-secondary'\r\n                  \"\r\n                  @click=\"eventType = 'single'\"\r\n                >\r\n                  Single Event\r\n                </button>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn\"\r\n                  :class=\"\r\n                    eventType === 'recurring'\r\n                      ? 'btn-primary active'\r\n                      : 'btn-outline-secondary'\r\n                  \"\r\n                  @click=\"eventType = 'recurring'\"\r\n                >\r\n                  Recurring Event\r\n                </button>\r\n              </div>\r\n              <!-- Start/End Date and Time -->\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6 mb-3\">\r\n                  <label class=\"form-label\">* Start date</label>\r\n                  <input type=\"date\" class=\"form-control\" v-model=\"startDate\" />\r\n                </div>\r\n                <div class=\"col-md-6 mb-3\" v-if=\"eventType === 'recurring'\">\r\n                  <label class=\"form-label\">* End date</label>\r\n                  <input type=\"date\" class=\"form-control\" v-model=\"endDate\" />\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6 mb-3\">\r\n                  <label class=\"form-label\">Start time</label>\r\n                  <div class=\"d-flex justify-content-between\">\r\n                    <div class=\"mt-2\">\r\n                      <input\r\n                        type=\"time\"\r\n                        class=\"form-control\"\r\n                        v-model=\"product.event.startTime\"\r\n                      />\r\n                    </div>\r\n\r\n                    <select class=\"form-select w-md-50\" v-model=\"startAmPm\">\r\n                      <option>AM</option>\r\n                      <option>PM</option>\r\n                    </select>\r\n                  </div>\r\n                </div>\r\n\r\n                <div class=\"col-md-6 mb-3\">\r\n                  <label class=\"form-label\">End time</label>\r\n                  <div class=\"d-flex justify-content-between\">\r\n                    <div class=\"mt-2\">\r\n                      <input\r\n                        type=\"time\"\r\n                        class=\"form-control\"\r\n                        v-model=\"product.event.endTime\"\r\n                      />\r\n                    </div>\r\n\r\n                    <select class=\"form-select w-md-50\" v-model=\"endAmPm\">\r\n                      <option>AM</option>\r\n                      <option>PM</option>\r\n                    </select>\r\n                  </div>\r\n                </div>\r\n\r\n                <!-- Recurring Options -->\r\n                <div class=\"col-md-4 mb-3\" v-if=\"eventType === 'recurring'\">\r\n                  <label class=\"form-label\">* Event frequency</label>\r\n                  <select class=\"form-select\" v-model=\"eventFrequency\">\r\n                    <option>Every day</option>\r\n                    <option>Weekdays</option>\r\n                    <option>Weekends</option>\r\n                    <option>Every week</option>\r\n                  </select>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- STEP 3: Media Upload -->\r\n          <div v-if=\"currentStep === 2\">\r\n            <div class=\"container py-5 text-start\">\r\n              <div class=\"row\">\r\n                <!-- Left Column: Event Image -->\r\n                <div class=\"col-md-6\">\r\n                  <h5><strong>Event Image</strong></h5>\r\n                  <p class=\"text-muted\">Upload a JPEG or PNG file</p>\r\n\r\n                  <div\r\n                    class=\"alert alert-light border text-muted py-2 small d-flex align-items-start\"\r\n                  >\r\n                    <i class=\"bi bi-info-circle-fill me-2 text-danger\"></i>\r\n                    Images with a 1:1 ratio (a square) work best on all event\r\n                    themes\r\n                  </div>\r\n\r\n                  <div>\r\n                    <!-- Upload box -->\r\n                    <label\r\n                      for=\"imageUpload\"\r\n                      class=\"border rounded bg-light d-flex flex-column justify-content-center align-items-center text-center py-5 mb-2\"\r\n                      style=\"cursor: pointer\"\r\n                    >\r\n                      <i class=\"bi bi-image fs-1 mb-2\"></i>\r\n                      <span>\r\n                        Drag an image here or\r\n                        <span class=\"text-danger fw-bold\">click to upload</span>\r\n                      </span>\r\n                      <input\r\n                        type=\"file\"\r\n                        multiple\r\n                        @change=\"handleFiles\"\r\n                        accept=\"image/*\"\r\n                        id=\"imageUpload\"\r\n                        class=\"d-none\"\r\n                      />\r\n                      <p v-if=\"photos.length\">\r\n                        Selected: {{ photos.length }} image(s)\r\n                      </p>\r\n                    </label>\r\n\r\n                    <!-- Preview -->\r\n                    <div v-if=\"previewUrl\" class=\"text-center mt-3\">\r\n                      <img\r\n                        :src=\"previewUrl\"\r\n                        alt=\"Preview\"\r\n                        class=\"img-fluid rounded shadow\"\r\n                        style=\"max-height: 250px\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <p class=\"text-muted small\">\r\n                    Upload an image with a size less than 2mb\r\n                  </p>\r\n                </div>\r\n\r\n                <!-- Right Column: Event Theme -->\r\n                <div class=\"col-md-6\">\r\n                  <div\r\n                    class=\"d-flex justify-content-between align-items-center\"\r\n                  >\r\n                    <div>\r\n                      <h5><strong>Event Page Theme</strong></h5>\r\n                      <p class=\"text-muted small mb-2\">\r\n                        Select the layout for your event page - it will only\r\n                        take 2 minutes\r\n                      </p>\r\n                    </div>\r\n                    <a href=\"#\" class=\"text-decoration-none text-muted small\">\r\n                      <i class=\"bi bi-play-circle\"></i> Preview theme\r\n                    </a>\r\n                  </div>\r\n\r\n                  <div\r\n                    class=\"border rounded bg-light p-3 d-flex align-items-center\"\r\n                    style=\"cursor: pointer\"\r\n                  >\r\n                    <i\r\n                      class=\"bi bi-layout-text-window-reverse fs-3 text-secondary me-3\"\r\n                    ></i>\r\n                    <div>\r\n                      <div class=\"small text-muted\">Theme</div>\r\n                      <div class=\"fw-bold\">Choose theme</div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- STEP 4: Location -->\r\n          <div v-if=\"currentStep === 3\">\r\n            <div class=\"my-4 text-start\">\r\n              <h5><strong>Create your ticket types</strong></h5>\r\n              <p class=\"text-danger small\">\r\n                *Please be sure to add taxes on top of ticket prices after this\r\n                event has been created.\r\n              </p>\r\n\r\n              <div class=\"row fw-bold border-bottom pb-2 mb-2\">\r\n                <div class=\"col-md-4\">Ticket name</div>\r\n                <div class=\"col-md-3\">Quantity</div>\r\n                <div class=\"col-md-3\">Price</div>\r\n                <div class=\"col-md-2\">Actions</div>\r\n              </div>\r\n\r\n              <!-- Ticket Rows -->\r\n              <div\r\n                v-for=\"(ticket, index) in tickets\"\r\n                :key=\"index\"\r\n                class=\"row align-items-center mb-2\"\r\n              >\r\n                <div class=\"col-md-4\">\r\n                  <input\r\n                    v-model=\"ticket.name\"\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"e.g. General Admission\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-3\">\r\n                  <input\r\n                    v-model.number=\"ticket.quantity\"\r\n                    type=\"number\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-3\">\r\n                  <input\r\n                    v-model.number=\"ticket.price\"\r\n                    type=\"number\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"Blank for a free event\"\r\n                  />\r\n                 \r\n                </div>\r\n                <div class=\"col-md-2 d-flex pt-2 gap-3\">\r\n                  \r\n                 \r\n                  <button\r\n                    class=\"btn btn-outline-danger\"\r\n                    title=\"Delete\"\r\n                    @click=\"removeTicket(index)\"\r\n                  >\r\n                    <i class=\"bi bi-trash\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- Timeslot Capacity -->\r\n\r\n              <!-- Add Ticket Buttons -->\r\n              <div class=\"d-flex gap-3\">\r\n                <button\r\n                  class=\"btn btn-primary btn-primary:hover\"\r\n                  type=\"button\"\r\n                  @click=\"addTicket('Paid')\"\r\n                >\r\n                  + ticket\r\n                </button>\r\n\r\n              \r\n\r\n                \r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Buttons -->\r\n          <div class=\"text-center mt-4 d-flex justify-content-between\">\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-danger\"\r\n              @click=\"prevStep\"\r\n              :disabled=\"currentStep === 0\"\r\n            >\r\n              Back\r\n            </button>\r\n            <button\r\n              v-if=\"currentStep < steps.length - 1\"\r\n              type=\"button\"\r\n              class=\"btn btn-primary btn-primary:hover\"\r\n              @click=\"nextStep\"\r\n            >\r\n              Next\r\n            </button>\r\n            <button\r\n              v-else\r\n              type=\"submit\"\r\n              class=\"btn btn-primary btn-primary:hover\"\r\n            >\r\n              Create Event\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\nimport { QuillEditor } from \"@vueup/vue-quill\";\r\nimport \"@vueup/vue-quill/dist/vue-quill.snow.css\";\r\n\r\nexport default {\r\n  components: {\r\n    QuillEditor,\r\n  },\r\n  data() {\r\n    return {\r\n      currentStep: 0,\r\n      steps: [\"Basic Info\", \"Event Details\", \"Media Upload\", \"Tickets\"],\r\n      categories: [],\r\n      categoryID: \"\",\r\n      product: {\r\n        title: \"\",\r\n        description: \"\",\r\n        tag: \"\",\r\n        price: \"\",\r\n        event: {\r\n          date: \"\",\r\n          startTime: \"\",\r\n          endTime: \"\",\r