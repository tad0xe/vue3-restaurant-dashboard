{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { defineComponent } from 'vue';\nimport FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport timeGridPlugin from '@fullcalendar/timegrid';\nimport interactionPlugin from '@fullcalendar/interaction';\nimport axios from 'axios';\nfunction createEventId() {\n  // Generate a unique ID for the event\n  return String(Date.now());\n}\nexport default defineComponent({\n  components: {\n    FullCalendar\n  },\n  data() {\n    return {\n      calendarOptions: {\n        plugins: [dayGridPlugin, timeGridPlugin, interactionPlugin // needed for dateClick\n        ],\n\n        headerToolbar: {\n          left: 'prev,next today',\n          center: 'title',\n          right: 'dayGridMonth,timeGridWeek,timeGridDay'\n        },\n        initialView: 'dayGridMonth',\n        editable: true,\n        selectable: true,\n        selectMirror: true,\n        dayMaxEvents: true,\n        weekends: true,\n        select: this.handleDateSelect.bind(this),\n        // Bind handleDateSelect method here\n        eventClick: this.handleEventClick.bind(this),\n        eventsSet: this.handleEvents.bind(this),\n        events: [] // Initial events array\n      },\n\n      currentEvents: []\n    };\n  },\n  created() {\n    this.fetchEvents();\n  },\n  methods: {\n    fetchEvents() {\n      axios.get('https://restaurant-n54j.onrender.com/api/events').then(response => {\n        this.currentEvents = response.data;\n        this.calendarOptions.events = response.data; // Update the events in the calendar\n      }).catch(error => {\n        console.error('Failed to fetch events:', error);\n      });\n    },\n    handleDateSelect(selectInfo) {\n      let title = prompt('Please enter a new title for your event');\n      if (title) {\n        let event = {\n          id: createEventId(),\n          title,\n          start: selectInfo.startStr,\n          end: selectInfo.endStr,\n          allDay: selectInfo.allDay\n        };\n        axios.post('https://restaurant-n54j.onrender.com/api/events', event).then(() => {\n          this.currentEvents.push(event);\n          this.calendarOptions.events.push(event); // Add the new event to the calendar\n        }).catch(error => {\n          console.error('Failed to create event:', error);\n        });\n      }\n    },\n    handleEventClick(clickInfo) {\n      if (confirm(`Are you sure you want to delete the event '${clickInfo.event.title}'`)) {\n        let eventId = clickInfo.event.id;\n        axios.delete(`https://restaurant-n54j.onrender.com/api/events/${eventId}`).then(() => {\n          this.currentEvents = this.currentEvents.filter(event => event.id !== eventId);\n          this.calendarOptions.events = this.calendarOptions.events.filter(event => event.id !== eventId); // Remove the event from the calendar\n        }).catch(error => {\n          console.error('Failed to delete event:', error);\n        });\n      }\n    },\n    handleEvents(events) {\n      this.currentEvents = events;\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","FullCalendar","dayGridPlugin","timeGridPlugin","interactionPlugin","axios","createEventId","String","Date","now","components","data","calendarOptions","plugins","headerToolbar","left","center","right","initialView","editable","selectable","selectMirror","dayMaxEvents","weekends","select","handleDateSelect","bind","eventClick","handleEventClick","eventsSet","handleEvents","events","currentEvents","created","fetchEvents","methods","get","then","response","catch","error","console","selectInfo","title","prompt","event","id","start","startStr","end","endStr","allDay","post","push","clickInfo","confirm","eventId","delete","filter"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\app\\calender.vue"],"sourcesContent":["<template>\r\n   <div class=\"conent-body\">\r\n    <!-- row -->\r\n    <div class=\"cotainer-fluid\">\r\n      <div class='demo-app-sidebar-section'>\r\n        <h2>Instructions</h2>\r\n        <ul>\r\n          <li>Select dates and you will be prompted to create a new event</li>\r\n          <li>Drag, drop, and resize events</li>\r\n          <li>Click an event to delete it</li>\r\n        </ul>\r\n      </div>\r\n      <div class='demo-app-sidebar-section'>\r\n        <label>\r\n          <input\r\n            type='checkbox'\r\n            v-model='calendarOptions.weekends'\r\n          />\r\n          toggle weekends\r\n        </label>\r\n      </div>\r\n      <div class='demo-app-sidebar-section'>\r\n        <h2>All Events ({{ currentEvents.length }})</h2>\r\n        <ul>\r\n          <li v-for='event in currentEvents' :key='event.id'>\r\n            <b>{{ event.start }}</b>\r\n            <i>{{ event.title }}</i>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <div class='demo-app-main'>\r\n      <FullCalendar\r\n        class='demo-app-calendar'\r\n        :options='calendarOptions'\r\n      >\r\n        <template #eventContent='arg'>\r\n          <b>{{ arg.timeText }}</b>\r\n          <i>{{ arg.event.title }}</i>\r\n        </template>\r\n      </FullCalendar>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent } from 'vue'\r\nimport FullCalendar from '@fullcalendar/vue3'\r\nimport dayGridPlugin from '@fullcalendar/daygrid'\r\nimport timeGridPlugin from '@fullcalendar/timegrid'\r\nimport interactionPlugin from '@fullcalendar/interaction'\r\nimport axios from 'axios'\r\n\r\nfunction createEventId() {\r\n  // Generate a unique ID for the event\r\n  return String(Date.now());\r\n}\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    FullCalendar,\r\n  },\r\n  data() {\r\n    return {\r\n      calendarOptions: {\r\n        plugins: [\r\n          dayGridPlugin,\r\n          timeGridPlugin,\r\n          interactionPlugin // needed for dateClick\r\n        ],\r\n        headerToolbar: {\r\n          left: 'prev,next today',\r\n          center: 'title',\r\n          right: 'dayGridMonth,timeGridWeek,timeGridDay'\r\n        },\r\n        initialView: 'dayGridMonth',\r\n        editable: true,\r\n        selectable: true,\r\n        selectMirror: true,\r\n        dayMaxEvents: true,\r\n        weekends: true,\r\n        select: this.handleDateSelect.bind(this), // Bind handleDateSelect method here\r\n        eventClick: this.handleEventClick.bind(this),\r\n        eventsSet: this.handleEvents.bind(this),\r\n        events: [] // Initial events array\r\n      },\r\n      currentEvents: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchEvents();\r\n  },\r\n  methods: {\r\n    fetchEvents() {\r\n      axios.get('https://restaurant-n54j.onrender.com/api/events')\r\n        .then(response => {\r\n          this.currentEvents = response.data;\r\n          this.calendarOptions.events = response.data; // Update the events in the calendar\r\n        })\r\n        .catch(error => {\r\n          console.error('Failed to fetch events:', error);\r\n        });\r\n    },\r\n    handleDateSelect(selectInfo) {\r\n      let title = prompt('Please enter a new title for your event');\r\n\r\n      if (title) {\r\n        let event = {\r\n          id: createEventId(),\r\n          title,\r\n          start: selectInfo.startStr,\r\n          end: selectInfo.endStr,\r\n          allDay: selectInfo.allDay\r\n        };\r\n\r\n        axios.post('https://restaurant-n54j.onrender.com/api/events', event)\r\n          .then(() => {\r\n            this.currentEvents.push(event);\r\n            this.calendarOptions.events.push(event); // Add the new event to the calendar\r\n          })\r\n          .catch(error => {\r\n            console.error('Failed to create event:', error);\r\n          });\r\n      }\r\n    },\r\n    handleEventClick(clickInfo) {\r\n      if (confirm(`Are you sure you want to delete the event '${clickInfo.event.title}'`)) {\r\n        let eventId = clickInfo.event.id;\r\n\r\n        axios.delete(`https://restaurant-n54j.onrender.com/api/events/${eventId}`)\r\n          .then(() => {\r\n            this.currentEvents = this.currentEvents.filter(event => event.id !== eventId);\r\n            this.calendarOptions.events = this.calendarOptions.events.filter(event => event.id !== eventId); // Remove the event from the calendar\r\n          })\r\n          .catch(error => {\r\n            console.error('Failed to delete event:', error);\r\n          });\r\n      }\r\n    },\r\n    handleEvents(events) {\r\n      this.currentEvents = events;\r\n    },\r\n  }\r\n});\r\n</script>\r\n\r\n<style lang='css'>\r\n/* CSS styles here */\r\n</style>\r\n"],"mappings":";AA8CA,SAASA,eAAc,QAAS,KAAI;AACpC,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,aAAY,MAAO,uBAAsB;AAChD,OAAOC,cAAa,MAAO,wBAAuB;AAClD,OAAOC,iBAAgB,MAAO,2BAA0B;AACxD,OAAOC,KAAI,MAAO,OAAM;AAExB,SAASC,aAAaA,CAAA,EAAG;EACvB;EACA,OAAOC,MAAM,CAACC,IAAI,CAACC,GAAG,CAAC,CAAC,CAAC;AAC3B;AAEA,eAAeT,eAAe,CAAC;EAC7BU,UAAU,EAAE;IACVT;EACF,CAAC;EACDU,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE;QACfC,OAAO,EAAE,CACPX,aAAa,EACbC,cAAc,EACdC,iBAAgB,CAAE;QAAA,CACnB;;QACDU,aAAa,EAAE;UACbC,IAAI,EAAE,iBAAiB;UACvBC,MAAM,EAAE,OAAO;UACfC,KAAK,EAAE;QACT,CAAC;QACDC,WAAW,EAAE,cAAc;QAC3BC,QAAQ,EAAE,IAAI;QACdC,UAAU,EAAE,IAAI;QAChBC,YAAY,EAAE,IAAI;QAClBC,YAAY,EAAE,IAAI;QAClBC,QAAQ,EAAE,IAAI;QACdC,MAAM,EAAE,IAAI,CAACC,gBAAgB,CAACC,IAAI,CAAC,IAAI,CAAC;QAAE;QAC1CC,UAAU,EAAE,IAAI,CAACC,gBAAgB,CAACF,IAAI,CAAC,IAAI,CAAC;QAC5CG,SAAS,EAAE,IAAI,CAACC,YAAY,CAACJ,IAAI,CAAC,IAAI,CAAC;QACvCK,MAAM,EAAE,EAAC,CAAE;MACb,CAAC;;MACDC,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZ7B,KAAK,CAAC+B,GAAG,CAAC,iDAAiD,EACxDC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,aAAY,GAAIM,QAAQ,CAAC3B,IAAI;QAClC,IAAI,CAACC,eAAe,CAACmB,MAAK,GAAIO,QAAQ,CAAC3B,IAAI,EAAE;MAC/C,CAAC,EACA4B,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD,