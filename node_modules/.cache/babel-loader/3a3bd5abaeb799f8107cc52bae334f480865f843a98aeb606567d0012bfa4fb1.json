{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProductList\",\n  data() {\n    return {\n      products: [],\n      searchTerm: \"\",\n      filters: {\n        minPrice: 0,\n        maxPrice: Infinity,\n        startDate: null,\n        endDate: null\n      },\n      tempFilters: {\n        minPrice: 0,\n        maxPrice: 100000\n      },\n      showPriceModal: false\n    };\n  },\n  computed: {\n    filteredProducts() {\n      const term = this.searchTerm.toLowerCase();\n      return this.products.filter(product => {\n        const titleMatch = product.title?.toLowerCase().includes(term);\n        const locationMatch = product.event?.location?.toLowerCase().includes(term);\n        const price = product.price || 0;\n        const priceMatch = (!this.filters.minPrice || price >= this.filters.minPrice) && (!this.filters.maxPrice || price <= this.filters.maxPrice);\n        const eventDate = new Date(product.event?.date);\n        const startDate = this.filters.startDate ? new Date(this.filters.startDate) : null;\n        const endDate = this.filters.endDate ? new Date(this.filters.endDate) : null;\n        const dateMatch = (!startDate || eventDate >= startDate) && (!endDate || eventDate <= endDate);\n        return (titleMatch || locationMatch) && priceMatch && dateMatch;\n      });\n    }\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const res = await axios.get(\"http://localhost:4000/api/products\");\n        this.products = res.data.products;\n      } catch (error) {\n        console.error(\"Error fetching products:\", error);\n      }\n    },\n    goToProduct(productTitle) {\n      const normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\n      this.$store.dispatch(\"setProductUrl\", productTitle);\n      this.$router.push({\n        name: \"ProductDetails\",\n        params: {\n          title: normalizedProductTitle\n        }\n      });\n    },\n    formatDate(date) {\n      if (!date) return \"No date\";\n      const options = {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return new Date(date).toLocaleDateString(\"en-US\", options);\n    },\n    formatPrice(price) {\n      if (!price || price === 0) return \"Free\";\n      return `$${price}`;\n    },\n    resetSearch() {\n      this.searchTerm = \"\";\n      this.filters = {\n        minPrice: 0,\n        maxPrice: Infinity,\n        startDate: null,\n        endDate: null\n      };\n    },\n    applyPriceFilters() {\n      this.filters.minPrice = this.tempFilters.minPrice;\n      this.filters.maxPrice = this.tempFilters.maxPrice;\n      this.showPriceModal = false;\n    },\n    clearPriceFilters() {\n      this.tempFilters.minPrice = 0;\n      this.tempFilters.maxPrice = 100000;\n      this.applyPriceFilters();\n    }\n  },\n  mounted() {\n    this.fetchProducts();\n  }\n};","map":{"version":3,"names":["axios","name","data","products","searchTerm","filters","minPrice","maxPrice","Infinity","startDate","endDate","tempFilters","showPriceModal","computed","filteredProducts","term","toLowerCase","filter","product","titleMatch","title","includes","locationMatch","event","location","price","priceMatch","eventDate","Date","date","dateMatch","methods","fetchProducts","res","get","error","console","goToProduct","productTitle","normalizedProductTitle","replace","$store","dispatch","$router","push","params","formatDate","options","weekday","year","month","day","toLocaleDateString","formatPrice","resetSearch","applyPriceFilters","clearPriceFilters","mounted"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\events.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 🔍 Search & Filters -->\r\n    <div class=\"container-fluid bg-light py-3 border-top\">\r\n      <div class=\"d-flex flex-wrap justify-content-around align-items-center gap-2\">\r\n        <!-- Search Input Group -->\r\n        <div class=\"input-group\" style=\"max-width: 600px\">\r\n          <input\r\n            v-model=\"searchTerm\"\r\n            type=\"text\"\r\n            class=\"form-control search-bar\"\r\n            placeholder=\"Search events by title or location...\"\r\n          />\r\n          <span class=\"input-group-text bg-white border-start\">\r\n            <i class=\"bi bi-geo-alt-fill text-primary\"></i> &nbsp; Location\r\n          </span>\r\n          <button class=\"btn btn-primary\" type=\"button\">\r\n            <i class=\"bi bi-search\"></i>\r\n          </button>\r\n        </div>\r\n\r\n        <!-- Filter & Reset -->\r\n        <div class=\"d-flex align-items-center gap-3\">\r\n          <button class=\"btn btn-outline-dark\" @click=\"showPriceModal = true\">Price</button>\r\n          <a href=\"#\" class=\"text-muted text-decoration-none\" @click.prevent=\"resetSearch\">\r\n            Reset Filters\r\n          </a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 🧾 Price Filter Modal -->\r\n    <div v-if=\"showPriceModal\" class=\"modal\" @click.self=\"showPriceModal = false\">\r\n      <div class=\"modal-content\">\r\n        <div class=\"d-flex justify-content-between align-items-center mb-3\">\r\n          <h5 class=\"m-0\">Price</h5>\r\n          <button class=\"btn-icon\" @click=\"showPriceModal = false\">\r\n            <i class=\"bi bi-x-lg\"></i>\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <label>Minimum amount</label>\r\n            <input v-model.number=\"tempFilters.minPrice\" type=\"number\" class=\"form-control\" />\r\n          </div>\r\n          <div class=\"col\">\r\n            <label>Maximum amount</label>\r\n            <input v-model.number=\"tempFilters.maxPrice\" type=\"number\" class=\"form-control\" />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"d-flex justify-content-between mt-4\">\r\n          <button class=\"btn btn-outline-secondary\" @click=\"clearPriceFilters\">Clear</button>\r\n          <button class=\"btn btn-primary\" @click=\"applyPriceFilters\">Apply</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 🗂 Event List -->\r\n    <div class=\"container-fluid\">\r\n      <div class=\"section_discover pt-4\">\r\n        <div class=\"container-large\">\r\n          <h3 class=\"mb-3\">Explore {{ filteredProducts.length }} Events</h3>\r\n\r\n          <!-- No Results -->\r\n          <div v-if=\"filteredProducts.length === 0\" class=\"alert alert-warning\">\r\n            No events match your search.\r\n          </div>\r\n\r\n          <!-- Product Cards -->\r\n          <div class=\"row\">\r\n            <div\r\n              class=\"col-md-4 pt-3 pt-md-0\"\r\n              v-for=\"product in filteredProducts\"\r\n              :key=\"product._id\"\r\n            >\r\n              <div>\r\n                <img\r\n                  :src=\"product.photos[0] || 'https://via.placeholder.com/400x300?text=No+Image'\"\r\n                  class=\"img-fluid rounded w-100\"\r\n                  style=\"height: 200px; object-fit: cover\"\r\n                  alt=\"Event Image\"\r\n                />\r\n                <div class=\"pt-2\">\r\n                  <h5>{{ product.title }}</h5>\r\n\r\n                  <p class=\"mb-1\">\r\n                    <i class=\"bi bi-calendar-event me-2\"></i>\r\n                    {{ formatDate(product.event.date) }}\r\n                  </p>\r\n\r\n                  <p class=\"mb-1\">\r\n                    <i class=\"bi bi-geo-alt me-2\"></i>\r\n                    {{ product.event.location || \"No location\" }}\r\n                  </p>\r\n\r\n                  <p>{{ formatPrice(product.price) }}</p>\r\n\r\n                  <button\r\n                    class=\"btn btn-outline-primary\"\r\n                    @click=\"goToProduct(product.title)\"\r\n                  >\r\n                    View event details\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"row g-4\"><div class=\"col-md-6\"></div></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"ProductList\",\r\n  data() {\r\n    return {\r\n      products: [],\r\n      searchTerm: \"\",\r\n      filters: {\r\n        minPrice: 0,\r\n        maxPrice: Infinity,\r\n        startDate: null,\r\n        endDate: null,\r\n      },\r\n      tempFilters: {\r\n        minPrice: 0,\r\n        maxPrice: 100000,\r\n      },\r\n      showPriceModal: false,\r\n    };\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      const term = this.searchTerm.toLowerCase();\r\n\r\n      return this.products.filter((product) => {\r\n        const titleMatch = product.title?.toLowerCase().includes(term);\r\n        const locationMatch = product.event?.location?.toLowerCase().includes(term);\r\n\r\n        const price = product.price || 0;\r\n        const priceMatch =\r\n          (!this.filters.minPrice || price >= this.filters.minPrice) &&\r\n          (!this.filters.maxPrice || price <= this.filters.maxPrice);\r\n\r\n        const eventDate = new Date(product.event?.date);\r\n        const startDate = this.filters.startDate ? new Date(this.filters.startDate) : null;\r\n        const endDate = this.filters.endDate ? new Date(this.filters.endDate) : null;\r\n\r\n        const dateMatch =\r\n          (!startDate || eventDate >= startDate) &&\r\n          (!endDate || eventDate <= endDate);\r\n\r\n        return (titleMatch || locationMatch) && priceMatch && dateMatch;\r\n      });\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchProducts() {\r\n      try {\r\n        const res = await axios.get(\"http://localhost:4000/api/products\");\r\n        this.products = res.data.products;\r\n      } catch (error) {\r\n        console.error(\"Error fetching products:\", error);\r\n      }\r\n    },\r\n    goToProduct(productTitle) {\r\n      const normalizedProductTitle = productTitle.replace(/\\s+/g, \"-\");\r\n      this.$store.dispatch(\"setProductUrl\", productTitle);\r\n      this.$router.push({\r\n        name: \"ProductDetails\",\r\n        params: { title: normalizedProductTitle },\r\n      });\r\n    },\r\n    formatDate(date) {\r\n      if (!date) return \"No date\";\r\n      const options = {\r\n        weekday: \"long\",\r\n        year: \"numeric\",\r\n        month: \"long\",\r\n        day: \"numeric\",\r\n      };\r\n      return new Date(date).toLocaleDateString(\"en-US\", options);\r\n    },\r\n    formatPrice(price) {\r\n      if (!price || price === 0) return \"Free\";\r\n      return `$${price}`;\r\n    },\r\n    resetSearch() {\r\n      this.searchTerm = \"\";\r\n      this.filters = {\r\n        minPrice: 0,\r\n        maxPrice: Infinity,\r\n        startDate: null,\r\n        endDate: null,\r\n      };\r\n    },\r\n    applyPriceFilters() {\r\n      this.filters.minPrice = this.tempFilters.minPrice;\r\n      this.filters.maxPrice = this.tempFilters.maxPrice;\r\n      this.showPriceModal = false;\r\n    },\r\n    clearPriceFilters() {\r\n      this.tempFilters.minPrice = 0;\r\n      this.tempFilters.maxPrice = 100000;\r\n      this.applyPriceFilters();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchProducts();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.search-bar {\r\n  padding: 12px 20px;\r\n  font-size: 1rem;\r\n  border-radius: 8px;\r\n  border: 1px solid #ccc;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n  width: 100%;\r\n  transition: all 0.3s ease;\r\n}\r\n.search-bar:focus {\r\n  outline: none;\r\n  border-color: #b71c1c;\r\n  box-shadow: 0 0 8px rgba(183, 28, 28, 0.6);\r\n}\r\n.search-bar::placeholder {\r\n  color: #aaa;\r\n  font-style: italic;\r\n}\r\n\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  z-index: 1050;\r\n}\r\n.modal-content {\r\n  background: #fff;\r\n  padding: 30px;\r\n  border-radius: 16px;\r\n  width: 90%;\r\n  max-width: 500px;\r\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\r\n  animation: fadeIn 0.3s ease-in-out;\r\n}\r\n@keyframes fadeIn {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n.btn-icon {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AAsHA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;QACPC,QAAQ,EAAE,CAAC;QACXC,QAAQ,EAAEC,QAAQ;QAClBC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE;MACX,CAAC;MACDC,WAAW,EAAE;QACXL,QAAQ,EAAE,CAAC;QACXC,QAAQ,EAAE;MACZ,CAAC;MACDK,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,MAAMC,IAAG,GAAI,IAAI,CAACX,UAAU,CAACY,WAAW,CAAC,CAAC;MAE1C,OAAO,IAAI,CAACb,QAAQ,CAACc,MAAM,CAAEC,OAAO,IAAK;QACvC,MAAMC,UAAS,GAAID,OAAO,CAACE,KAAK,EAAEJ,WAAW,CAAC,CAAC,CAACK,QAAQ,CAACN,IAAI,CAAC;QAC9D,MAAMO,aAAY,GAAIJ,OAAO,CAACK,KAAK,EAAEC,QAAQ,EAAER,WAAW,CAAC,CAAC,CAACK,QAAQ,CAACN,IAAI,CAAC;QAE3E,MAAMU,KAAI,GAAIP,OAAO,CAACO,KAAI,IAAK,CAAC;QAChC,MAAMC,UAAS,GACb,CAAC,CAAC,IAAI,CAACrB,OAAO,CAACC,QAAO,IAAKmB,KAAI,IAAK,IAAI,CAACpB,OAAO,CAACC,QAAQ,MACxD,CAAC,IAAI,CAACD,OAAO,CAACE,QAAO,IAAKkB,KAAI,IAAK,IAAI,CAACpB,OAAO,CAACE,QAAQ,CAAC;QAE5D,MAAMoB,SAAQ,GAAI,IAAIC,IAAI,CAACV,OAAO,CAACK,KAAK,EAAEM,IAAI,CAAC;QAC/C,MAAMpB,SAAQ,GAAI,IAAI,CAACJ,OAAO,CAACI,SAAQ,GAAI,IAAImB,IAAI,CAAC,IAAI,CAACvB,OAAO,CAACI,SAAS,IAAI,IAAI;QAClF,MAAMC,OAAM,GAAI,IAAI,CAACL,OAAO,CAACK,OAAM,GAAI,IAAIkB,IAAI,CAAC,IAAI,CAACvB,OAAO,CAACK,OAAO,IAAI,IAAI;QAE5E,MAAMoB,SAAQ,GACZ,CAAC,CAACrB,SAAQ,IAAKkB,SAAQ,IAAKlB,SAAS,MA