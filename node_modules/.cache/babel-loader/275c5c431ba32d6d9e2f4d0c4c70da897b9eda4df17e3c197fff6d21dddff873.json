{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"ProductForm\",\n  data() {\n    return {\n      categories: [],\n      // Categories fetched from the server\n      categoryID: \"\",\n      // Selected category ID\n      product: {\n        title: \"\",\n        description: \"\",\n        tag: \"\",\n        price: {\n          normal: 0,\n          vip: 0,\n          vvip: 0\n        },\n        event: {\n          date: \"\",\n          startTime: \"\",\n          endTime: \"\",\n          timezone: \"UTC\",\n          location: \"\"\n        }\n      },\n      photos: [],\n      tickets: [{\n        name: \"\",\n        price: 0,\n        quantity: 0\n      } // initial ticket input\n      ]\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"getToken\"])\n  },\n  async created() {\n    try {\n      const response = await axios.get(\"http://localhost:4000/api/categories\");\n      this.categories = response.data.categories || response.data;\n      console.log(\"Categories fetched:\", this.categories);\n    } catch (error) {\n      console.error(\"Failed to fetch categories:\", error);\n    }\n  },\n  methods: {\n    handleFiles(event) {\n      this.photos = Array.from(event.target.files);\n    },\n    addTicket() {\n      this.tickets.push({\n        name: \"\",\n        price: 0,\n        quantity: 0\n      });\n    },\n    removeTicket(index) {\n      this.tickets.splice(index, 1);\n    },\n    async submitProduct() {\n      const formData = new FormData();\n      formData.append(\"title\", this.product.title);\n      formData.append(\"description\", this.product.description);\n      formData.append(\"tag\", this.product.tag);\n      formData.append(\"price\", JSON.stringify(this.product.price));\n      formData.append(\"eventDate\", this.product.event.date);\n      formData.append(\"startTime\", this.product.event.startTime);\n      formData.append(\"endTime\", this.product.event.endTime);\n      formData.append(\"timezone\", this.product.event.timezone);\n      formData.append(\"location\", this.product.event.location);\n\n      // ðŸ‘‡ Tickets - send in parallel arrays\n      this.tickets.forEach(ticket => {\n        formData.append(\"ticketName\", ticket.name);\n        formData.append(\"ticketPrice\", ticket.price);\n        formData.append(\"ticketQuantity\", ticket.quantity);\n      });\n\n      // Photos\n      this.photos.forEach(photo => {\n        formData.append(\"photos\", photo);\n      });\n      try {\n        const response = await axios.post(\"http://localhost:4000/api/products\", formData, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`,\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        console.log(\"Product created:\", response.data);\n        alert(\"Product created successfully!\");\n      } catch (error) {\n        console.error(\"Error creating product:\", error);\n        alert(\"Failed to create product\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","categories","categoryID","product","title","description","tag","price","normal","vip","vvip","event","date","startTime","endTime","timezone","location","photos","tickets","quantity","computed","created","response","get","console","log","error","methods","handleFiles","Array","from","target","files","addTicket","push","removeTicket","index","splice","submitProduct","formData","FormData","append","JSON","stringify","forEach","ticket","photo","post","headers","Authorization","getToken","alert"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\rex.vue"],"sourcesContent":["<template>\r\n  <form @submit.prevent=\"submitProduct\" enctype=\"multipart/form-data\">\r\n    <h2>Create Product</h2>\r\n    <div class=\"mb-3\">\r\n      <label class=\"form-label\">Event Category</label>\r\n\r\n      <div>\r\n        <select class=\"form-select\" v-model=\"categoryID\">\r\n          <option value=\"\" disabled>Select a category</option>\r\n          <option v-for=\"category in categories\" :key=\"category._id\">\r\n            {{ category.type }}\r\n          </option>\r\n          <!-- Add more as needed -->\r\n        </select>\r\n      </div>\r\n    </div>\r\n    <!-- Title -->\r\n    <input v-model=\"product.title\" type=\"text\" placeholder=\"Title\" required />\r\n\r\n    <!-- Description -->\r\n    <textarea\r\n      v-model=\"product.description\"\r\n      placeholder=\"Description\"\r\n      required\r\n    ></textarea>\r\n\r\n    <!-- Tags -->\r\n    <input\r\n      v-model=\"product.tag\"\r\n      type=\"text\"\r\n      placeholder=\"Tags (comma-separated)\"\r\n    />\r\n\r\n    <!-- Price Tiers -->\r\n    <input\r\n      v-model.number=\"product.price.normal\"\r\n      type=\"number\"\r\n      placeholder=\"Normal Price\"\r\n      required\r\n    />\r\n    <input\r\n      v-model.number=\"product.price.vip\"\r\n      type=\"number\"\r\n      placeholder=\"VIP Price\"\r\n      required\r\n    />\r\n    <input\r\n      v-model.number=\"product.price.vvip\"\r\n      type=\"number\"\r\n      placeholder=\"VVIP Price\"\r\n      required\r\n    />\r\n\r\n    <!-- Event Details -->\r\n    <input v-model=\"product.event.date\" type=\"date\" required />\r\n    <input v-model=\"product.event.startTime\" type=\"time\" required />\r\n    <input v-model=\"product.event.endTime\" type=\"time\" required />\r\n    <input\r\n      v-model=\"product.event.timezone\"\r\n      type=\"text\"\r\n      placeholder=\"Timezone (e.g. UTC)\"\r\n      required\r\n    />\r\n    <input\r\n      v-model=\"product.event.location\"\r\n      type=\"text\"\r\n      placeholder=\"Location\"\r\n      required\r\n    />\r\n\r\n    <!-- ðŸŽŸ Tickets -->\r\n    <div v-for=\"(ticket, index) in tickets\" :key=\"index\" class=\"ticket-group\">\r\n      <input\r\n        v-model=\"ticket.name\"\r\n        type=\"text\"\r\n        placeholder=\"Ticket Name\"\r\n        required\r\n      />\r\n      <input\r\n        v-model.number=\"ticket.price\"\r\n        type=\"number\"\r\n        placeholder=\"Ticket Price\"\r\n        required\r\n      />\r\n      <input\r\n        v-model.number=\"ticket.quantity\"\r\n        type=\"number\"\r\n        placeholder=\"Ticket Quantity\"\r\n        required\r\n      />\r\n      <button type=\"button\" @click=\"removeTicket(index)\">Remove</button>\r\n    </div>\r\n    <button type=\"button\" @click=\"addTicket\">Add Ticket</button>\r\n\r\n    <!-- Photos Upload -->\r\n    <input type=\"file\" multiple @change=\"handleFiles\" accept=\"image/*\" />\r\n\r\n    <!-- Submit -->\r\n    <button type=\"submit\">Submit Product</button>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ProductForm\",\r\n\r\n  data() {\r\n    return {\r\n      categories: [], // Categories fetched from the server\r\n      categoryID: \"\", // Selected category ID\r\n      product: {\r\n        title: \"\",\r\n        description: \"\",\r\n        tag: \"\",\r\n        price: {\r\n          normal: 0,\r\n          vip: 0,\r\n          vvip: 0,\r\n        },\r\n        event: {\r\n          date: \"\",\r\n          startTime: \"\",\r\n          endTime: \"\",\r\n          timezone: \"UTC\",\r\n          location: \"\",\r\n        },\r\n      },\r\n      photos: [],\r\n      tickets: [\r\n        { name: \"\", price: 0, quantity: 0 }, // initial ticket input\r\n      ],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get(\"http://localhost:4000/api/categories\");\r\n\r\n      this.categories = response.data.categories || response.data;\r\n      console.log(\"Categories fetched:\", this.categories);\r\n    } catch (error) {\r\n      console.error(\"Failed to fetch categories:\", error);\r\n    }\r\n  },\r\n  methods: {\r\n    handleFiles(event) {\r\n      this.photos = Array.from(event.target.files);\r\n    },\r\n\r\n    addTicket() {\r\n      this.tickets.push({ name: \"\", price: 0, quantity: 0 });\r\n    },\r\n\r\n    removeTicket(index) {\r\n      this.tickets.splice(index, 1);\r\n    },\r\n\r\n    async submitProduct() {\r\n      const formData = new FormData();\r\n\r\n      formData.append(\"title\", this.product.title);\r\n      formData.append(\"description\", this.product.description);\r\n      formData.append(\"tag\", this.product.tag);\r\n      formData.append(\"price\", JSON.stringify(this.product.price));\r\n\r\n      formData.append(\"eventDate\", this.product.event.date);\r\n      formData.append(\"startTime\", this.product.event.startTime);\r\n      formData.append(\"endTime\", this.product.event.endTime);\r\n      formData.append(\"timezone\", this.product.event.timezone);\r\n      formData.append(\"location\", this.product.event.location);\r\n\r\n      // ðŸ‘‡ Tickets - send in parallel arrays\r\n      this.tickets.forEach((ticket) => {\r\n        formData.append(\"ticketName\", ticket.name);\r\n        formData.append(\"ticketPrice\", ticket.price);\r\n        formData.append(\"ticketQuantity\", ticket.quantity);\r\n      });\r\n\r\n      // Photos\r\n      this.photos.forEach((photo) => {\r\n        formData.append(\"photos\", photo);\r\n      });\r\n\r\n      try {\r\n        const response = await axios.post(\r\n          \"http://localhost:4000/api/products\",\r\n          formData,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${this.getToken}`,\r\n              \"Content-Type\": \"multipart/form-data\",\r\n            },\r\n          }\r\n        );\r\n\r\n        console.log(\"Product created:\", response.data);\r\n        alert(\"Product created successfully!\");\r\n      } catch (error) {\r\n        console.error(\"Error creating product:\", error);\r\n        alert(\"Failed to create product\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAuGA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EAEnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MAAE;MAChBC,UAAU,EAAE,EAAE;MAAE;MAChBC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,GAAG,EAAE,EAAE;QACPC,KAAK,EAAE;UACLC,MAAM,EAAE,CAAC;UACTC,GAAG,EAAE,CAAC;UACNC,IAAI,EAAE;QACR,CAAC;QACDC,KAAK,EAAE;UACLC,IAAI,EAAE,EAAE;UACRC,SAAS,EAAE,EAAE;UACbC,OAAO,EAAE,EAAE;UACXC,QAAQ,EAAE,KAAK;UACfC,QAAQ,EAAE;QACZ;MACF,CAAC;MACDC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,CACP;QAAEnB,IAAI,EAAE,EAAE;QAAEQ,KAAK,EAAE,CAAC;QAAEY,QAAQ,EAAE;MAAE,CAAC,CAAE;MAAA;IAEzC,CAAC;EACH,CAAC;;EAEDC,QAAQ,EAAE;IACR,GAAGtB,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EACD,MAAMuB,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMzB,KAAK,CAAC0B,GAAG,CAAC,sCAAsC,CAAC;MAExE,IAAI,CAACtB,UAAS,GAAIqB,QAAQ,CAACtB,IAAI,CAACC,UAAS,IAAKqB,QAAQ,CAACtB,IAAI;MAC3DwB,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAE,IAAI,CAACxB,UAAU,CAAC;IACrD,EAAE,OAAOyB,KAAK,EAAE;MACdF,OAAO,CAACE,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;IACrD;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACjB,KAAK,EAAE;MACjB,IAAI,CAACM,MAAK,GAAIY,KAAK,CAACC,IAAI,CAACnB,KAAK,CAACoB,MAAM,CAACC,KAAK,CAAC;IAC9C,CAAC;IAEDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACf,OAAO,CAACgB,IAAI,CAAC;QAAEnC,IAAI,EAAE,EAAE;QAAEQ,KAAK,EAAE,CAAC;QAAEY,QAAQ,EAAE;MAAE,CAAC,CAAC;IACxD,CAAC;IAEDgB,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAI,CAAClB,OAAO,CAACmB,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAC/B,CAAC;IAED,MAAME,aAAaA,CAAA,EAAG;MACpB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAE/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACtC,OAAO,CAACC,KAAK,CAAC;MAC5CmC,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAACtC,OAAO,CAACE,WAAW,CAAC;MACxDkC,QAAQ,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAACtC,OAAO,CAACG,GAAG,CAAC;MACxCiC,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACxC,OAAO,CAACI,KAAK,CAAC,CAAC;MAE5DgC,QAAQ,CAACE,MAAM,CAAC,WAAW,EAAE,IAAI,CAACtC,OAAO,CAACQ,KAAK,CAACC,IAAI